<?xml version="1.0" encoding="UTF-8"?>
<bpmn:definitions
  xmlns:bpmn="http://www.omg.org/spec/BPMN/20100524/MODEL"
  xmlns:bpmndi="http://www.omg.org/spec/BPMN/20100524/DI"
  xmlns:dc="http://www.omg.org/spec/DD/20100524/DC"
  xmlns:di="http://www.omg.org/spec/DD/20100524/DI"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xmlns:camunda="http://camunda.org/schema/1.0/bpmn"
  targetNamespace="http://example.com/bpmn"
  exporter="GitHub-Copilot-GovModeler"
  exporterVersion="2.0"
  modeler:executionPlatform="Camunda Platform"
  modeler:executionPlatformVersion="7.20.0">

  <!-- =====================================================
       COLLABORATION - Strategische Personalbedarfsplanung
       ===================================================== -->
  <bpmn:collaboration id="Collab_Strategische_Personalbedarfsplanung">
    
    <!-- Participant: Kunde -->
    <bpmn:participant id="Participant_Kunde"
                      name="Kunde (Organisationseinheit/Führung)"
                      processRef="Process_Kunde" />
    
    <!-- Participant: Personalstelle dezentral -->
    <bpmn:participant id="Participant_PS_Dezentral"
                      name="Personalstelle dezentral"
                      processRef="Process_PS_Dezentral" />
    
    <!-- Participant: Personalstelle zentral -->
    <bpmn:participant id="Participant_PS_Zentral"
                      name="Personalstelle zentral"
                      processRef="Process_PS_Zentral" />
    
    <!-- Message Flows zwischen Participanten -->
    <bpmn:messageFlow id="MsgFlow_Planungsanfrage_Kunde_Dezentral"
                      name="Planungsanfrage"
                      sourceRef="Task_Planungsanfrage_uebermitteln"
                      targetRef="Task_Anfrage_aufnehmen" />
    
    <bpmn:messageFlow id="MsgFlow_Unterlagen_Dezentral_Zentral"
                      name="Strukturierte Unterlagen"
                      sourceRef="Task_Unterlagen_weiterleiten"
                      targetRef="StartEvent_Unterlagen_erhalten" />
    
    <bpmn:messageFlow id="MsgFlow_Ergebnis_Zentral_Dezentral"
                      name="Planungsergebnis"
                      sourceRef="Task_Ergebnis_bereitstellen"
                      targetRef="Task_Ergebnis_erlaeutern" />
    
    <bpmn:messageFlow id="MsgFlow_Ergebnis_Dezentral_Kunde"
                      name="Strategische Personalbedarfsplanung"
                      sourceRef="Task_Ergebnis_kommunizieren"
                      targetRef="Event_Planung_erhalten" />
  </bpmn:collaboration>

  <!-- =====================================================
       KUNDE PROZESS
       ===================================================== -->
  <bpmn:process id="Process_Kunde"
                name="Strategische_Personalbedarfsplanung_Kunde"
                isExecutable="true">
    
    <!-- Start Event -->
    <bpmn:startEvent id="StartEvent_Kunde" name="Strategischer Planungsbedarf gemeldet">
      <bpmn:outgoing>Flow_Start_Dokumentieren</bpmn:outgoing>
    </bpmn:startEvent>
    
    <!-- User Task: Planungsanlass dokumentieren -->
    <bpmn:userTask id="Task_Planungsanlass_dokumentieren" name="Planungsanlass dokumentieren">
      <bpmn:incoming>Flow_Start_Dokumentieren</bpmn:incoming>
      <bpmn:outgoing>Flow_Dokumentieren_Bereitstellen</bpmn:outgoing>
    </bpmn:userTask>
    
    <!-- User Task: Personaldaten und Annahmen bereitstellen -->
    <bpmn:userTask id="Task_Daten_bereitstellen" name="Personaldaten und Annahmen bereitstellen">
      <bpmn:incoming>Flow_Dokumentieren_Bereitstellen</bpmn:incoming>
      <bpmn:outgoing>Flow_Bereitstellen_Uebermitteln</bpmn:outgoing>
    </bpmn:userTask>
    
    <!-- User Task: Planungsanfrage übermitteln -->
    <bpmn:userTask id="Task_Planungsanfrage_uebermitteln" name="Planungsanfrage übermitteln">
      <bpmn:incoming>Flow_Bereitstellen_Uebermitteln</bpmn:incoming>
      <bpmn:outgoing>Flow_Uebermitteln_Warten</bpmn:outgoing>
    </bpmn:userTask>
    
    <!-- Intermediate Catch Event: Planung erhalten -->
    <bpmn:intermediateCatchEvent id="Event_Planung_erhalten" name="Strategische Planung erhalten">
      <bpmn:incoming>Flow_Uebermitteln_Warten</bpmn:incoming>
      <bpmn:outgoing>Flow_Planung_Nutzen</bpmn:outgoing>
      <bpmn:messageEventDefinition id="MessageEventDef_Planung" messageRef="Message_Planung" />
    </bpmn:intermediateCatchEvent>
    
    <!-- User Task: Planungsergebnis für Maßnahmen nutzen -->
    <bpmn:userTask id="Task_Planung_nutzen" name="Planungsergebnis für Maßnahmen nutzen">
      <bpmn:incoming>Flow_Planung_Nutzen</bpmn:incoming>
      <bpmn:outgoing>Flow_Nutzen_Ende</bpmn:outgoing>
    </bpmn:userTask>
    
    <!-- End Event -->
    <bpmn:endEvent id="EndEvent_Kunde" name="Planung genutzt">
      <bpmn:incoming>Flow_Nutzen_Ende</bpmn:incoming>
    </bpmn:endEvent>
    
    <!-- Sequence Flows - Kunde -->
    <bpmn:sequenceFlow id="Flow_Start_Dokumentieren"
                       sourceRef="StartEvent_Kunde"
                       targetRef="Task_Planungsanlass_dokumentieren" />
    
    <bpmn:sequenceFlow id="Flow_Dokumentieren_Bereitstellen"
                       sourceRef="Task_Planungsanlass_dokumentieren"
                       targetRef="Task_Daten_bereitstellen" />
    
    <bpmn:sequenceFlow id="Flow_Bereitstellen_Uebermitteln"
                       sourceRef="Task_Daten_bereitstellen"
                       targetRef="Task_Planungsanfrage_uebermitteln" />
    
    <bpmn:sequenceFlow id="Flow_Uebermitteln_Warten"
                       sourceRef="Task_Planungsanfrage_uebermitteln"
                       targetRef="Event_Planung_erhalten" />
    
    <bpmn:sequenceFlow id="Flow_Planung_Nutzen"
                       sourceRef="Event_Planung_erhalten"
                       targetRef="Task_Planung_nutzen" />
    
    <bpmn:sequenceFlow id="Flow_Nutzen_Ende"
                       sourceRef="Task_Planung_nutzen"
                       targetRef="EndEvent_Kunde" />
  </bpmn:process>

  <!-- =====================================================
       PERSONALSTELLE DEZENTRAL PROZESS (serviceorientiert beratend)
       ===================================================== -->
  <bpmn:process id="Process_PS_Dezentral"
                name="Strategische_Personalbedarfsplanung_Dezentral"
                isExecutable="true">
    
    <!-- User Task: Anfrage aufnehmen -->
    <bpmn:userTask id="Task_Anfrage_aufnehmen" name="Anfrage aufnehmen">
      <bpmn:outgoing>Flow_Aufnehmen_Konkretisieren</bpmn:outgoing>
    </bpmn:userTask>
    
    <!-- User Task: Kundenseitige Anforderungen konkretisieren -->
    <bpmn:userTask id="Task_Anforderungen_konkretisieren" name="Kundenseitige Anforderungen konkretisieren">
      <bpmn:incoming>Flow_Aufnehmen_Konkretisieren</bpmn:incoming>
      <bpmn:outgoing>Flow_Konkretisieren_Weiterleiten</bpmn:outgoing>
    </bpmn:userTask>
    
    <!-- User Task: Unterlagen strukturieren und weiterleiten -->
    <bpmn:userTask id="Task_Unterlagen_weiterleiten" name="Unterlagen strukturieren und weiterleiten">
      <bpmn:incoming>Flow_Konkretisieren_Weiterleiten</bpmn:incoming>
      <bpmn:outgoing>Flow_Weiterleiten_Warten</bpmn:outgoing>
    </bpmn:userTask>
    
    <!-- Intermediate Catch Event: Ergebnis erhalten -->
    <bpmn:intermediateCatchEvent id="Event_Ergebnis_erhalten" name="Planungsergebnis erhalten">
      <bpmn:incoming>Flow_Weiterleiten_Warten</bpmn:incoming>
      <bpmn:outgoing>Flow_Ergebnis_Erlaeutern</bpmn:outgoing>
      <bpmn:messageEventDefinition id="MessageEventDef_Ergebnis" messageRef="Message_Ergebnis" />
    </bpmn:intermediateCatchEvent>
    
    <!-- User Task: Ergebnis erläutern und Anwendung unterstützen -->
    <bpmn:userTask id="Task_Ergebnis_erlaeutern" name="Ergebnis erläutern und Anwendung unterstützen">
      <bpmn:incoming>Flow_Ergebnis_Erlaeutern</bpmn:incoming>
      <bpmn:outgoing>Flow_Erlaeutern_Kommunizieren</bpmn:outgoing>
    </bpmn:userTask>
    
    <!-- User Task: Ergebnis kommunizieren -->
    <bpmn:userTask id="Task_Ergebnis_kommunizieren" name="Ergebnis kommunizieren">
      <bpmn:incoming>Flow_Erlaeutern_Kommunizieren</bpmn:incoming>
      <bpmn:outgoing>Flow_Kommunizieren_Ende</bpmn:outgoing>
    </bpmn:userTask>
    
    <!-- End Event -->
    <bpmn:endEvent id="EndEvent_PS_Dezentral" name="Beratung abgeschlossen">
      <bpmn:incoming>Flow_Kommunizieren_Ende</bpmn:incoming>
    </bpmn:endEvent>
    
    <!-- Sequence Flows - PS Dezentral -->
    <bpmn:sequenceFlow id="Flow_Aufnehmen_Konkretisieren"
                       sourceRef="Task_Anfrage_aufnehmen"
                       targetRef="Task_Anforderungen_konkretisieren" />
    
    <bpmn:sequenceFlow id="Flow_Konkretisieren_Weiterleiten"
                       sourceRef="Task_Anforderungen_konkretisieren"
                       targetRef="Task_Unterlagen_weiterleiten" />
    
    <bpmn:sequenceFlow id="Flow_Weiterleiten_Warten"
                       sourceRef="Task_Unterlagen_weiterleiten"
                       targetRef="Event_Ergebnis_erhalten" />
    
    <bpmn:sequenceFlow id="Flow_Ergebnis_Erlaeutern"
                       sourceRef="Event_Ergebnis_erhalten"
                       targetRef="Task_Ergebnis_erlaeutern" />
    
    <bpmn:sequenceFlow id="Flow_Erlaeutern_Kommunizieren"
                       sourceRef="Task_Ergebnis_erlaeutern"
                       targetRef="Task_Ergebnis_kommunizieren" />
    
    <bpmn:sequenceFlow id="Flow_Kommunizieren_Ende"
                       sourceRef="Task_Ergebnis_kommunizieren"
                       targetRef="EndEvent_PS_Dezentral" />
  </bpmn:process>

  <!-- =====================================================
       PERSONALSTELLE ZENTRAL PROZESS (ganzheitliche Bearbeitung)
       ===================================================== -->
  <bpmn:process id="Process_PS_Zentral"
                name="Strategische_Personalbedarfsplanung_Zentral"
                isExecutable="true">
    
    <!-- Start Event -->
    <bpmn:startEvent id="StartEvent_Unterlagen_erhalten" name="Unterlagen erhalten">
      <bpmn:outgoing>Flow_Start_Rahmen</bpmn:outgoing>
    </bpmn:startEvent>
    
    <!-- User Task: Planungsrahmen festlegen -->
    <bpmn:userTask id="Task_Planungsrahmen_festlegen" name="Planungsrahmen festlegen">
      <bpmn:incoming>Flow_Start_Rahmen</bpmn:incoming>
      <bpmn:outgoing>Flow_Rahmen_Ermitteln</bpmn:outgoing>
    </bpmn:userTask>
    
    <!-- User Task: Personalbedarfe ermitteln und konsolidieren -->
    <bpmn:userTask id="Task_Bedarfe_ermitteln" name="Personalbedarfe ermitteln und konsolidieren">
      <bpmn:incoming>Flow_Rahmen_Ermitteln</bpmn:incoming>
      <bpmn:outgoing>Flow_Ermitteln_Dokumentieren</bpmn:outgoing>
    </bpmn:userTask>
    
    <!-- User Task: Strategische Planung dokumentieren -->
    <bpmn:userTask id="Task_Planung_dokumentieren" name="Strategische Planung dokumentieren">
      <bpmn:incoming>Flow_Ermitteln_Dokumentieren</bpmn:incoming>
      <bpmn:outgoing>Flow_Dokumentieren_Bereitstellen</bpmn:outgoing>
    </bpmn:userTask>
    
    <!-- User Task: Ergebnis bereitstellen -->
    <bpmn:userTask id="Task_Ergebnis_bereitstellen" name="Ergebnis bereitstellen">
      <bpmn:incoming>Flow_Dokumentieren_Bereitstellen</bpmn:incoming>
      <bpmn:outgoing>Flow_Bereitstellen_Ende</bpmn:outgoing>
    </bpmn:userTask>
    
    <!-- End Event -->
    <bpmn:endEvent id="EndEvent_PS_Zentral" name="Planung bereitgestellt">
      <bpmn:incoming>Flow_Bereitstellen_Ende</bpmn:incoming>
    </bpmn:endEvent>
    
    <!-- Sequence Flows - PS Zentral -->
    <bpmn:sequenceFlow id="Flow_Start_Rahmen"
                       sourceRef="StartEvent_Unterlagen_erhalten"
                       targetRef="Task_Planungsrahmen_festlegen" />
    
    <bpmn:sequenceFlow id="Flow_Rahmen_Ermitteln"
                       sourceRef="Task_Planungsrahmen_festlegen"
                       targetRef="Task_Bedarfe_ermitteln" />
    
    <bpmn:sequenceFlow id="Flow_Ermitteln_Dokumentieren"
                       sourceRef="Task_Bedarfe_ermitteln"
                       targetRef="Task_Planung_dokumentieren" />
    
    <bpmn:sequenceFlow id="Flow_Dokumentieren_Bereitstellen"
                       sourceRef="Task_Planung_dokumentieren"
                       targetRef="Task_Ergebnis_bereitstellen" />
    
    <bpmn:sequenceFlow id="Flow_Bereitstellen_Ende"
                       sourceRef="Task_Ergebnis_bereitstellen"
                       targetRef="EndEvent_PS_Zentral" />
  </bpmn:process>

  <!-- =====================================================
       MESSAGE DEFINITIONEN
       ===================================================== -->
  <bpmn:message id="Message_Planungsanfrage" name="Planungsanfrage" />
  <bpmn:message id="Message_Planung" name="Strategische Personalbedarfsplanung" />
  <bpmn:message id="Message_Ergebnis" name="Planungsergebnis" />

  <!-- =====================================================
       BPMN-DI VISUALISIERUNG
       ===================================================== -->
  <bpmndi:BPMNDiagram id="BPMNDiagram_Strategische_Personalbedarfsplanung">
    <bpmndi:BPMNPlane id="BPMNPlane_Strategische_Personalbedarfsplanung" bpmnElement="Collab_Strategische_Personalbedarfsplanung">
      
