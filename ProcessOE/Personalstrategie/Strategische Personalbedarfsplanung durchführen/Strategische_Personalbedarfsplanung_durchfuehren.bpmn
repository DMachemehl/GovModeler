      <bpmndi:BPMNEdge id="Flow_Weiterleiten_Warten_di" bpmnElement="Flow_Weiterleiten_Warten">
        <di:waypoint x="730" y="540" />
        <di:waypoint x="792" y="540" />
      </bpmndi:BPMNEdge>
      
      <bpmndi:BPMNEdge id="Flow_Ergebnis_Erlaeutern_di" bpmnElement="Flow_Ergebnis_Erlaeutern">
