        <dc:Bounds x="310" y="500" width="100" height="80" />
      </bpmndi:BPMNShape>
      
      <bpmndi:BPMNShape id="Task_Anforderungen_konkretisieren_di" bpmnElement="Task_Anforderungen_konkretisieren">
        <dc:Bounds x="470" y="500" width="100" height="80" />
      </bpmndi:BPMNShape>
      
      <bpmndi:BPMNShape id="Task_Unterlagen_weiterleiten_di" bpmnElement="Task_Unterlagen_weiterleiten">
        <dc:Bounds x="630" y="500" width="100" height="80" />
      </bpmndi:BPMNShape>
      
      <bpmndi:BPMNShape id="Event_Ergebnis_erhalten_di" bpmnElement="Event_Ergebnis_erhalten">
