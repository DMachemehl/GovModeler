      </bpmndi:BPMNShape>
      
      <!-- PS Dezentral Sequence Flows -->
      <bpmndi:BPMNEdge id="Flow_Aufnehmen_Konkretisieren_di" bpmnElement="Flow_Aufnehmen_Konkretisieren">
        <di:waypoint x="410" y="540" />
        <di:waypoint x="470" y="540" />
      </bpmndi:BPMNEdge>
      
      <bpmndi:BPMNEdge id="Flow_Konkretisieren_Weiterleiten_di" bpmnElement="Flow_Konkretisieren_Weiterleiten">
