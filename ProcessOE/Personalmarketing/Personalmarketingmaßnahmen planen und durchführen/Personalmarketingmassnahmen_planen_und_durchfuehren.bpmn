<?xml version="1.0" encoding="UTF-8"?>
<bpmn:definitions
  xmlns:bpmn="http://www.omg.org/spec/BPMN/20100524/MODEL"
  xmlns:bpmndi="http://www.omg.org/spec/BPMN/20100524/DI"
  xmlns:dc="http://www.omg.org/spec/DD/20100524/DC"
  xmlns:di="http://www.omg.org/spec/DD/20100524/DI"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xmlns:camunda="http://camunda.org/schema/1.0/bpmn"
  targetNamespace="http://example.com/bpmn/personalmarketing"
  exporter="GitHub-Copilot-GovModeler"
  exporterVersion="2.0"
  modeler:executionPlatform="Camunda Platform"
  modeler:executionPlatformVersion="7.20.0">

  <!-- =====================================================
       COLLABORATION - Personalmarketingmassnahmen planen und durchfuehren
       ===================================================== -->
  <bpmn:collaboration id="Collab_Personalmarketing">
    
    <!-- Participant: Buerger/Zielgruppe -->
    <bpmn:participant id="Participant_Buerger"
                      name="Buerger"
                      processRef="Process_Buerger" />
    
    <!-- Participant: IT -->
    <bpmn:participant id="Participant_IT"
                      name="IT"
                      processRef="Process_IT" />
    
    <!-- Participant: Behoerde/Personalmarketing -->
    <bpmn:participant id="Participant_Behoerde"
                      name="Personalmarketing"
                      processRef="Process_Behoerde" />
    
    <!-- Message Flows zwischen Participanten -->
    <bpmn:messageFlow id="MsgFlow_Briefing"
                      name="Briefing"
                      sourceRef="Task_Briefing_bereitstellen"
                      targetRef="StartEvent_IT" />
    
    <bpmn:messageFlow id="MsgFlow_Content_Freigabe"
                      name="Content zur Freigabe"
                      sourceRef="Task_Content_erstellen"
                      targetRef="Event_Entwuerfe_erhalten" />
    
    <bpmn:messageFlow id="MsgFlow_Kampagne_Ausspielung"
                      name="Kampagne"
                      sourceRef="Task_Kampagne_ausspielen"
                      targetRef="StartEvent_Buerger" />
    
    <bpmn:messageFlow id="MsgFlow_Interesse"
                      name="Interesse"
                      sourceRef="Task_Interesse_bekunden"
                      targetRef="Event_Lead_erhalten" />
    
    <bpmn:messageFlow id="MsgFlow_Lead_Uebergabe"
                      name="Qualifizierte Leads"
                      sourceRef="Task_Lead_Erfassung"
                      targetRef="Event_Leads_erhalten" />
    
    <bpmn:messageFlow id="MsgFlow_Performance_Report"
                      name="Performance-Report"
                      sourceRef="Task_Performance_reporten"
                      targetRef="Event_Report_erhalten" />
    
    <bpmn:messageFlow id="MsgFlow_Rueckmeldung"
                      name="Bestaetigung"
                      sourceRef="Task_Ergebnisse_bewerten"
                      targetRef="Event_Rueckmeldung_erhalten" />
  </bpmn:collaboration>

  <!-- =====================================================
       BUERGER PROZESS - Zielgruppe/Interessierte
       ===================================================== -->
  <bpmn:process id="Process_Buerger"
                name="Personalmarketing_Buerger"
                isExecutable="true">
    
    <!-- Start Event -->
    <bpmn:startEvent id="StartEvent_Buerger" name="Kampagne wahrgenommen">
      <bpmn:outgoing>Flow_Start_Konsumieren</bpmn:outgoing>
    </bpmn:startEvent>
    
    <!-- User Task: Informationen konsumieren -->
    <bpmn:userTask id="Task_Informationen_konsumieren" name="Informationen konsumieren">
      <bpmn:incoming>Flow_Start_Konsumieren</bpmn:incoming>
      <bpmn:outgoing>Flow_Konsumieren_Interesse</bpmn:outgoing>
    </bpmn:userTask>
    
    <!-- User Task: Interesse bekunden -->
    <bpmn:userTask id="Task_Interesse_bekunden" name="Interesse bekunden">
      <bpmn:incoming>Flow_Konsumieren_Interesse</bpmn:incoming>
      <bpmn:outgoing>Flow_Interesse_Warten</bpmn:outgoing>
    </bpmn:userTask>
    
    <!-- Intermediate Catch Event: Rueckmeldung erhalten -->
    <bpmn:intermediateCatchEvent id="Event_Rueckmeldung_erhalten" name="Rueckmeldung erhalten">
      <bpmn:incoming>Flow_Interesse_Warten</bpmn:incoming>
      <bpmn:outgoing>Flow_Rueckmeldung_Ende</bpmn:outgoing>
      <bpmn:messageEventDefinition id="MessageEventDef_Rueckmeldung" messageRef="Message_Rueckmeldung" />
    </bpmn:intermediateCatchEvent>
    
    <!-- End Event -->
    <bpmn:endEvent id="EndEvent_Buerger" name="Prozess abgeschlossen">
      <bpmn:incoming>Flow_Rueckmeldung_Ende</bpmn:incoming>
    </bpmn:endEvent>
    
    <!-- Sequence Flows - Buerger -->
    <bpmn:sequenceFlow id="Flow_Start_Konsumieren"
                       sourceRef="StartEvent_Buerger"
                       targetRef="Task_Informationen_konsumieren" />
    
    <bpmn:sequenceFlow id="Flow_Konsumieren_Interesse"
                       sourceRef="Task_Informationen_konsumieren"
                       targetRef="Task_Interesse_bekunden" />
    
    <bpmn:sequenceFlow id="Flow_Interesse_Warten"
                       sourceRef="Task_Interesse_bekunden"
                       targetRef="Event_Rueckmeldung_erhalten" />
    
    <bpmn:sequenceFlow id="Flow_Rueckmeldung_Ende"
                       sourceRef="Event_Rueckmeldung_erhalten"
                       targetRef="EndEvent_Buerger" />
  </bpmn:process>

  <!-- =====================================================
       BEHOERDEN PROZESS - Personalmarketing
       ===================================================== -->
  <bpmn:process id="Process_Behoerde"
                name="Personalmarketing_Behoerde"
                isExecutable="true">
    
    <!-- Start Event -->
    <bpmn:startEvent id="StartEvent_Behoerde" name="Personalmarketingbedarf festgestellt">
      <bpmn:outgoing>Flow_Start_Zielsetzung</bpmn:outgoing>
    </bpmn:startEvent>
    
    <!-- User Task: Zielsetzung definieren -->
    <bpmn:userTask id="Task_Zielsetzung_definieren" name="Zielsetzung definieren">
      <bpmn:incoming>Flow_Start_Zielsetzung</bpmn:incoming>
      <bpmn:outgoing>Flow_Zielsetzung_Briefing</bpmn:outgoing>
    </bpmn:userTask>
    
    <!-- User Task: Briefing bereitstellen -->
    <bpmn:userTask id="Task_Briefing_bereitstellen" name="Briefing bereitstellen">
      <bpmn:incoming>Flow_Zielsetzung_Briefing</bpmn:incoming>
      <bpmn:outgoing>Flow_Briefing_Warten</bpmn:outgoing>
    </bpmn:userTask>
    
    <!-- Intermediate Catch Event: Entwuerfe erhalten -->
    <bpmn:intermediateCatchEvent id="Event_Entwuerfe_erhalten" name="Entwuerfe erhalten">
      <bpmn:incoming>Flow_Briefing_Warten</bpmn:incoming>
      <bpmn:outgoing>Flow_Entwuerfe_Freigabe</bpmn:outgoing>
      <bpmn:messageEventDefinition id="MessageEventDef_Content" messageRef="Message_Content" />
    </bpmn:intermediateCatchEvent>
    
    <!-- User Task: Inhalte freigeben -->
    <bpmn:userTask id="Task_Inhalte_freigeben" name="Inhalte freigeben">
      <bpmn:incoming>Flow_Entwuerfe_Freigabe</bpmn:incoming>
      <bpmn:outgoing>Flow_Freigabe_Leads</bpmn:outgoing>
    </bpmn:userTask>
    
    <!-- Intermediate Catch Event: Leads erhalten -->
    <bpmn:intermediateCatchEvent id="Event_Leads_erhalten" name="Leads erhalten">
      <bpmn:incoming>Flow_Freigabe_Leads</bpmn:incoming>
      <bpmn:outgoing>Flow_Leads_Report</bpmn:outgoing>
      <bpmn:messageEventDefinition id="MessageEventDef_Leads" messageRef="Message_Leads" />
    </bpmn:intermediateCatchEvent>
    
    <!-- Intermediate Catch Event: Report erhalten -->
    <bpmn:intermediateCatchEvent id="Event_Report_erhalten" name="Report erhalten">
      <bpmn:incoming>Flow_Leads_Report</bpmn:incoming>
      <bpmn:outgoing>Flow_Report_Bewerten</bpmn:outgoing>
      <bpmn:messageEventDefinition id="MessageEventDef_Report" messageRef="Message_Report" />
    </bpmn:intermediateCatchEvent>
    
    <!-- User Task: Ergebnisse bewerten -->
    <bpmn:userTask id="Task_Ergebnisse_bewerten" name="Ergebnisse bewerten">
      <bpmn:incoming>Flow_Report_Bewerten</bpmn:incoming>
      <bpmn:outgoing>Flow_Bewerten_Ende</bpmn:outgoing>
    </bpmn:userTask>
    
    <!-- End Event -->
    <bpmn:endEvent id="EndEvent_Behoerde" name="Massnahmen durchgefuehrt">
      <bpmn:incoming>Flow_Bewerten_Ende</bpmn:incoming>
    </bpmn:endEvent>
    
    <!-- Sequence Flows - Behoerde -->
    <bpmn:sequenceFlow id="Flow_Start_Zielsetzung"
                       sourceRef="StartEvent_Behoerde"
                       targetRef="Task_Zielsetzung_definieren" />
    
    <bpmn:sequenceFlow id="Flow_Zielsetzung_Briefing"
                       sourceRef="Task_Zielsetzung_definieren"
                       targetRef="Task_Briefing_bereitstellen" />
    
    <bpmn:sequenceFlow id="Flow_Briefing_Warten"
                       sourceRef="Task_Briefing_bereitstellen"
                       targetRef="Event_Entwuerfe_erhalten" />
    
    <bpmn:sequenceFlow id="Flow_Entwuerfe_Freigabe"
                       sourceRef="Event_Entwuerfe_erhalten"
                       targetRef="Task_Inhalte_freigeben" />
    
    <bpmn:sequenceFlow id="Flow_Freigabe_Leads"
                       sourceRef="Task_Inhalte_freigeben"
                       targetRef="Event_Leads_erhalten" />
    
    <bpmn:sequenceFlow id="Flow_Leads_Report"
                       sourceRef="Event_Leads_erhalten"
                       targetRef="Event_Report_erhalten" />
    
    <bpmn:sequenceFlow id="Flow_Report_Bewerten"
                       sourceRef="Event_Report_erhalten"
                       targetRef="Task_Ergebnisse_bewerten" />
    
    <bpmn:sequenceFlow id="Flow_Bewerten_Ende"
                       sourceRef="Task_Ergebnisse_bewerten"
                       targetRef="EndEvent_Behoerde" />
  </bpmn:process>

  <!-- =====================================================
       IT-PROZESS
       ===================================================== -->
  <bpmn:process id="Process_IT"
                name="IT_Prozess"
                isExecutable="true">
    
    <!-- Start Event -->
    <bpmn:startEvent id="StartEvent_IT" name="Briefing erhalten">
      <bpmn:outgoing>Flow_IT_Start_Konfigurieren</bpmn:outgoing>
    </bpmn:startEvent>
    
    <!-- Service Task: Kampagne konfigurieren -->
    <bpmn:serviceTask id="Task_Kampagne_konfigurieren" name="Kampagne konfigurieren">
      <bpmn:incoming>Flow_IT_Start_Konfigurieren</bpmn:incoming>
      <bpmn:outgoing>Flow_Konfigurieren_Content</bpmn:outgoing>
    </bpmn:serviceTask>
    
    <!-- Service Task: Content erstellen -->
    <bpmn:serviceTask id="Task_Content_erstellen" name="Content erstellen und bereitstellen">
      <bpmn:incoming>Flow_Konfigurieren_Content</bpmn:incoming>
      <bpmn:outgoing>Flow_Content_Ausspielen</bpmn:outgoing>
    </bpmn:serviceTask>
    
    <!-- Service Task: Kampagne ausspielen -->
    <bpmn:serviceTask id="Task_Kampagne_ausspielen" name="Kampagne ausspielen">
      <bpmn:incoming>Flow_Content_Ausspielen</bpmn:incoming>
      <bpmn:outgoing>Flow_Ausspielen_Lead</bpmn:outgoing>
    </bpmn:serviceTask>
    
    <!-- Intermediate Catch Event: Lead erhalten -->
    <bpmn:intermediateCatchEvent id="Event_Lead_erhalten" name="Lead erhalten">
      <bpmn:incoming>Flow_Ausspielen_Lead</bpmn:incoming>
      <bpmn:outgoing>Flow_Lead_Erfassung</bpmn:outgoing>
      <bpmn:messageEventDefinition id="MessageEventDef_Lead" messageRef="Message_Lead" />
    </bpmn:intermediateCatchEvent>
    
    <!-- Service Task: Lead-Erfassung verarbeiten -->
    <bpmn:serviceTask id="Task_Lead_Erfassung" name="Lead-Erfassung verarbeiten">
      <bpmn:incoming>Flow_Lead_Erfassung</bpmn:incoming>
      <bpmn:outgoing>Flow_Lead_Performance</bpmn:outgoing>
    </bpmn:serviceTask>
    
    <!-- Service Task: Performance reporten -->
    <bpmn:serviceTask id="Task_Performance_reporten" name="Performance reporten">
      <bpmn:incoming>Flow_Lead_Performance</bpmn:incoming>
      <bpmn:outgoing>Flow_Performance_Ende</bpmn:outgoing>
    </bpmn:serviceTask>
    
    <!-- End Event -->
    <bpmn:endEvent id="EndEvent_IT" name="Kampagne abgeschlossen">
      <bpmn:incoming>Flow_Performance_Ende</bpmn:incoming>
    </bpmn:endEvent>
    
    <!-- Sequence Flows - IT-Prozess -->
    <bpmn:sequenceFlow id="Flow_IT_Start_Konfigurieren"
                       sourceRef="StartEvent_IT"
                       targetRef="Task_Kampagne_konfigurieren" />
    
    <bpmn:sequenceFlow id="Flow_Konfigurieren_Content"
                       sourceRef="Task_Kampagne_konfigurieren"
                       targetRef="Task_Content_erstellen" />
    
    <bpmn:sequenceFlow id="Flow_Content_Ausspielen"
                       sourceRef="Task_Content_erstellen"
                       targetRef="Task_Kampagne_ausspielen" />
    
    <bpmn:sequenceFlow id="Flow_Ausspielen_Lead"
                       sourceRef="Task_Kampagne_ausspielen"
                       targetRef="Event_Lead_erhalten" />
    
    <bpmn:sequenceFlow id="Flow_Lead_Erfassung"
                       sourceRef="Event_Lead_erhalten"
                       targetRef="Task_Lead_Erfassung" />
    
    <bpmn:sequenceFlow id="Flow_Lead_Performance"
                       sourceRef="Task_Lead_Erfassung"
                       targetRef="Task_Performance_reporten" />
    
    <bpmn:sequenceFlow id="Flow_Performance_Ende"
                       sourceRef="Task_Performance_reporten"
                       targetRef="EndEvent_IT" />
  </bpmn:process>

  <!-- =====================================================
       MESSAGE DEFINITIONEN
       ===================================================== -->
  <bpmn:message id="Message_Briefing" name="Briefing" />
  <bpmn:message id="Message_Content" name="Content zur Freigabe" />
  <bpmn:message id="Message_Kampagne" name="Kampagne" />
  <bpmn:message id="Message_Lead" name="Interesse" />
  <bpmn:message id="Message_Leads" name="Qualifizierte Leads" />
  <bpmn:message id="Message_Report" name="Performance-Report" />
  <bpmn:message id="Message_Rueckmeldung" name="Bestaetigung" />

  <!-- =====================================================
