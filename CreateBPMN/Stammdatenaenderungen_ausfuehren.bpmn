      <!-- Message Flow: Pruefvorgang (IT -> Behörde) -->
      <bpmndi:BPMNEdge id="MsgFlow_Pruefvorgang_di" bpmnElement="MsgFlow_Pruefvorgang">
        <di:waypoint x="680" y="535" />
        <di:waypoint x="680" y="645" />
        <di:waypoint x="360" y="645" />
        <di:waypoint x="360" y="782" />
        <bpmndi:BPMNLabel>
