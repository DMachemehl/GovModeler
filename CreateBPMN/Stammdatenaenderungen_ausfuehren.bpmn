      </bpmndi:BPMNShape>
      
      <bpmndi:BPMNShape id="Event_Bestaetigung_empfangen_di" bpmnElement="Event_Bestaetigung_empfangen">
        <dc:Bounds x="472" y="182" width="36" height="36" />
        <bpmndi:BPMNLabel>
          <dc:Bounds x="450" y="225" width="80" height="27" />
        </bpmndi:BPMNLabel>
      </bpmndi:BPMNShape>
      
      <bpmndi:BPMNShape id="EndEvent_Buerger_di" bpmnElement="EndEvent_Buerger">
