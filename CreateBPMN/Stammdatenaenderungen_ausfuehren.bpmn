        <dc:Bounds x="222" y="782" width="36" height="36" />
        <bpmndi:BPMNLabel>
          <dc:Bounds x="200" y="825" width="80" height="27" />
        </bpmndi:BPMNLabel>
      </bpmndi:BPMNShape>
      
      <bpmndi:BPMNShape id="Event_Pruefvorgang_erhalten_di" bpmnElement="Event_Pruefvorgang_erhalten">
