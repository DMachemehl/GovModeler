        <di:waypoint x="258" y="495" />
        <di:waypoint x="310" y="495" />
      </bpmndi:BPMNEdge>
      
      <bpmndi:BPMNEdge id="Flow_Empfangen_Pruefen_di" bpmnElement="Flow_Empfangen_Pruefen">
        <di:waypoint x="410" y="495" />
        <di:waypoint x="470" y="495" />
      </bpmndi:BPMNEdge>
      
      <bpmndi:BPMNEdge id="Flow_Pruefen_Uebermitteln_di" bpmnElement="Flow_Pruefen_Uebermitteln">
