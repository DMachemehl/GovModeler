        </bpmndi:BPMNLabel>
      </bpmndi:BPMNEdge>
      
      <!-- Message Flow: Freigabe (Behörde -> IT) -->
      <bpmndi:BPMNEdge id="MsgFlow_Freigabe_di" bpmnElement="MsgFlow_Freigabe">
