<?xml version="1.0" encoding="UTF-8"?>
<bpmn:definitions
  xmlns:bpmn="http://www.omg.org/spec/BPMN/20100524/MODEL"
  xmlns:bpmndi="http://www.omg.org/spec/BPMN/20100524/DI"
  xmlns:dc="http://www.omg.org/spec/DD/20100524/DC"
  xmlns:di="http://www.omg.org/spec/DD/20100524/DI"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xmlns:camunda="http://camunda.org/schema/1.0/bpmn"
  targetNamespace="http://example.com/bpmn"
  exporter="GitHub-Copilot-GovModeler"
  exporterVersion="2.0"
  modeler:executionPlatform="Camunda Platform"
  modeler:executionPlatformVersion="7.20.0">

  <!-- =====================================================
       COLLABORATION - Prozess-Interaktion
       ===================================================== -->
  <bpmn:collaboration id="Collab_Stammdatenaenderungen">
    
    <!-- Participant: Bürger/Antragsteller -->
    <bpmn:participant id="Participant_Buerger"
                      name="Buerger"
                      processRef="Process_Buerger" />
    
    <!-- Participant: IT -->
    <bpmn:participant id="Participant_IT"
                      name="IT"
                      processRef="Process_IT" />
    
    <!-- Participant: Behörde -->
    <bpmn:participant id="Participant_Behoerde"
                      name="Behoerde"
                      processRef="Process_Behoerde" />
    
    <!-- Message Flows zwischen Participanten -->
    <bpmn:messageFlow id="MsgFlow_Antrag"
                      name="Aenderungsantrag"
                      sourceRef="Task_Antrag_uebermitteln"
                      targetRef="Task_Antrag_empfangen" />
    
    <bpmn:messageFlow id="MsgFlow_Pruefvorgang"
                      name="Pruefvorgang"
                      sourceRef="Task_Pruefvorgang_uebermitteln"
                      targetRef="Event_Pruefvorgang_erhalten" />
    
    <bpmn:messageFlow id="MsgFlow_Freigabe"
                      name="Freigabe"
                      sourceRef="Task_Aenderung_freigeben"
                      targetRef="Event_Freigabe_erhalten" />
    
    <bpmn:messageFlow id="MsgFlow_Bestaetigung"
                      name="Bestaetigung"
                      sourceRef="Task_Bestaetigung_versenden"
                      targetRef="Event_Bestaetigung_empfangen" />
  </bpmn:collaboration>

  <!-- =====================================================
       BÜRGER PROZESS
       ===================================================== -->
  <bpmn:process id="Process_Buerger"
                name="Stammdatenaenderung_Buerger"
                isExecutable="true">
    
    <!-- Start Event -->
    <bpmn:startEvent id="StartEvent_Buerger" name="Aenderungsantrag eingegangen">
      <bpmn:outgoing>Flow_Start_Uebermitteln</bpmn:outgoing>
    </bpmn:startEvent>
    
    <!-- User Task: Aenderungsantrag uebermitteln -->
    <bpmn:userTask id="Task_Antrag_uebermitteln" name="Aenderungsantrag uebermitteln" camunda:formKey="embedded:app:forms/Stammdatenaenderung_Antrag.form">
      <bpmn:incoming>Flow_Start_Uebermitteln</bpmn:incoming>
      <bpmn:outgoing>Flow_Uebermitteln_Bestaetigung</bpmn:outgoing>
    </bpmn:userTask>
    
    <!-- Intermediate Catch Event: Bestaetigung empfangen -->
    <bpmn:intermediateCatchEvent id="Event_Bestaetigung_empfangen" name="Bestaetigung empfangen">
      <bpmn:incoming>Flow_Uebermitteln_Bestaetigung</bpmn:incoming>
      <bpmn:outgoing>Flow_Bestaetigung_Ende</bpmn:outgoing>
      <bpmn:messageEventDefinition id="MessageEventDef_Bestaetigung" messageRef="Message_Bestaetigung" />
    </bpmn:intermediateCatchEvent>
    
    <!-- End Event -->
    <bpmn:endEvent id="EndEvent_Buerger" name="Stammdaten aktualisiert">
      <bpmn:incoming>Flow_Bestaetigung_Ende</bpmn:incoming>
    </bpmn:endEvent>
    
    <!-- Sequence Flows - Bürger -->
    <bpmn:sequenceFlow id="Flow_Start_Uebermitteln"
                       sourceRef="StartEvent_Buerger"
                       targetRef="Task_Antrag_uebermitteln" />
    
    <bpmn:sequenceFlow id="Flow_Uebermitteln_Bestaetigung"
                       sourceRef="Task_Antrag_uebermitteln"
                       targetRef="Event_Bestaetigung_empfangen" />
    
    <bpmn:sequenceFlow id="Flow_Bestaetigung_Ende"
                       sourceRef="Event_Bestaetigung_empfangen"
                       targetRef="EndEvent_Buerger" />
  </bpmn:process>

  <!-- =====================================================
       IT-PROZESS
       ===================================================== -->
  <bpmn:process id="Process_IT"
                name="IT_Prozess"
                isExecutable="true">
    
    <!-- Start Event -->
    <bpmn:startEvent id="StartEvent_IT" name="System gestartet">
      <bpmn:outgoing>Flow_IT_Start_Empfangen</bpmn:outgoing>
    </bpmn:startEvent>
    
    <!-- Service Task: Antrag empfangen und registrieren -->
    <bpmn:serviceTask id="Task_Antrag_empfangen" name="Antrag empfangen und registrieren">
      <bpmn:incoming>Flow_IT_Start_Empfangen</bpmn:incoming>
      <bpmn:outgoing>Flow_Empfangen_Pruefen</bpmn:outgoing>
    </bpmn:serviceTask>
    
    <!-- Service Task: Vollstaendigkeit automatisch pruefen -->
    <bpmn:serviceTask id="Task_Vollstaendigkeit_pruefen" name="Vollstaendigkeit automatisch pruefen">
      <bpmn:incoming>Flow_Empfangen_Pruefen</bpmn:incoming>
      <bpmn:outgoing>Flow_Pruefen_Uebermitteln</bpmn:outgoing>
    </bpmn:serviceTask>
    
    <!-- Service Task: Pruefvorgang an Behoerde uebermitteln -->
    <bpmn:serviceTask id="Task_Pruefvorgang_uebermitteln" name="Pruefvorgang an Behoerde uebermitteln">
      <bpmn:incoming>Flow_Pruefen_Uebermitteln</bpmn:incoming>
      <bpmn:outgoing>Flow_Uebermitteln_Freigabe</bpmn:outgoing>
    </bpmn:serviceTask>
    
    <!-- Intermediate Catch Event: Freigabe erhalten -->
    <bpmn:intermediateCatchEvent id="Event_Freigabe_erhalten" name="Freigabe erhalten">
      <bpmn:incoming>Flow_Uebermitteln_Freigabe</bpmn:incoming>
      <bpmn:outgoing>Flow_Freigabe_Aktualisieren</bpmn:outgoing>
      <bpmn:messageEventDefinition id="MessageEventDef_Freigabe" messageRef="Message_Freigabe" />
    </bpmn:intermediateCatchEvent>
    
    <!-- Service Task: Stammdaten aktualisieren -->
    <bpmn:serviceTask id="Task_Stammdaten_aktualisieren" name="Stammdaten aktualisieren">
      <bpmn:incoming>Flow_Freigabe_Aktualisieren</bpmn:incoming>
      <bpmn:outgoing>Flow_Aktualisieren_Versenden</bpmn:outgoing>
    </bpmn:serviceTask>
    
    <!-- Service Task: Bestaetigung erstellen und versenden -->
    <bpmn:serviceTask id="Task_Bestaetigung_versenden" name="Bestaetigung erstellen und versenden">
      <bpmn:incoming>Flow_Aktualisieren_Versenden</bpmn:incoming>
      <bpmn:outgoing>Flow_Versenden_Ende</bpmn:outgoing>
    </bpmn:serviceTask>
    
    <!-- End Event -->
    <bpmn:endEvent id="EndEvent_IT" name="System abgeschlossen">
      <bpmn:incoming>Flow_Versenden_Ende</bpmn:incoming>
    </bpmn:endEvent>
    
    <!-- Sequence Flows - IT-Prozess -->
    <bpmn:sequenceFlow id="Flow_IT_Start_Empfangen"
                       sourceRef="StartEvent_IT"
                       targetRef="Task_Antrag_empfangen" />
    
    <bpmn:sequenceFlow id="Flow_Empfangen_Pruefen"
                       sourceRef="Task_Antrag_empfangen"
                       targetRef="Task_Vollstaendigkeit_pruefen" />
    
    <bpmn:sequenceFlow id="Flow_Pruefen_Uebermitteln"
                       sourceRef="Task_Vollstaendigkeit_pruefen"
                       targetRef="Task_Pruefvorgang_uebermitteln" />
    
    <bpmn:sequenceFlow id="Flow_Uebermitteln_Freigabe"
                       sourceRef="Task_Pruefvorgang_uebermitteln"
                       targetRef="Event_Freigabe_erhalten" />
    
    <bpmn:sequenceFlow id="Flow_Freigabe_Aktualisieren"
                       sourceRef="Event_Freigabe_erhalten"
                       targetRef="Task_Stammdaten_aktualisieren" />
    
    <bpmn:sequenceFlow id="Flow_Aktualisieren_Versenden"
                       sourceRef="Task_Stammdaten_aktualisieren"
                       targetRef="Task_Bestaetigung_versenden" />
    
    <bpmn:sequenceFlow id="Flow_Versenden_Ende"
                       sourceRef="Task_Bestaetigung_versenden"
                       targetRef="EndEvent_IT" />
  </bpmn:process>

  <!-- =====================================================
       BEHÖRDEN PROZESS
       ===================================================== -->
  <bpmn:process id="Process_Behoerde"
                name="Stammdatenaenderung_Behoerde"
                isExecutable="true">
    
    <!-- Start Event -->
    <bpmn:startEvent id="StartEvent_Behoerde" name="Vorgang eingegangen">
      <bpmn:outgoing>Flow_Start_Pruefvorgang</bpmn:outgoing>
    </bpmn:startEvent>
    
    <!-- Intermediate Catch Event: Pruefvorgang erhalten -->
    <bpmn:intermediateCatchEvent id="Event_Pruefvorgang_erhalten" name="Pruefvorgang erhalten">
      <bpmn:incoming>Flow_Start_Pruefvorgang</bpmn:incoming>
      <bpmn:outgoing>Flow_Pruefvorgang_Pruefen</bpmn:outgoing>
      <bpmn:messageEventDefinition id="MessageEventDef_Pruefvorgang" messageRef="Message_Pruefvorgang" />
    </bpmn:intermediateCatchEvent>
    
    <!-- User Task: Aenderungsantrag fachlich pruefen -->
    <bpmn:userTask id="Task_Antrag_pruefen" name="Aenderungsantrag fachlich pruefen">
      <bpmn:incoming>Flow_Pruefvorgang_Pruefen</bpmn:incoming>
      <bpmn:outgoing>Flow_Pruefen_Freigeben</bpmn:outgoing>
    </bpmn:userTask>
    
    <!-- User Task: Aenderung freigeben -->
    <bpmn:userTask id="Task_Aenderung_freigeben" name="Aenderung freigeben">
      <bpmn:incoming>Flow_Pruefen_Freigeben</bpmn:incoming>
      <bpmn:outgoing>Flow_Freigeben_Ende</bpmn:outgoing>
    </bpmn:userTask>
    
    <!-- End Event -->
    <bpmn:endEvent id="EndEvent_Behoerde" name="Freigabe erteilt">
      <bpmn:incoming>Flow_Freigeben_Ende</bpmn:incoming>
    </bpmn:endEvent>
    
    <!-- Sequence Flows - Behörde -->
    <bpmn:sequenceFlow id="Flow_Start_Pruefvorgang"
                       sourceRef="StartEvent_Behoerde"
                       targetRef="Event_Pruefvorgang_erhalten" />
    
    <bpmn:sequenceFlow id="Flow_Pruefvorgang_Pruefen"
                       sourceRef="Event_Pruefvorgang_erhalten"
                       targetRef="Task_Antrag_pruefen" />
    
    <bpmn:sequenceFlow id="Flow_Pruefen_Freigeben"
                       sourceRef="Task_Antrag_pruefen"
                       targetRef="Task_Aenderung_freigeben" />
    
    <bpmn:sequenceFlow id="Flow_Freigeben_Ende"
                       sourceRef="Task_Aenderung_freigeben"
                       targetRef="EndEvent_Behoerde" />
  </bpmn:process>

  <!-- =====================================================
       MESSAGE DEFINITIONEN
       ===================================================== -->
  <bpmn:message id="Message_Bestaetigung" name="Bestaetigung" />
  <bpmn:message id="Message_Pruefvorgang" name="Pruefvorgang" />
  <bpmn:message id="Message_Freigabe" name="Freigabe" />

