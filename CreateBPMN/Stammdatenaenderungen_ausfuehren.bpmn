      <bpmndi:BPMNShape id="Task_Antrag_pruefen_di" bpmnElement="Task_Antrag_pruefen">
        <dc:Bounds x="470" y="760" width="100" height="80" />
      </bpmndi:BPMNShape>
      
      <bpmndi:BPMNShape id="Task_Aenderung_freigeben_di" bpmnElement="Task_Aenderung_freigeben">
        <dc:Bounds x="630" y="760" width="100" height="80" />
      </bpmndi:BPMNShape>
      
      <bpmndi:BPMNShape id="EndEvent_Behoerde_di" bpmnElement="EndEvent_Behoerde">
        <dc:Bounds x="792" y="782" width="36" height="36" />
        <bpmndi:BPMNLabel>
          <dc:Bounds x="770" y="825" width="80" height="27" />
        </bpmndi:BPMNLabel>
      </bpmndi:BPMNShape>
      
      <!-- Sequence Flows - Behörde -->
      <bpmndi:BPMNEdge id="Flow_Start_Pruefvorgang_di" bpmnElement="Flow_Start_Pruefvorgang">
