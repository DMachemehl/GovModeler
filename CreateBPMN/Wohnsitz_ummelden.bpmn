<?xml version="1.0" encoding="UTF-8"?>
<bpmn:definitions
  xmlns:bpmn="http://www.omg.org/spec/BPMN/20100524/MODEL"
  xmlns:bpmndi="http://www.omg.org/spec/BPMN/20100524/DI"
  xmlns:dc="http://www.omg.org/spec/DD/20100524/DC"
  xmlns:di="http://www.omg.org/spec/DD/20100524/DI"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xmlns:camunda="http://camunda.org/schema/1.0/bpmn"
  targetNamespace="http://example.com/bpmn"
  exporter="GitHub-Copilot-GovModeler"
  exporterVersion="2.0"
  modeler:executionPlatform="Camunda Platform"
  modeler:executionPlatformVersion="7.20.0">

  <!-- =====================================================
       COLLABORATION - Haupt-Container für Prozess-Interaktion
       ===================================================== -->
  <bpmn:collaboration id="Collab_Wohnsitz_ummelden">
    
    <!-- Participant: Bürger/Antragsteller -->
    <bpmn:participant id="Participant_Buerger"
                      name="Buerger"
                      processRef="Process_Buerger" />
    
    <!-- Participant: IT -->
    <bpmn:participant id="Participant_IT"
                      name="IT"
                      processRef="Process_IT" />
    
    <!-- Participant: Meldebehörde -->
    <bpmn:participant id="Participant_Meldebehoerde"
                      name="Meldebehoerde"
                      processRef="Process_Meldebehoerde" />
    
    <!-- Message Flows zwischen Participanten -->
    <bpmn:messageFlow id="MsgFlow_Ummeldungsantrag"
                      name="Ummeldungsantrag"
                      sourceRef="Task_Ummeldung_beantragen"
                      targetRef="StartEvent_Ummeldungsantrag_eingegangen" />
    
    <bpmn:messageFlow id="MsgFlow_Meldebestaetigung"
                      name="Meldebestaetigung"
                      sourceRef="Task_Meldebestaetigung_ausstellen"
                      targetRef="Event_Meldebestaetigung_erhalten" />
    
    <!-- IT Message Flows -->
    <bpmn:messageFlow id="MsgFlow_Datenabgleich_Anfrage"
                      name="Datenabgleich-Anfrage"
                      sourceRef="Task_Datenabgleich_beauftragen"
                      targetRef="StartEvent_Datenabgleich_Anfrage_erhalten" />
                      
    <bpmn:messageFlow id="MsgFlow_Datenabgleich_Antwort"
                      name="Datenabgleich-Ergebnis"
                      sourceRef="Task_Datenabgleich_Antwort_senden"
                      targetRef="Event_Datenabgleich_Antwort_erhalten" />
  </bpmn:collaboration>

  <!-- =====================================================
       BÜRGER PROZESS - Ummeldung beantragen
       ===================================================== -->
  <bpmn:process id="Process_Buerger"
                name="Wohnsitz_ummelden_Buerger"
                isExecutable="true">
    
    <!-- Start Event -->
    <bpmn:startEvent id="StartEvent_Buerger" name="Umzug durchgefuehrt">
      <bpmn:outgoing>Flow_Start_Ummeldung</bpmn:outgoing>
    </bpmn:startEvent>
    
    <!-- User Task: Ummeldung beantragen -->
    <bpmn:userTask id="Task_Ummeldung_beantragen" name="Ummeldung beantragen">
      <bpmn:incoming>Flow_Start_Ummeldung</bpmn:incoming>
      <bpmn:outgoing>Flow_Ummeldung_Unterlagen</bpmn:outgoing>
    </bpmn:userTask>
    
    <!-- User Task: Unterlagen einreichen -->
    <bpmn:userTask id="Task_Unterlagen_einreichen" name="Unterlagen einreichen">
      <bpmn:incoming>Flow_Ummeldung_Unterlagen</bpmn:incoming>
      <bpmn:outgoing>Flow_Unterlagen_Warten</bpmn:outgoing>
    </bpmn:userTask>
    
    <!-- Intermediate Catch Event: Meldebestätigung erhalten -->
    <bpmn:intermediateCatchEvent id="Event_Meldebestaetigung_erhalten" name="Meldebestaetigung erhalten">
      <bpmn:incoming>Flow_Unterlagen_Warten</bpmn:incoming>
      <bpmn:outgoing>Flow_Meldebestaetigung_Ende</bpmn:outgoing>
      <bpmn:messageEventDefinition id="MessageEventDef_Meldebestaetigung" messageRef="Message_Meldebestaetigung" />
    </bpmn:intermediateCatchEvent>
    
    <!-- End Event -->
    <bpmn:endEvent id="EndEvent_Buerger" name="Ummeldung abgeschlossen">
      <bpmn:incoming>Flow_Meldebestaetigung_Ende</bpmn:incoming>
    </bpmn:endEvent>
    
    <!-- Sequence Flows - Bürger -->
    <bpmn:sequenceFlow id="Flow_Start_Ummeldung"
                       sourceRef="StartEvent_Buerger"
                       targetRef="Task_Ummeldung_beantragen" />
    
    <bpmn:sequenceFlow id="Flow_Ummeldung_Unterlagen"
                       sourceRef="Task_Ummeldung_beantragen"
                       targetRef="Task_Unterlagen_einreichen" />
    
    <bpmn:sequenceFlow id="Flow_Unterlagen_Warten"
                       sourceRef="Task_Unterlagen_einreichen"
                       targetRef="Event_Meldebestaetigung_erhalten" />
    
    <bpmn:sequenceFlow id="Flow_Meldebestaetigung_Ende"
                       sourceRef="Event_Meldebestaetigung_erhalten"
                       targetRef="EndEvent_Buerger" />
  </bpmn:process>

  <!-- =====================================================
       IT PROZESS - Datenabgleich und Melderegister
       ===================================================== -->
  <bpmn:process id="Process_IT"
                name="IT_Prozess"
                isExecutable="true">
    
    <!-- Start Event -->
    <bpmn:startEvent id="StartEvent_Datenabgleich_Anfrage_erhalten" name="Datenabgleich-Anfrage erhalten">
      <bpmn:outgoing>Flow_IT_Start_Erfassen</bpmn:outgoing>
    </bpmn:startEvent>
    
    <!-- Service Task: Antragsdaten erfassen -->
    <bpmn:serviceTask id="Task_Antragsdaten_erfassen" name="Antragsdaten erfassen">
      <bpmn:incoming>Flow_IT_Start_Erfassen</bpmn:incoming>
      <bpmn:outgoing>Flow_IT_Erfassen_Abgleich</bpmn:outgoing>
    </bpmn:serviceTask>
    
    <!-- Service Task: Datenabgleich durchführen -->
    <bpmn:serviceTask id="Task_Datenabgleich_durchfuehren" name="Datenabgleich durchfuehren">
      <bpmn:incoming>Flow_IT_Erfassen_Abgleich</bpmn:incoming>
      <bpmn:outgoing>Flow_IT_Abgleich_Aktualisieren</bpmn:outgoing>
    </bpmn:serviceTask>
    
    <!-- Service Task: Melderegister aktualisieren -->
    <bpmn:serviceTask id="Task_Melderegister_aktualisieren" name="Melderegister aktualisieren">
      <bpmn:incoming>Flow_IT_Abgleich_Aktualisieren</bpmn:incoming>
      <bpmn:outgoing>Flow_IT_Aktualisieren_Antwort</bpmn:outgoing>
    </bpmn:serviceTask>
    
    <!-- Service Task: Antwort senden -->
    <bpmn:serviceTask id="Task_Datenabgleich_Antwort_senden" name="Datenabgleich-Antwort senden">
      <bpmn:incoming>Flow_IT_Aktualisieren_Antwort</bpmn:incoming>
      <bpmn:outgoing>Flow_IT_Antwort_Ende</bpmn:outgoing>
    </bpmn:serviceTask>
    
    <!-- End Event -->
    <bpmn:endEvent id="EndEvent_IT" name="Datenabgleich abgeschlossen">
      <bpmn:incoming>Flow_IT_Antwort_Ende</bpmn:incoming>
    </bpmn:endEvent>
    
    <!-- Sequence Flows - IT-Prozess -->
    <bpmn:sequenceFlow id="Flow_IT_Start_Erfassen"
                       sourceRef="StartEvent_Datenabgleich_Anfrage_erhalten"
                       targetRef="Task_Antragsdaten_erfassen" />
    
    <bpmn:sequenceFlow id="Flow_IT_Erfassen_Abgleich"
                       sourceRef="Task_Antragsdaten_erfassen"
                       targetRef="Task_Datenabgleich_durchfuehren" />
    
    <bpmn:sequenceFlow id="Flow_IT_Abgleich_Aktualisieren"
                       sourceRef="Task_Datenabgleich_durchfuehren"
                       targetRef="Task_Melderegister_aktualisieren" />
    
    <bpmn:sequenceFlow id="Flow_IT_Aktualisieren_Antwort"
                       sourceRef="Task_Melderegister_aktualisieren"
                       targetRef="Task_Datenabgleich_Antwort_senden" />
    
    <bpmn:sequenceFlow id="Flow_IT_Antwort_Ende"
                       sourceRef="Task_Datenabgleich_Antwort_senden"
                       targetRef="EndEvent_IT" />
  </bpmn:process>

  <!-- =====================================================
       MELDEBEHÖRDE PROZESS - Ummeldung bearbeiten
       ===================================================== -->
  <bpmn:process id="Process_Meldebehoerde"
                name="Wohnsitz_ummelden_Meldebehoerde"
                isExecutable="true">
    
    <!-- Start Event -->
    <bpmn:startEvent id="StartEvent_Ummeldungsantrag_eingegangen" name="Ummeldungsantrag eingegangen">
      <bpmn:outgoing>Flow_Start_Zustaendigkeit</bpmn:outgoing>
    </bpmn:startEvent>
    
    <!-- Service Task: Zuständigkeit prüfen -->
    <bpmn:serviceTask id="Task_Zustaendigkeit_pruefen" name="Zustaendigkeit pruefen">
      <bpmn:incoming>Flow_Start_Zustaendigkeit</bpmn:incoming>
      <bpmn:outgoing>Flow_Zustaendigkeit_Unterlagen</bpmn:outgoing>
    </bpmn:serviceTask>
    
    <!-- User Task: Unterlagen prüfen -->
    <bpmn:userTask id="Task_Unterlagen_pruefen" name="Unterlagen pruefen">
      <bpmn:incoming>Flow_Zustaendigkeit_Unterlagen</bpmn:incoming>
      <bpmn:outgoing>Flow_Unterlagen_Wohnberechtigung</bpmn:outgoing>
    </bpmn:userTask>
    
    <!-- User Task: Wohnsitzberechtigung bestätigen -->
    <bpmn:userTask id="Task_Wohnsitzberechtigung_bestaetigen" name="Wohnsitzberechtigung bestaetigen">
      <bpmn:incoming>Flow_Unterlagen_Wohnberechtigung</bpmn:incoming>
      <bpmn:outgoing>Flow_Wohnberechtigung_Abmeldung</bpmn:outgoing>
    </bpmn:userTask>
    
    <!-- User Task: Abmeldung veranlassen -->
    <bpmn:userTask id="Task_Abmeldung_veranlassen" name="Abmeldung veranlassen">
      <bpmn:incoming>Flow_Wohnberechtigung_Abmeldung</bpmn:incoming>
      <bpmn:outgoing>Flow_Abmeldung_Datenabgleich</bpmn:outgoing>
    </bpmn:userTask>
    
    <!-- User Task: Datenabgleich beauftragen -->
    <bpmn:userTask id="Task_Datenabgleich_beauftragen" name="Datenabgleich beauftragen">
      <bpmn:incoming>Flow_Abmeldung_Datenabgleich</bpmn:incoming>
      <bpmn:outgoing>Flow_Datenabgleich_Warten</bpmn:outgoing>
    </bpmn:userTask>
    
    <!-- Intermediate Catch Event: IT-Antwort erhalten -->
    <bpmn:intermediateCatchEvent id="Event_Datenabgleich_Antwort_erhalten" name="Datenabgleich-Antwort erhalten">
      <bpmn:incoming>Flow_Datenabgleich_Warten</bpmn:incoming>
      <bpmn:outgoing>Flow_IT_Antwort_Anmeldung</bpmn:outgoing>
      <bpmn:messageEventDefinition id="MessageEventDef_IT_Antwort" messageRef="Message_IT_Antwort" />
    </bpmn:intermediateCatchEvent>
    
    <!-- User Task: Anmeldung durchführen -->
    <bpmn:userTask id="Task_Anmeldung_durchfuehren" name="Anmeldung durchfuehren">
      <bpmn:incoming>Flow_IT_Antwort_Anmeldung</bpmn:incoming>
      <bpmn:outgoing>Flow_Anmeldung_Bescheid</bpmn:outgoing>
    </bpmn:userTask>
    
    <!-- User Task: Meldebestätigung ausstellen -->
    <bpmn:userTask id="Task_Meldebestaetigung_ausstellen" name="Meldebestaetigung ausstellen">
      <bpmn:incoming>Flow_Anmeldung_Bescheid</bpmn:incoming>
      <bpmn:outgoing>Flow_Bescheid_Ende</bpmn:outgoing>
    </bpmn:userTask>
    
    <!-- End Event -->
    <bpmn:endEvent id="EndEvent_Meldebehoerde" name="Ummeldung registriert">
      <bpmn:incoming>Flow_Bescheid_Ende</bpmn:incoming>
    </bpmn:endEvent>
    
    <!-- Sequence Flows - Meldebehörde -->
    <bpmn:sequenceFlow id="Flow_Start_Zustaendigkeit"
                       sourceRef="StartEvent_Ummeldungsantrag_eingegangen"
                       targetRef="Task_Zustaendigkeit_pruefen" />
    
    <bpmn:sequenceFlow id="Flow_Zustaendigkeit_Unterlagen"
                       sourceRef="Task_Zustaendigkeit_pruefen"
                       targetRef="Task_Unterlagen_pruefen" />
    
    <bpmn:sequenceFlow id="Flow_Unterlagen_Wohnberechtigung"
                       sourceRef="Task_Unterlagen_pruefen"
                       targetRef="Task_Wohnsitzberechtigung_bestaetigen" />
    
    <bpmn:sequenceFlow id="Flow_Wohnberechtigung_Abmeldung"
                       sourceRef="Task_Wohnsitzberechtigung_bestaetigen"
                       targetRef="Task_Abmeldung_veranlassen" />
    
    <bpmn:sequenceFlow id="Flow_Abmeldung_Datenabgleich"
                       sourceRef="Task_Abmeldung_veranlassen"
                       targetRef="Task_Datenabgleich_beauftragen" />
    
    <bpmn:sequenceFlow id="Flow_Datenabgleich_Warten"
                       sourceRef="Task_Datenabgleich_beauftragen"
                       targetRef="Event_Datenabgleich_Antwort_erhalten" />
    
    <bpmn:sequenceFlow id="Flow_IT_Antwort_Anmeldung"
                       sourceRef="Event_Datenabgleich_Antwort_erhalten"
                       targetRef="Task_Anmeldung_durchfuehren" />
    
    <bpmn:sequenceFlow id="Flow_Anmeldung_Bescheid"
                       sourceRef="Task_Anmeldung_durchfuehren"
                       targetRef="Task_Meldebestaetigung_ausstellen" />
    
    <bpmn:sequenceFlow id="Flow_Bescheid_Ende"
                       sourceRef="Task_Meldebestaetigung_ausstellen"
                       targetRef="EndEvent_Meldebehoerde" />
  </bpmn:process>

  <!-- =====================================================
       MESSAGE DEFINITIONEN
       ===================================================== -->
  <bpmn:message id="Message_Ummeldungsantrag" name="Ummeldungsantrag" />
  <bpmn:message id="Message_Meldebestaetigung" name="Meldebestaetigung" />
  <bpmn:message id="Message_Datenabgleich_Anfrage" name="Datenabgleich-Anfrage" />
  <bpmn:message id="Message_IT_Antwort" name="Datenabgleich-Ergebnis" />

  <!-- =====================================================
       BPMN-DI VISUALISIERUNG - Basiert auf Guidelines
       ===================================================== -->
  <bpmndi:BPMNDiagram id="BPMNDiagram_Wohnsitz_ummelden">
    <bpmndi:BPMNPlane id="BPMNPlane_Wohnsitz_ummelden" bpmnElement="Collab_Wohnsitz_ummelden">
      
      <!-- ===============================
           POOL: BÜRGER
           =============================== -->
      <bpmndi:BPMNShape id="Participant_Buerger_di" bpmnElement="Participant_Buerger" isHorizontal="true">
        <dc:Bounds x="160" y="80" width="1000" height="300" />
        <bpmndi:BPMNLabel>
          <dc:Bounds x="170" y="215" width="20" height="50" />
        </bpmndi:BPMNLabel>
      </bpmndi:BPMNShape>
      
      <!-- Bürger Process Elements -->
      <bpmndi:BPMNShape id="StartEvent_Buerger_di" bpmnElement="StartEvent_Buerger">
        <dc:Bounds x="222" y="182" width="36" height="36" />
        <bpmndi:BPMNLabel>
          <dc:Bounds x="200" y="225" width="80" height="27" />
        </bpmndi:BPMNLabel>
      </bpmndi:BPMNShape>
      
      <bpmndi:BPMNShape id="Task_Ummeldung_beantragen_di" bpmnElement="Task_Ummeldung_beantragen">
        <dc:Bounds x="310" y="160" width="100" height="80" />
      </bpmndi:BPMNShape>
      
      <bpmndi:BPMNShape id="Task_Unterlagen_einreichen_di" bpmnElement="Task_Unterlagen_einreichen">
        <dc:Bounds x="470" y="160" width="100" height="80" />
      </bpmndi:BPMNShape>
      
      <bpmndi:BPMNShape id="Event_Meldebestaetigung_erhalten_di" bpmnElement="Event_Meldebestaetigung_erhalten">
        <dc:Bounds x="622" y="182" width="36" height="36" />
        <bpmndi:BPMNLabel>
          <dc:Bounds x="600" y="225" width="80" height="27" />
        </bpmndi:BPMNLabel>
      </bpmndi:BPMNShape>
      
      <bpmndi:BPMNShape id="EndEvent_Buerger_di" bpmnElement="EndEvent_Buerger">
        <dc:Bounds x="712" y="182" width="36" height="36" />
        <bpmndi:BPMNLabel>
          <dc:Bounds x="690" y="225" width="80" height="27" />
        </bpmndi:BPMNLabel>
      </bpmndi:BPMNShape>
      
      <!-- ===============================
           POOL: IT
           =============================== -->
      <bpmndi:BPMNShape id="Participant_IT_di" bpmnElement="Participant_IT" isHorizontal="true">
        <dc:Bounds x="160" y="420" width="1200" height="300" />
        <bpmndi:BPMNLabel>
          <dc:Bounds x="170" y="555" width="20" height="30" />
        </bpmndi:BPMNLabel>
      </bpmndi:BPMNShape>
      
      <!-- IT Process Elements -->
      <bpmndi:BPMNShape id="StartEvent_Datenabgleich_Anfrage_erhalten_di" bpmnElement="StartEvent_Datenabgleich_Anfrage_erhalten">
        <dc:Bounds x="222" y="552" width="36" height="36" />
        <bpmndi:BPMNLabel>
          <dc:Bounds x="200" y="595" width="80" height="27" />
        </bpmndi:BPMNLabel>
      </bpmndi:BPMNShape>
      
      <bpmndi:BPMNShape id="Task_Antragsdaten_erfassen_di" bpmnElement="Task_Antragsdaten_erfassen">
        <dc:Bounds x="310" y="530" width="100" height="80" />
      </bpmndi:BPMNShape>
      
      <bpmndi:BPMNShape id="Task_Datenabgleich_durchfuehren_di" bpmnElement="Task_Datenabgleich_durchfuehren">
        <dc:Bounds x="470" y="530" width="100" height="80" />
      </bpmndi:BPMNShape>
      
      <bpmndi:BPMNShape id="Task_Melderegister_aktualisieren_di" bpmnElement="Task_Melderegister_aktualisieren">
        <dc:Bounds x="630" y="530" width="100" height="80" />
      </bpmndi:BPMNShape>
      
      <bpmndi:BPMNShape id="Task_Datenabgleich_Antwort_senden_di" bpmnElement="Task_Datenabgleich_Antwort_senden">
        <dc:Bounds x="790" y="530" width="100" height="80" />
      </bpmndi:BPMNShape>
      
      <bpmndi:BPMNShape id="EndEvent_IT_di" bpmnElement="EndEvent_IT">
        <dc:Bounds x="942" y="552" width="36" height="36" />
        <bpmndi:BPMNLabel>
          <dc:Bounds x="920" y="595" width="80" height="27" />
        </bpmndi:BPMNLabel>
      </bpmndi:BPMNShape>
      
      <!-- ===============================
           POOL: MELDEBEHÖRDE
           =============================== -->
      <bpmndi:BPMNShape id="Participant_Meldebehoerde_di" bpmnElement="Participant_Meldebehoerde" isHorizontal="true">
        <dc:Bounds x="160" y="760" width="1600" height="300" />
        <bpmndi:BPMNLabel>
          <dc:Bounds x="170" y="895" width="20" height="80" />
        </bpmndi:BPMNLabel>
      </bpmndi:BPMNShape>
      
      <!-- Meldebehörde Process Elements - Alle horizontal auf Y=895 -->
      <bpmndi:BPMNShape id="StartEvent_Ummeldungsantrag_eingegangen_di" bpmnElement="StartEvent_Ummeldungsantrag_eingegangen">
        <dc:Bounds x="222" y="917" width="36" height="36" />
        <bpmndi:BPMNLabel>
          <dc:Bounds x="200" y="960" width="80" height="27" />
        </bpmndi:BPMNLabel>
      </bpmndi:BPMNShape>
      
      <bpmndi:BPMNShape id="Task_Zustaendigkeit_pruefen_di" bpmnElement="Task_Zustaendigkeit_pruefen">
        <dc:Bounds x="310" y="895" width="100" height="80" />
      </bpmndi:BPMNShape>
      
      <bpmndi:BPMNShape id="Task_Unterlagen_pruefen_di" bpmnElement="Task_Unterlagen_pruefen">
        <dc:Bounds x="470" y="895" width="100" height="80" />
      </bpmndi:BPMNShape>
      
      <bpmndi:BPMNShape id="Task_Wohnsitzberechtigung_bestaetigen_di" bpmnElement="Task_Wohnsitzberechtigung_bestaetigen">
        <dc:Bounds x="630" y="895" width="100" height="80" />
      </bpmndi:BPMNShape>
      
      <bpmndi:BPMNShape id="Task_Abmeldung_veranlassen_di" bpmnElement="Task_Abmeldung_veranlassen">
        <dc:Bounds x="790" y="895" width="100" height="80" />
      </bpmndi:BPMNShape>
      
      <bpmndi:BPMNShape id="Task_Datenabgleich_beauftragen_di" bpmnElement="Task_Datenabgleich_beauftragen">
        <dc:Bounds x="950" y="895" width="100" height="80" />
      </bpmndi:BPMNShape>
      
      <bpmndi:BPMNShape id="Event_Datenabgleich_Antwort_erhalten_di" bpmnElement="Event_Datenabgleich_Antwort_erhalten">
        <dc:Bounds x="1102" y="917" width="36" height="36" />
        <bpmndi:BPMNLabel>
          <dc:Bounds x="1080" y="960" width="80" height="27" />
        </bpmndi:BPMNLabel>
      </bpmndi:BPMNShape>
      
      <bpmndi:BPMNShape id="Task_Anmeldung_durchfuehren_di" bpmnElement="Task_Anmeldung_durchfuehren">
        <dc:Bounds x="1200" y="895" width="100" height="80" />
      </bpmndi:BPMNShape>
      
      <bpmndi:BPMNShape id="Task_Meldebestaetigung_ausstellen_di" bpmnElement="Task_Meldebestaetigung_ausstellen">
        <dc:Bounds x="1360" y="895" width="100" height="80" />
      </bpmndi:BPMNShape>
      
      <bpmndi:BPMNShape id="EndEvent_Meldebehoerde_di" bpmnElement="EndEvent_Meldebehoerde">
        <dc:Bounds x="1502" y="917" width="36" height="36" />
        <bpmndi:BPMNLabel>
          <dc:Bounds x="1480" y="960" width="80" height="27" />
        </bpmndi:BPMNLabel>
      </bpmndi:BPMNShape>
      
      <!-- ===============================
           SEQUENCE FLOWS - BÜRGER
           =============================== -->
      <bpmndi:BPMNEdge id="Flow_Start_Ummeldung_di" bpmnElement="Flow_Start_Ummeldung">
        <di:waypoint x="258" y="200" />
        <di:waypoint x="310" y="200" />
      </bpmndi:BPMNEdge>
      
      <bpmndi:BPMNEdge id="Flow_Ummeldung_Unterlagen_di" bpmnElement="Flow_Ummeldung_Unterlagen">
        <di:waypoint x="410" y="200" />
        <di:waypoint x="470" y="200" />
      </bpmndi:BPMNEdge>
      
      <bpmndi:BPMNEdge id="Flow_Unterlagen_Warten_di" bpmnElement="Flow_Unterlagen_Warten">
        <di:waypoint x="570" y="200" />
        <di:waypoint x="622" y="200" />
      </bpmndi:BPMNEdge>
      
      <bpmndi:BPMNEdge id="Flow_Meldebestaetigung_Ende_di" bpmnElement="Flow_Meldebestaetigung_Ende">
        <di:waypoint x="658" y="200" />
        <di:waypoint x="712" y="200" />
      </bpmndi:BPMNEdge>
      
      <!-- ===============================
