<?xml version="1.0" encoding="UTF-8"?>
<bpmn:definitions
  xmlns:bpmn="http://www.omg.org/spec/BPMN/20100524/MODEL"
  xmlns:bpmndi="http://www.omg.org/spec/BPMN/20100524/DI"
  xmlns:dc="http://www.omg.org/spec/DD/20100524/DC"
  xmlns:di="http://www.omg.org/spec/DD/20100524/DI"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xmlns:camunda="http://camunda.org/schema/1.0/bpmn"
  targetNamespace="http://example.com/bpmn"
  exporter="GitHub-Copilot-GovModeler"
  exporterVersion="2.0"
  modeler:executionPlatform="Camunda Platform"
  modeler:executionPlatformVersion="7.20.0">

  <!-- =====================================================
       COLLABORATION - Grundsätze des Personalmanagements aufstellen
       ===================================================== -->
  <bpmn:collaboration id="Collab_Grundsaetze_Personalmanagement">
    
    <!-- Participant: Kunde -->
    <bpmn:participant id="Participant_Kunde"
                      name="Kunde (Organisationseinheit)"
                      processRef="Process_Kunde" />
    
    <!-- Participant: Personalstelle dezentral -->
    <bpmn:participant id="Participant_PS_Dezentral"
                      name="Personalstelle dezentral"
                      processRef="Process_PS_Dezentral" />
    
    <!-- Participant: Personalstelle zentral -->
    <bpmn:participant id="Participant_PS_Zentral"
                      name="Personalstelle zentral"
                      processRef="Process_PS_Zentral" />
    
    <!-- Message Flows zwischen Participanten -->
    <bpmn:messageFlow id="MsgFlow_Anfrage_Kunde_Dezentral"
                      name="Anfrage"
                      sourceRef="Task_Anfrage_uebermitteln"
                      targetRef="Task_Anfrage_aufnehmen" />
    
    <bpmn:messageFlow id="MsgFlow_Anfrage_Dezentral_Zentral"
                      name="Anfrage weitergeleitet"
                      sourceRef="Task_Anfrage_weiterleiten"
                      targetRef="StartEvent_Auftrag_erhalten" />
    
    <bpmn:messageFlow id="MsgFlow_Abstimmung_Extern"
                      name="Abstimmung Tarifpartner/KVV"
                      sourceRef="Task_Externe_Abstimmung"
                      targetRef="Event_Abstimmung_abgeschlossen" />
    
    <bpmn:messageFlow id="MsgFlow_Unterlagen_Zentral_Dezentral"
                      name="Finale Unterlagen"
                      sourceRef="Task_Unterlagen_bereitstellen"
                      targetRef="Task_Kundenberatung" />
    
    <bpmn:messageFlow id="MsgFlow_Ergebnis_Dezentral_Kunde"
                      name="Grundsätze/Regelungen"
                      sourceRef="Task_Ergebnis_kommunizieren"
                      targetRef="Event_Ergebnis_erhalten" />
  </bpmn:collaboration>

  <!-- =====================================================
       KUNDE PROZESS
       ===================================================== -->
  <bpmn:process id="Process_Kunde"
                name="Grundsaetze_PM_Kunde"
                isExecutable="true">
    
    <!-- Start Event -->
    <bpmn:startEvent id="StartEvent_Kunde" name="Regelungsbedarf gemeldet">
      <bpmn:outgoing>Flow_Start_Formulieren</bpmn:outgoing>
    </bpmn:startEvent>
    
    <!-- User Task: Regelungsbedarf formulieren -->
    <bpmn:userTask id="Task_Regelungsbedarf_formulieren" name="Regelungsbedarf formulieren">
      <bpmn:incoming>Flow_Start_Formulieren</bpmn:incoming>
      <bpmn:outgoing>Flow_Formulieren_Uebermitteln</bpmn:outgoing>
    </bpmn:userTask>
    
    <!-- User Task: Anfrage übermitteln -->
    <bpmn:userTask id="Task_Anfrage_uebermitteln" name="Anfrage übermitteln">
      <bpmn:incoming>Flow_Formulieren_Uebermitteln</bpmn:incoming>
      <bpmn:outgoing>Flow_Uebermitteln_Warten</bpmn:outgoing>
    </bpmn:userTask>
    
    <!-- Intermediate Catch Event: Ergebnis erhalten -->
    <bpmn:intermediateCatchEvent id="Event_Ergebnis_erhalten" name="Grundsätze/Regelungen erhalten">
      <bpmn:incoming>Flow_Uebermitteln_Warten</bpmn:incoming>
      <bpmn:outgoing>Flow_Ergebnis_Anwenden</bpmn:outgoing>
      <bpmn:messageEventDefinition id="MessageEventDef_Ergebnis" messageRef="Message_Ergebnis" />
    </bpmn:intermediateCatchEvent>
    
    <!-- User Task: Grundsätze anwenden -->
    <bpmn:userTask id="Task_Grundsaetze_anwenden" name="Grundsätze anwenden">
      <bpmn:incoming>Flow_Ergebnis_Anwenden</bpmn:incoming>
      <bpmn:outgoing>Flow_Anwenden_Ende</bpmn:outgoing>
    </bpmn:userTask>
    
    <!-- End Event -->
    <bpmn:endEvent id="EndEvent_Kunde" name="Grundsätze angewendet">
      <bpmn:incoming>Flow_Anwenden_Ende</bpmn:incoming>
    </bpmn:endEvent>
    
    <!-- Sequence Flows - Kunde -->
    <bpmn:sequenceFlow id="Flow_Start_Formulieren"
                       sourceRef="StartEvent_Kunde"
                       targetRef="Task_Regelungsbedarf_formulieren" />
    
    <bpmn:sequenceFlow id="Flow_Formulieren_Uebermitteln"
                       sourceRef="Task_Regelungsbedarf_formulieren"
                       targetRef="Task_Anfrage_uebermitteln" />
    
    <bpmn:sequenceFlow id="Flow_Uebermitteln_Warten"
                       sourceRef="Task_Anfrage_uebermitteln"
                       targetRef="Event_Ergebnis_erhalten" />
    
    <bpmn:sequenceFlow id="Flow_Ergebnis_Anwenden"
                       sourceRef="Event_Ergebnis_erhalten"
                       targetRef="Task_Grundsaetze_anwenden" />
    
    <bpmn:sequenceFlow id="Flow_Anwenden_Ende"
                       sourceRef="Task_Grundsaetze_anwenden"
                       targetRef="EndEvent_Kunde" />
  </bpmn:process>

  <!-- =====================================================
       PERSONALSTELLE DEZENTRAL PROZESS (serviceorientiert beratend)
       ===================================================== -->
  <bpmn:process id="Process_PS_Dezentral"
                name="Grundsaetze_PM_Dezentral"
                isExecutable="true">
    
    <!-- User Task: Anfrage aufnehmen -->
    <bpmn:userTask id="Task_Anfrage_aufnehmen" name="Anfrage aufnehmen">
      <bpmn:outgoing>Flow_Aufnehmen_Konkretisieren</bpmn:outgoing>
    </bpmn:userTask>
    
    <!-- User Task: Beratungs- und Unterstützungsbedarf konkretisieren -->
    <bpmn:userTask id="Task_Bedarf_konkretisieren" name="Beratungs- und Unterstützungsbedarf konkretisieren">
      <bpmn:incoming>Flow_Aufnehmen_Konkretisieren</bpmn:incoming>
      <bpmn:outgoing>Flow_Konkretisieren_Weiterleiten</bpmn:outgoing>
    </bpmn:userTask>
    
    <!-- User Task: Anfrage weiterleiten -->
    <bpmn:userTask id="Task_Anfrage_weiterleiten" name="Anfrage weiterleiten">
      <bpmn:incoming>Flow_Konkretisieren_Weiterleiten</bpmn:incoming>
      <bpmn:outgoing>Flow_Weiterleiten_Warten</bpmn:outgoing>
    </bpmn:userTask>
    
    <!-- Intermediate Catch Event: Finale Unterlagen erhalten -->
    <bpmn:intermediateCatchEvent id="Event_Unterlagen_erhalten" name="Finale Unterlagen erhalten">
      <bpmn:incoming>Flow_Weiterleiten_Warten</bpmn:incoming>
      <bpmn:outgoing>Flow_Unterlagen_Beratung</bpmn:outgoing>
      <bpmn:messageEventDefinition id="MessageEventDef_Unterlagen" messageRef="Message_Unterlagen" />
    </bpmn:intermediateCatchEvent>
    
    <!-- User Task: Kundenberatung zur Umsetzung -->
    <bpmn:userTask id="Task_Kundenberatung" name="Kundenberatung zur Umsetzung durchführen">
      <bpmn:incoming>Flow_Unterlagen_Beratung</bpmn:incoming>
      <bpmn:outgoing>Flow_Beratung_Kommunizieren</bpmn:outgoing>
    </bpmn:userTask>
    
    <!-- User Task: Ergebnis kommunizieren -->
    <bpmn:userTask id="Task_Ergebnis_kommunizieren" name="Ergebnis kommunizieren">
      <bpmn:incoming>Flow_Beratung_Kommunizieren</bpmn:incoming>
      <bpmn:outgoing>Flow_Kommunizieren_Ende</bpmn:outgoing>
    </bpmn:userTask>
    
    <!-- End Event -->
    <bpmn:endEvent id="EndEvent_PS_Dezentral" name="Beratung abgeschlossen">
      <bpmn:incoming>Flow_Kommunizieren_Ende</bpmn:incoming>
    </bpmn:endEvent>
    
    <!-- Sequence Flows - PS Dezentral -->
    <bpmn:sequenceFlow id="Flow_Aufnehmen_Konkretisieren"
                       sourceRef="Task_Anfrage_aufnehmen"
                       targetRef="Task_Bedarf_konkretisieren" />
    
    <bpmn:sequenceFlow id="Flow_Konkretisieren_Weiterleiten"
                       sourceRef="Task_Bedarf_konkretisieren"
                       targetRef="Task_Anfrage_weiterleiten" />
    
    <bpmn:sequenceFlow id="Flow_Weiterleiten_Warten"
                       sourceRef="Task_Anfrage_weiterleiten"
                       targetRef="Event_Unterlagen_erhalten" />
    
    <bpmn:sequenceFlow id="Flow_Unterlagen_Beratung"
                       sourceRef="Event_Unterlagen_erhalten"
                       targetRef="Task_Kundenberatung" />
    
    <bpmn:sequenceFlow id="Flow_Beratung_Kommunizieren"
                       sourceRef="Task_Kundenberatung"
                       targetRef="Task_Ergebnis_kommunizieren" />
    
    <bpmn:sequenceFlow id="Flow_Kommunizieren_Ende"
                       sourceRef="Task_Ergebnis_kommunizieren"
                       targetRef="EndEvent_PS_Dezentral" />
  </bpmn:process>

  <!-- =====================================================
       PERSONALSTELLE ZENTRAL PROZESS (ganzheitliche Bearbeitung)
       ===================================================== -->
  <bpmn:process id="Process_PS_Zentral"
                name="Grundsaetze_PM_Zentral"
                isExecutable="true">
    
    <!-- Start Event -->
    <bpmn:startEvent id="StartEvent_Auftrag_erhalten" name="Auftrag erhalten">
      <bpmn:outgoing>Flow_Start_Auftrag</bpmn:outgoing>
    </bpmn:startEvent>
    
    <!-- User Task: Auftrag und Rahmen festlegen -->
    <bpmn:userTask id="Task_Auftrag_Rahmen_festlegen" name="Auftrag und Rahmen festlegen">
      <bpmn:incoming>Flow_Start_Auftrag</bpmn:incoming>
      <bpmn:outgoing>Flow_Rahmen_Erarbeiten</bpmn:outgoing>
    </bpmn:userTask>
    
    <!-- User Task: Grundsatz-/Regelwerk erarbeiten -->
    <bpmn:userTask id="Task_Regelwerk_erarbeiten" name="Grundsatz-/Regelwerk erarbeiten">
      <bpmn:incoming>Flow_Rahmen_Erarbeiten</bpmn:incoming>
      <bpmn:outgoing>Flow_Erarbeiten_Abstimmung</bpmn:outgoing>
    </bpmn:userTask>
    
    <!-- User Task: Externe Abstimmung durchführen -->
    <bpmn:userTask id="Task_Externe_Abstimmung" name="Externe Abstimmung durchführen">
      <bpmn:incoming>Flow_Erarbeiten_Abstimmung</bpmn:incoming>
      <bpmn:outgoing>Flow_Abstimmung_Warten</bpmn:outgoing>
    </bpmn:userTask>
    
    <!-- Intermediate Catch Event: Abstimmung abgeschlossen -->
    <bpmn:intermediateCatchEvent id="Event_Abstimmung_abgeschlossen" name="Abstimmung abgeschlossen">
      <bpmn:incoming>Flow_Abstimmung_Warten</bpmn:incoming>
      <bpmn:outgoing>Flow_Abstimmung_Koordinieren</bpmn:outgoing>
      <bpmn:messageEventDefinition id="MessageEventDef_Abstimmung" messageRef="Message_Abstimmung" />
    </bpmn:intermediateCatchEvent>
    
    <!-- User Task: Stadtweiten Vollzug koordinieren -->
    <bpmn:userTask id="Task_Vollzug_koordinieren" name="Stadtweiten Vollzug koordinieren">
      <bpmn:incoming>Flow_Abstimmung_Koordinieren</bpmn:incoming>
      <bpmn:outgoing>Flow_Koordinieren_Bereitstellen</bpmn:outgoing>
    </bpmn:userTask>
    
    <!-- User Task: Finale Unterlagen bereitstellen -->
    <bpmn:userTask id="Task_Unterlagen_bereitstellen" name="Finale Unterlagen bereitstellen">
      <bpmn:incoming>Flow_Koordinieren_Bereitstellen</bpmn:incoming>
      <bpmn:outgoing>Flow_Bereitstellen_Ende</bpmn:outgoing>
    </bpmn:userTask>
    
    <!-- End Event -->
    <bpmn:endEvent id="EndEvent_PS_Zentral" name="Grundsätze bereitgestellt">
      <bpmn:incoming>Flow_Bereitstellen_Ende</bpmn:incoming>
    </bpmn:endEvent>
    
    <!-- Sequence Flows - PS Zentral -->
    <bpmn:sequenceFlow id="Flow_Start_Auftrag"
                       sourceRef="StartEvent_Auftrag_erhalten"
                       targetRef="Task_Auftrag_Rahmen_festlegen" />
    
    <bpmn:sequenceFlow id="Flow_Rahmen_Erarbeiten"
                       sourceRef="Task_Auftrag_Rahmen_festlegen"
                       targetRef="Task_Regelwerk_erarbeiten" />
    
    <bpmn:sequenceFlow id="Flow_Erarbeiten_Abstimmung"
                       sourceRef="Task_Regelwerk_erarbeiten"
                       targetRef="Task_Externe_Abstimmung" />
    
    <bpmn:sequenceFlow id="Flow_Abstimmung_Warten"
                       sourceRef="Task_Externe_Abstimmung"
                       targetRef="Event_Abstimmung_abgeschlossen" />
    
    <bpmn:sequenceFlow id="Flow_Abstimmung_Koordinieren"
                       sourceRef="Event_Abstimmung_abgeschlossen"
                       targetRef="Task_Vollzug_koordinieren" />
    
    <bpmn:sequenceFlow id="Flow_Koordinieren_Bereitstellen"
                       sourceRef="Task_Vollzug_koordinieren"
                       targetRef="Task_Unterlagen_bereitstellen" />
    
    <bpmn:sequenceFlow id="Flow_Bereitstellen_Ende"
                       sourceRef="Task_Unterlagen_bereitstellen"
                       targetRef="EndEvent_PS_Zentral" />
  </bpmn:process>

  <!-- =====================================================
       MESSAGE DEFINITIONEN
       ===================================================== -->
  <bpmn:message id="Message_Anfrage" name="Anfrage" />
  <bpmn:message id="Message_Ergebnis" name="Grundsätze/Regelungen" />
  <bpmn:message id="Message_Unterlagen" name="Finale Unterlagen" />
  <bpmn:message id="Message_Abstimmung" name="Abstimmung abgeschlossen" />

  <!-- =====================================================
       BPMN-DI VISUALISIERUNG
       ===================================================== -->
  <bpmndi:BPMNDiagram id="BPMNDiagram_Grundsaetze_PM">
    <bpmndi:BPMNPlane id="BPMNPlane_Grundsaetze_PM" bpmnElement="Collab_Grundsaetze_Personalmanagement">
      
