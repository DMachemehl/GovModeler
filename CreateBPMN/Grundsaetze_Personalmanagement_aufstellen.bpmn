      </bpmndi:BPMNEdge>
      
      <bpmndi:BPMNEdge id="Flow_Anwenden_Ende_di" bpmnElement="Flow_Anwenden_Ende">
        <di:waypoint x="820" y="200" />
        <di:waypoint x="872" y="200" />
      </bpmndi:BPMNEdge>
      
      <!-- ===============================
           POOL: PERSONALSTELLE DEZENTRAL (Mitte)
           =============================== -->
      <bpmndi:BPMNShape id="Participant_PS_Dezentral_di" bpmnElement="Participant_PS_Dezentral" isHorizontal="true">
        <dc:Bounds x="160" y="420" width="1200" height="300" />
        <bpmndi:BPMNLabel>
          <dc:Bounds x="170" y="535" width="20" height="150" />
        </bpmndi:BPMNLabel>
      </bpmndi:BPMNShape>
      
      <!-- PS Dezentral Process Elements -->
      <bpmndi:BPMNShape id="Task_Anfrage_aufnehmen_di" bpmnElement="Task_Anfrage_aufnehmen">
        <dc:Bounds x="310" y="500" width="100" height="80" />
      </bpmndi:BPMNShape>
      
      <bpmndi:BPMNShape id="Task_Bedarf_konkretisieren_di" bpmnElement="Task_Bedarf_konkretisieren">
        <dc:Bounds x="470" y="500" width="100" height="80" />
      </bpmndi:BPMNShape>
      
      <bpmndi:BPMNShape id="Task_Anfrage_weiterleiten_di" bpmnElement="Task_Anfrage_weiterleiten">
        <dc:Bounds x="630" y="500" width="100" height="80" />
      </bpmndi:BPMNShape>
      
      <bpmndi:BPMNShape id="Event_Unterlagen_erhalten_di" bpmnElement="Event_Unterlagen_erhalten">
        <dc:Bounds x="792" y="522" width="36" height="36" />
        <bpmndi:BPMNLabel>
          <dc:Bounds x="770" y="565" width="80" height="27" />
        </bpmndi:BPMNLabel>
      </bpmndi:BPMNShape>
      
      <bpmndi:BPMNShape id="Task_Kundenberatung_di" bpmnElement="Task_Kundenberatung">
        <dc:Bounds x="880" y="500" width="100" height="80" />
      </bpmndi:BPMNShape>
      
      <bpmndi:BPMNShape id="Task_Ergebnis_kommunizieren_di" bpmnElement="Task_Ergebnis_kommunizieren">
        <dc:Bounds x="1040" y="500" width="100" height="80" />
      </bpmndi:BPMNShape>
      
      <bpmndi:BPMNShape id="EndEvent_PS_Dezentral_di" bpmnElement="EndEvent_PS_Dezentral">
        <dc:Bounds x="1192" y="522" width="36" height="36" />
