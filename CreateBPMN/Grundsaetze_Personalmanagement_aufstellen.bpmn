        <di:waypoint x="880" y="540" />
      </bpmndi:BPMNEdge>
      
      <bpmndi:BPMNEdge id="Flow_Beratung_Kommunizieren_di" bpmnElement="Flow_Beratung_Kommunizieren">
        <di:waypoint x="980" y="540" />
        <di:waypoint x="1040" y="540" />
      </bpmndi:BPMNEdge>
      
      <bpmndi:BPMNEdge id="Flow_Kommunizieren_Ende_di" bpmnElement="Flow_Kommunizieren_Ende">
        <di:waypoint x="1140" y="540" />
        <di:waypoint x="1192" y="540" />
      </bpmndi:BPMNEdge>
      
      <!-- ===============================
           POOL: PERSONALSTELLE ZENTRAL (unten)
           =============================== -->
      <bpmndi:BPMNShape id="Participant_PS_Zentral_di" bpmnElement="Participant_PS_Zentral" isHorizontal="true">
        <dc:Bounds x="160" y="760" width="1200" height="300" />
        <bpmndi:BPMNLabel>
          <dc:Bounds x="170" y="875" width="20" height="150" />
        </bpmndi:BPMNLabel>
      </bpmndi:BPMNShape>
      
      <!-- PS Zentral Process Elements -->
      <bpmndi:BPMNShape id="StartEvent_Auftrag_erhalten_di" bpmnElement="StartEvent_Auftrag_erhalten">
        <dc:Bounds x="222" y="862" width="36" height="36" />
        <bpmndi:BPMNLabel>
          <dc:Bounds x="200" y="905" width="80" height="27" />
        </bpmndi:BPMNLabel>
      </bpmndi:BPMNShape>
      
      <bpmndi:BPMNShape id="Task_Auftrag_Rahmen_festlegen_di" bpmnElement="Task_Auftrag_Rahmen_festlegen">
        <dc:Bounds x="310" y="840" width="100" height="80" />
      </bpmndi:BPMNShape>
      
      <bpmndi:BPMNShape id="Task_Regelwerk_erarbeiten_di" bpmnElement="Task_Regelwerk_erarbeiten">
