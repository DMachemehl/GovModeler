        <bpmndi:BPMNLabel>
          <dc:Bounds x="200" y="225" width="80" height="27" />
        </bpmndi:BPMNLabel>
      </bpmndi:BPMNShape>
      
      <bpmndi:BPMNShape id="Task_Regelungsbedarf_formulieren_di" bpmnElement="Task_Regelungsbedarf_formulieren">
