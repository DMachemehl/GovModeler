      </bpmndi:BPMNShape>
      
      <bpmndi:BPMNShape id="Event_Ergebnis_erhalten_di" bpmnElement="Event_Ergebnis_erhalten">
        <dc:Bounds x="632" y="182" width="36" height="36" />
        <bpmndi:BPMNLabel>
          <dc:Bounds x="610" y="225" width="80" height="27" />
        </bpmndi:BPMNLabel>
      </bpmndi:BPMNShape>
      
      <bpmndi:BPMNShape id="Task_Grundsaetze_anwenden_di" bpmnElement="Task_Grundsaetze_anwenden">
        <dc:Bounds x="720" y="160" width="100" height="80" />
      </bpmndi:BPMNShape>
      
      <bpmndi:BPMNShape id="EndEvent_Kunde_di" bpmnElement="EndEvent_Kunde">
