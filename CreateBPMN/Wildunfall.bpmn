<?xml version="1.0" encoding="UTF-8"?>
<bpmn:definitions
  xmlns:bpmn="http://www.omg.org/spec/BPMN/20100524/MODEL"
  xmlns:bpmndi="http://www.omg.org/spec/BPMN/20100524/DI"
  xmlns:dc="http://www.omg.org/spec/DD/20100524/DC"
  xmlns:di="http://www.omg.org/spec/DD/20100524/DI"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  targetNamespace="http://example.com/bpmn"
  exporter="ChatGPT-Template"
  exporterVersion="1.0" xmlns:camunda="http://camunda.org/schema/1.0/bpmn">

  <bpmn:collaboration id="Collab_Wildunfall">
    <bpmn:participant id="Participant_Buerger"
                      name="Buerger"
                      processRef="Process_Buerger" />

    <bpmn:participant id="Participant_Polizei"
                      name="Polizei"
                      processRef="Process_Polizei" />

    <bpmn:messageFlow id="MsgFlow_Notruf"
                      name="Notruf"
                      sourceRef="Task_NotrufTaetigen"
                      targetRef="StartEvent_NotrufEingegangen" />

    <bpmn:messageFlow id="MsgFlow_Bescheinigung"
                      name="Unfallbescheinigung"
                      sourceRef="Task_BescheinigungAusstellen"
                      targetRef="Task_BescheinigungEmpfangen" />
  </bpmn:collaboration>

  <bpmn:process id="Process_Buerger"
                name="Wildunfall_Meldung_Buerger"
                isExecutable="true">

    <bpmn:startEvent id="StartEvent_Buerger" name="Wildunfall ereignet">
      <bpmn:outgoing>Flow_Start_Notruf</bpmn:outgoing>
    </bpmn:startEvent>

    <bpmn:task id="Task_NotrufTaetigen" name="Notruf taetigen">
      <bpmn:incoming>Flow_Start_Notruf</bpmn:incoming>
      <bpmn:outgoing>Flow_Notruf_Bescheinigung</bpmn:outgoing>
    </bpmn:task>

    <bpmn:task id="Task_BescheinigungEmpfangen" name="Bescheinigung empfangen">
      <bpmn:incoming>Flow_Notruf_Bescheinigung</bpmn:incoming>
      <bpmn:outgoing>Flow_Bescheinigung_Ende</bpmn:outgoing>
    </bpmn:task>

    <bpmn:endEvent id="EndEvent_Buerger" name="Prozess abgeschlossen">
      <bpmn:incoming>Flow_Bescheinigung_Ende</bpmn:incoming>
    </bpmn:endEvent>

    <bpmn:sequenceFlow id="Flow_Start_Notruf"
                       sourceRef="StartEvent_Buerger"
                       targetRef="Task_NotrufTaetigen" />

    <bpmn:sequenceFlow id="Flow_Notruf_Bescheinigung"
                       sourceRef="Task_NotrufTaetigen"
                       targetRef="Task_BescheinigungEmpfangen" />

    <bpmn:sequenceFlow id="Flow_Bescheinigung_Ende"
                       sourceRef="Task_BescheinigungEmpfangen"
                       targetRef="EndEvent_Buerger" />
  </bpmn:process>

  <bpmn:process id="Process_Polizei"
                name="Wildunfall_Meldung_Polizei"
                isExecutable="true">

    <bpmn:startEvent id="StartEvent_NotrufEingegangen" name="Notruf eingegangen">
      <bpmn:outgoing>Flow_Start_Zustaendigkeit</bpmn:outgoing>
    </bpmn:startEvent>

    <bpmn:task id="Task_NotrufEntgegennehmen" name="Notruf entgegennehmen">
      <bpmn:incoming>Flow_Start_Zustaendigkeit</bpmn:incoming>
      <bpmn:outgoing>Flow_Notruf_Zustaendigkeit</bpmn:outgoing>
    </bpmn:task>

    <bpmn:task id="Task_ZustaendigkeitPruefen" name="Zustaendigkeit pruefen">
      <bpmn:incoming>Flow_Notruf_Zustaendigkeit</bpmn:incoming>
      <bpmn:outgoing>Flow_Zustaendigkeit_Dokumentation</bpmn:outgoing>
    </bpmn:task>

    <bpmn:task id="Task_EinsatzDokumentieren" name="Einsatz dokumentieren">
      <bpmn:incoming>Flow_Zustaendigkeit_Dokumentation</bpmn:incoming>
      <bpmn:outgoing>Flow_Dokumentation_Protokoll</bpmn:outgoing>
    </bpmn:task>

    <bpmn:task id="Task_UnfallprotokollErstellen" name="Unfallprotokoll erstellen">
      <bpmn:incoming>Flow_Dokumentation_Protokoll</bpmn:incoming>
      <bpmn:outgoing>Flow_Protokoll_Bescheinigung</bpmn:outgoing>
    </bpmn:task>

    <bpmn:task id="Task_BescheinigungAusstellen" name="Bescheinigung ausstellen">
      <bpmn:incoming>Flow_Protokoll_Bescheinigung</bpmn:incoming>
      <bpmn:outgoing>Flow_Bescheinigung_Ende_Polizei</bpmn:outgoing>
    </bpmn:task>

    <bpmn:endEvent id="EndEvent_Polizei" name="Prozess abgeschlossen">
      <bpmn:incoming>Flow_Bescheinigung_Ende_Polizei</bpmn:incoming>
    </bpmn:endEvent>

    <bpmn:sequenceFlow id="Flow_Start_Zustaendigkeit"
                       sourceRef="StartEvent_NotrufEingegangen"
                       targetRef="Task_NotrufEntgegennehmen" />

    <bpmn:sequenceFlow id="Flow_Notruf_Zustaendigkeit"
                       sourceRef="Task_NotrufEntgegennehmen"
                       targetRef="Task_ZustaendigkeitPruefen" />

    <bpmn:sequenceFlow id="Flow_Zustaendigkeit_Dokumentation"
                       sourceRef="Task_ZustaendigkeitPruefen"
                       targetRef="Task_EinsatzDokumentieren" />

    <bpmn:sequenceFlow id="Flow_Dokumentation_Protokoll"
                       sourceRef="Task_EinsatzDokumentieren"
                       targetRef="Task_UnfallprotokollErstellen" />

    <bpmn:sequenceFlow id="Flow_Protokoll_Bescheinigung"
                       sourceRef="Task_UnfallprotokollErstellen"
                       targetRef="Task_BescheinigungAusstellen" />

    <bpmn:sequenceFlow id="Flow_Bescheinigung_Ende_Polizei"
                       sourceRef="Task_BescheinigungAusstellen"
                       targetRef="EndEvent_Polizei" />
  </bpmn:process>

  <bpmndi:BPMNDiagram id="BPMNDiagram_1">
    <bpmndi:BPMNPlane id="BPMNPlane_1" bpmnElement="Collab_Wildunfall">
      <!-- Pool Buerger -->
      <bpmndi:BPMNShape id="Participant_Buerger_di" bpmnElement="Participant_Buerger" isHorizontal="true">
        <dc:Bounds x="160" y="80" width="900" height="250" />
      </bpmndi:BPMNShape>
      
      <!-- Pool Polizei -->
      <bpmndi:BPMNShape id="Participant_Polizei_di" bpmnElement="Participant_Polizei" isHorizontal="true">
        <dc:Bounds x="160" y="350" width="900" height="400" />
      </bpmndi:BPMNShape>

      <!-- Buerger Process Elements -->
      <bpmndi:BPMNShape id="StartEvent_Buerger_di" bpmnElement="StartEvent_Buerger">
        <dc:Bounds x="222" y="182" width="36" height="36" />
        <bpmndi:BPMNLabel>
          <dc:Bounds x="200" y="225" width="80" height="27" />
        </bpmndi:BPMNLabel>
      </bpmndi:BPMNShape>

      <bpmndi:BPMNShape id="Task_NotrufTaetigen_di" bpmnElement="Task_NotrufTaetigen">
        <dc:Bounds x="310" y="160" width="100" height="80" />
      </bpmndi:BPMNShape>

      <bpmndi:BPMNShape id="Task_BescheinigungEmpfangen_di" bpmnElement="Task_BescheinigungEmpfangen">
        <dc:Bounds x="760" y="160" width="100" height="80" />
      </bpmndi:BPMNShape>

      <bpmndi:BPMNShape id="EndEvent_Buerger_di" bpmnElement="EndEvent_Buerger">
        <dc:Bounds x="912" y="182" width="36" height="36" />
        <bpmndi:BPMNLabel>
          <dc:Bounds x="890" y="225" width="80" height="27" />
        </bpmndi:BPMNLabel>
      </bpmndi:BPMNShape>

      <!-- Polizei Process Elements -->
      <bpmndi:BPMNShape id="StartEvent_NotrufEingegangen_di" bpmnElement="StartEvent_NotrufEingegangen">
        <dc:Bounds x="222" y="452" width="36" height="36" />
        <bpmndi:BPMNLabel>
          <dc:Bounds x="200" y="495" width="80" height="27" />
        </bpmndi:BPMNLabel>
      </bpmndi:BPMNShape>

      <bpmndi:BPMNShape id="Task_NotrufEntgegennehmen_di" bpmnElement="Task_NotrufEntgegennehmen">
        <dc:Bounds x="310" y="430" width="100" height="80" />
      </bpmndi:BPMNShape>

      <bpmndi:BPMNShape id="Task_ZustaendigkeitPruefen_di" bpmnElement="Task_ZustaendigkeitPruefen">
        <dc:Bounds x="420" y="430" width="100" height="80" />
      </bpmndi:BPMNShape>

      <bpmndi:BPMNShape id="Task_EinsatzDokumentieren_di" bpmnElement="Task_EinsatzDokumentieren">
        <dc:Bounds x="530" y="430" width="100" height="80" />
      </bpmndi:BPMNShape>

      <bpmndi:BPMNShape id="Task_UnfallprotokollErstellen_di" bpmnElement="Task_UnfallprotokollErstellen">
        <dc:Bounds x="640" y="430" width="100" height="80" />
      </bpmndi:BPMNShape>

      <bpmndi:BPMNShape id="Task_BescheinigungAusstellen_di" bpmnElement="Task_BescheinigungAusstellen">
        <dc:Bounds x="750" y="430" width="100" height="80" />
      </bpmndi:BPMNShape>

      <bpmndi:BPMNShape id="EndEvent_Polizei_di" bpmnElement="EndEvent_Polizei">
        <dc:Bounds x="912" y="452" width="36" height="36" />
        <bpmndi:BPMNLabel>
          <dc:Bounds x="890" y="495" width="80" height="27" />
        </bpmndi:BPMNLabel>
      </bpmndi:BPMNShape>

      <!-- Sequence Flows - Buerger -->
      <bpmndi:BPMNEdge id="Flow_Start_Notruf_di" bpmnElement="Flow_Start_Notruf">
        <di:waypoint x="258" y="200" />
        <di:waypoint x="310" y="200" />
      </bpmndi:BPMNEdge>

      <bpmndi:BPMNEdge id="Flow_Notruf_Bescheinigung_di" bpmnElement="Flow_Notruf_Bescheinigung">
        <di:waypoint x="410" y="200" />
        <di:waypoint x="760" y="200" />
      </bpmndi:BPMNEdge>

      <bpmndi:BPMNEdge id="Flow_Bescheinigung_Ende_di" bpmnElement="Flow_Bescheinigung_Ende">
        <di:waypoint x="860" y="200" />
        <di:waypoint x="912" y="200" />
      </bpmndi:BPMNEdge>

      <!-- Sequence Flows - Polizei -->
      <bpmndi:BPMNEdge id="Flow_Start_Zustaendigkeit_di" bpmnElement="Flow_Start_Zustaendigkeit">
        <di:waypoint x="258" y="470" />
        <di:waypoint x="310" y="470" />
      </bpmndi:BPMNEdge>

      <bpmndi:BPMNEdge id="Flow_Notruf_Zustaendigkeit_di" bpmnElement="Flow_Notruf_Zustaendigkeit">
        <di:waypoint x="410" y="470" />
        <di:waypoint x="420" y="470" />
      </bpmndi:BPMNEdge>

      <bpmndi:BPMNEdge id="Flow_Zustaendigkeit_Dokumentation_di" bpmnElement="Flow_Zustaendigkeit_Dokumentation">
        <di:waypoint x="520" y="470" />
        <di:waypoint x="530" y="470" />
      </bpmndi:BPMNEdge>

      <bpmndi:BPMNEdge id="Flow_Dokumentation_Protokoll_di" bpmnElement="Flow_Dokumentation_Protokoll">
        <di:waypoint x="630" y="470" />
        <di:waypoint x="640" y="470" />
      </bpmndi:BPMNEdge>

      <bpmndi:BPMNEdge id="Flow_Protokoll_Bescheinigung_di" bpmnElement="Flow_Protokoll_Bescheinigung">
        <di:waypoint x="740" y="470" />
        <di:waypoint x="750" y="470" />
      </bpmndi:BPMNEdge>

      <bpmndi:BPMNEdge id="Flow_Bescheinigung_Ende_Polizei_di" bpmnElement="Flow_Bescheinigung_Ende_Polizei">
        <di:waypoint x="850" y="470" />
        <di:waypoint x="912" y="470" />
      </bpmndi:BPMNEdge>

      <!-- Message Flows -->
      <bpmndi:BPMNEdge id="MsgFlow_Notruf_di" bpmnElement="MsgFlow_Notruf">
        <di:waypoint x="360" y="240" />
        <di:waypoint x="360" y="346" />
        <di:waypoint x="240" y="346" />
        <di:waypoint x="240" y="452" />
      </bpmndi:BPMNEdge>

      <bpmndi:BPMNEdge id="MsgFlow_Bescheinigung_di" bpmnElement="MsgFlow_Bescheinigung">
        <di:waypoint x="800" y="430" />
        <di:waypoint x="800" y="335" />
        <di:waypoint x="810" y="335" />
        <di:waypoint x="810" y="240" />
      </bpmndi:BPMNEdge>
    </bpmndi:BPMNPlane>
  </bpmndi:BPMNDiagram>

</bpmn:definitions>
