      </bpmndi:BPMNEdge>
      
      <bpmndi:BPMNEdge id="MsgFlow_Abholung_di" bpmnElement="MsgFlow_Abholung">
        <di:waypoint x="520" y="580" />
        <di:waypoint x="520" y="389" />
        <di:waypoint x="498" y="389" />
        <di:waypoint x="498" y="218" />
        <bpmndi:BPMNLabel>
          <dc:Bounds x="480" y="369" width="80" height="14" />
        </bpmndi:BPMNLabel>
      </bpmndi:BPMNEdge>
      
    </bpmndi:BPMNPlane>
  </bpmndi:BPMNDiagram>

</bpmn:definitions>
