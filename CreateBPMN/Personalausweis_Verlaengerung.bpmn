<?xml version="1.0" encoding="UTF-8"?>
<bpmn:definitions
  xmlns:bpmn="http://www.omg.org/spec/BPMN/20100524/MODEL"
  xmlns:bpmndi="http://www.omg.org/spec/BPMN/20100524/DI"
  xmlns:dc="http://www.omg.org/spec/DD/20100524/DC"
  xmlns:di="http://www.omg.org/spec/DD/20100524/DI"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xmlns:camunda="http://camunda.org/schema/1.0/bpmn"
  targetNamespace="http://example.com/bpmn"
  exporter="GitHub-Copilot-GovModeler"
  exporterVersion="2.0"
  modeler:executionPlatform="Camunda Platform"
  modeler:executionPlatformVersion="7.20.0">

  <!-- =====================================================
       COLLABORATION - Haupt-Container für Prozess-Interaktion
       ===================================================== -->
  <bpmn:collaboration id="Collab_Personalausweis_Verlaengerung">
    
    <!-- Participant: Bürger/Antragsteller -->
    <bpmn:participant id="Participant_Buerger"
                      name="Buerger"
                      processRef="Process_Buerger" />
    
    <!-- Participant: Behörde/Amt -->
    <bpmn:participant id="Participant_Buergeramt"
                      name="Buergeramt"
                      processRef="Process_Buergeramt" />
    
    <!-- Message Flows zwischen Participanten -->
    <bpmn:messageFlow id="MsgFlow_Antrag"
                      name="Verlaengerungsantrag"
                      sourceRef="Task_Antrag_stellen"
                      targetRef="StartEvent_Antrag_eingegangen" />
    
    <bpmn:messageFlow id="MsgFlow_Abholung"
                      name="Abholinformation"
                      sourceRef="Task_Abholinformation_versenden"
                      targetRef="Event_Abholung_erhalten" />
  </bpmn:collaboration>

  <!-- =====================================================
       BÜRGER PROZESS - Standard Antragsteller-Prozess
       ===================================================== -->
  <bpmn:process id="Process_Buerger"
                name="Personalausweis_Verlaengerung_Buerger"
                isExecutable="true">
    
    <!-- Start Event -->
    <bpmn:startEvent id="StartEvent_Buerger" name="Personalausweis-Verlaengerung notwendig">
      <bpmn:outgoing>Flow_Start_Antrag</bpmn:outgoing>
    </bpmn:startEvent>
    
    <!-- User Task: Antrag stellen -->
    <bpmn:userTask id="Task_Antrag_stellen" name="Verlaengerungsantrag stellen">
      <bpmn:incoming>Flow_Start_Antrag</bpmn:incoming>
      <bpmn:outgoing>Flow_Antrag_Warten</bpmn:outgoing>
    </bpmn:userTask>
    
    <!-- Intermediate Catch Event: Abholung -->
    <bpmn:intermediateCatchEvent id="Event_Abholung_erhalten" name="Abholinformation erhalten">
      <bpmn:incoming>Flow_Antrag_Warten</bpmn:incoming>
      <bpmn:outgoing>Flow_Abholung_Ende</bpmn:outgoing>
      <bpmn:messageEventDefinition id="MessageEventDef_Abholung" messageRef="Message_Abholung" />
    </bpmn:intermediateCatchEvent>
    
    <!-- End Event -->
    <bpmn:endEvent id="EndEvent_Buerger" name="Prozess abgeschlossen">
      <bpmn:incoming>Flow_Abholung_Ende</bpmn:incoming>
    </bpmn:endEvent>
    
    <!-- Sequence Flows - Bürger -->
    <bpmn:sequenceFlow id="Flow_Start_Antrag"
                       sourceRef="StartEvent_Buerger"
                       targetRef="Task_Antrag_stellen" />
    
    <bpmn:sequenceFlow id="Flow_Antrag_Warten"
                       sourceRef="Task_Antrag_stellen"
                       targetRef="Event_Abholung_erhalten" />
    
    <bpmn:sequenceFlow id="Flow_Abholung_Ende"
                       sourceRef="Event_Abholung_erhalten"
                       targetRef="EndEvent_Buerger" />
  </bpmn:process>

  <!-- =====================================================
       BEHÖRDEN PROZESS - Standard Bearbeitungs-Prozess
       ===================================================== -->
  <bpmn:process id="Process_Buergeramt"
                name="Personalausweis_Verlaengerung_Buergeramt"
                isExecutable="true">
    
    <!-- Start Event -->
    <bpmn:startEvent id="StartEvent_Antrag_eingegangen" name="Verlaengerungsantrag eingegangen">
      <bpmn:outgoing>Flow_Start_Zustaendigkeit</bpmn:outgoing>
    </bpmn:startEvent>
    
    <!-- Service Task: Automatische Prüfungen -->
    <bpmn:serviceTask id="Task_Zustaendigkeit_pruefen" name="Zustaendigkeit pruefen">
      <bpmn:incoming>Flow_Start_Zustaendigkeit</bpmn:incoming>
      <bpmn:outgoing>Flow_Zustaendigkeit_Vollstaendigkeit</bpmn:outgoing>
    </bpmn:serviceTask>
    
    <bpmn:serviceTask id="Task_Vollstaendigkeit_pruefen" name="Vollstaendigkeit pruefen">
      <bpmn:incoming>Flow_Zustaendigkeit_Vollstaendigkeit</bpmn:incoming>
      <bpmn:outgoing>Flow_Vollstaendigkeit_Biometrie</bpmn:outgoing>
    </bpmn:serviceTask>
    
    <!-- User Tasks: Sachbearbeitung -->
    <bpmn:userTask id="Task_Biometrie_erfassen" name="Biometrische Daten erfassen">
      <bpmn:incoming>Flow_Vollstaendigkeit_Biometrie</bpmn:incoming>
      <bpmn:outgoing>Flow_Biometrie_Erfassung</bpmn:outgoing>
    </bpmn:userTask>
    
    <bpmn:userTask id="Task_Antrag_erfassen" name="Antrag digital erfassen">
      <bpmn:incoming>Flow_Biometrie_Erfassung</bpmn:incoming>
      <bpmn:outgoing>Flow_Erfassung_Bestellung</bpmn:outgoing>
    </bpmn:userTask>
    
    <!-- Service Task: Bestellung -->
    <bpmn:serviceTask id="Task_Ausweis_bestellen" name="Personalausweis bestellen">
      <bpmn:incoming>Flow_Erfassung_Bestellung</bpmn:incoming>
      <bpmn:outgoing>Flow_Bestellung_Abholung</bpmn:outgoing>
    </bpmn:serviceTask>
    
    <!-- Task: Benachrichtigung -->
    <bpmn:userTask id="Task_Abholinformation_versenden" name="Abholinformation versenden">
      <bpmn:incoming>Flow_Bestellung_Abholung</bpmn:incoming>
      <bpmn:outgoing>Flow_Abholung_Ende</bpmn:outgoing>
    </bpmn:userTask>
    
    <!-- End Event -->
    <bpmn:endEvent id="EndEvent_Buergeramt" name="Prozess abgeschlossen">
      <bpmn:incoming>Flow_Abholung_Ende</bpmn:incoming>
    </bpmn:endEvent>
    
    <!-- Sequence Flows - Behörde -->
    <bpmn:sequenceFlow id="Flow_Start_Zustaendigkeit"
                       sourceRef="StartEvent_Antrag_eingegangen"
                       targetRef="Task_Zustaendigkeit_pruefen" />
    
    <bpmn:sequenceFlow id="Flow_Zustaendigkeit_Vollstaendigkeit"
                       sourceRef="Task_Zustaendigkeit_pruefen"
                       targetRef="Task_Vollstaendigkeit_pruefen" />
    
    <bpmn:sequenceFlow id="Flow_Vollstaendigkeit_Biometrie"
                       sourceRef="Task_Vollstaendigkeit_pruefen"
                       targetRef="Task_Biometrie_erfassen" />
    
    <bpmn:sequenceFlow id="Flow_Biometrie_Erfassung"
                       sourceRef="Task_Biometrie_erfassen"
                       targetRef="Task_Antrag_erfassen" />
    
    <bpmn:sequenceFlow id="Flow_Erfassung_Bestellung"
                       sourceRef="Task_Antrag_erfassen"
                       targetRef="Task_Ausweis_bestellen" />
    
    <bpmn:sequenceFlow id="Flow_Bestellung_Abholung"
                       sourceRef="Task_Ausweis_bestellen"
                       targetRef="Task_Abholinformation_versenden" />
    
    <bpmn:sequenceFlow id="Flow_Abholung_Ende"
                       sourceRef="Task_Abholinformation_versenden"
                       targetRef="EndEvent_Buergeramt" />
  </bpmn:process>

  <!-- =====================================================
       MESSAGE DEFINITIONEN
       ===================================================== -->
  <bpmn:message id="Message_Abholung" name="Abholinformation" />

  <!-- =====================================================
       BPMN-DI VISUALISIERUNG - Basiert auf Guidelines
       ===================================================== -->
  <bpmndi:BPMNDiagram id="BPMNDiagram_Personalausweis_Verlaengerung">
    <bpmndi:BPMNPlane id="BPMNPlane_Personalausweis_Verlaengerung" bpmnElement="Collab_Personalausweis_Verlaengerung">
      
      <!-- ===============================
           POOL: BÜRGER
           =============================== -->
      <bpmndi:BPMNShape id="Participant_Buerger_di" bpmnElement="Participant_Buerger" isHorizontal="true">
