      <bpmndi:BPMNShape id="Event_Abholung_erhalten_di" bpmnElement="Event_Abholung_erhalten">
        <dc:Bounds x="480" y="182" width="36" height="36" />
        <bpmndi:BPMNLabel>
          <dc:Bounds x="460" y="225" width="76" height="27" />
        </bpmndi:BPMNLabel>
      </bpmndi:BPMNShape>
      
      <bpmndi:BPMNShape id="EndEvent_Buerger_di" bpmnElement="EndEvent_Buerger">
        <dc:Bounds x="580" y="182" width="36" height="36" />
        <bpmndi:BPMNLabel>
          <dc:Bounds x="558" y="225" width="80" height="27" />
        </bpmndi:BPMNLabel>
      </bpmndi:BPMNShape>
      
      <!-- ===============================
           POOL: BÜRGERAMT
           =============================== -->
      <bpmndi:BPMNShape id="Participant_Buergeramt_di" bpmnElement="Participant_Buergeramt" isHorizontal="true">
        <dc:Bounds x="160" y="380" width="900" height="300" />
        <bpmndi:BPMNLabel>
          <dc:Bounds x="170" y="515" width="20" height="70" />
        </bpmndi:BPMNLabel>
      </bpmndi:BPMNShape>
      
      <!-- Bürgeramt Process Elements -->
      <bpmndi:BPMNShape id="StartEvent_Antrag_eingegangen_di" bpmnElement="StartEvent_Antrag_eingegangen">
        <dc:Bounds x="222" y="492" width="36" height="36" />
        <bpmndi:BPMNLabel>
          <dc:Bounds x="200" y="535" width="80" height="40" />
        </bpmndi:BPMNLabel>
      </bpmndi:BPMNShape>
      
      <bpmndi:BPMNShape id="Task_Zustaendigkeit_pruefen_di" bpmnElement="Task_Zustaendigkeit_pruefen">
        <dc:Bounds x="320" y="470" width="100" height="80" />
      </bpmndi:BPMNShape>
      
      <bpmndi:BPMNShape id="Task_Vollstaendigkeit_pruefen_di" bpmnElement="Task_Vollstaendigkeit_pruefen">
        <dc:Bounds x="470" y="470" width="100" height="80" />
      </bpmndi:BPMNShape>
      
      <bpmndi:BPMNShape id="Task_Biometrie_erfassen_di" bpmnElement="Task_Biometrie_erfassen">
        <dc:Bounds x="620" y="470" width="100" height="80" />
      </bpmndi:BPMNShape>
      
      <bpmndi:BPMNShape id="Task_Antrag_erfassen_di" bpmnElement="Task_Antrag_erfassen">
        <dc:Bounds x="770" y="470" width="100" height="80" />
      </bpmndi:BPMNShape>
      
      <bpmndi:BPMNShape id="Task_Ausweis_bestellen_di" bpmnElement="Task_Ausweis_bestellen">
        <dc:Bounds x="320" y="580" width="100" height="80" />
      </bpmndi:BPMNShape>
      
      <bpmndi:BPMNShape id="Task_Abholinformation_versenden_di" bpmnElement="Task_Abholinformation_versenden">
        <dc:Bounds x="470" y="580" width="100" height="80" />
      </bpmndi:BPMNShape>
      
      <bpmndi:BPMNShape id="EndEvent_Buergeramt_di" bpmnElement="EndEvent_Buergeramt">
        <dc:Bounds x="622" y="602" width="36" height="36" />
        <bpmndi:BPMNLabel>
          <dc:Bounds x="600" y="645" width="80" height="27" />
        </bpmndi:BPMNLabel>
      </bpmndi:BPMNShape>
      
      <!-- ===============================
           SEQUENCE FLOWS - BÜRGER
           =============================== -->
      <bpmndi:BPMNEdge id="Flow_Start_Antrag_di" bpmnElement="Flow_Start_Antrag">
        <di:waypoint x="258" y="200" />
        <di:waypoint x="320" y="200" />
      </bpmndi:BPMNEdge>
      
      <bpmndi:BPMNEdge id="Flow_Antrag_Warten_di" bpmnElement="Flow_Antrag_Warten">
        <di:waypoint x="420" y="200" />
        <di:waypoint x="480" y="200" />
      </bpmndi:BPMNEdge>
      
      <bpmndi:BPMNEdge id="Flow_Abholung_Ende_di" bpmnElement="Flow_Abholung_Ende">
        <di:waypoint x="516" y="200" />
        <di:waypoint x="580" y="200" />
      </bpmndi:BPMNEdge>
      
      <!-- ===============================
           SEQUENCE FLOWS - BÜRGERAMT
           =============================== -->
      <bpmndi:BPMNEdge id="Flow_Start_Zustaendigkeit_di" bpmnElement="Flow_Start_Zustaendigkeit">
        <di:waypoint x="258" y="510" />
        <di:waypoint x="320" y="510" />
      </bpmndi:BPMNEdge>
      
      <bpmndi:BPMNEdge id="Flow_Zustaendigkeit_Vollstaendigkeit_di" bpmnElement="Flow_Zustaendigkeit_Vollstaendigkeit">
        <di:waypoint x="420" y="510" />
        <di:waypoint x="470" y="510" />
      </bpmndi:BPMNEdge>
      
      <bpmndi:BPMNEdge id="Flow_Vollstaendigkeit_Biometrie_di" bpmnElement="Flow_Vollstaendigkeit_Biometrie">
        <di:waypoint x="570" y="510" />
        <di:waypoint x="620" y="510" />
      </bpmndi:BPMNEdge>
      
      <bpmndi:BPMNEdge id="Flow_Biometrie_Erfassung_di" bpmnElement="Flow_Biometrie_Erfassung">
        <di:waypoint x="720" y="510" />
        <di:waypoint x="770" y="510" />
      </bpmndi:BPMNEdge>
      
      <bpmndi:BPMNEdge id="Flow_Erfassung_Bestellung_di" bpmnElement="Flow_Erfassung_Bestellung">
        <di:waypoint x="820" y="550" />
        <di:waypoint x="820" y="620" />
        <di:waypoint x="420" y="620" />
      </bpmndi:BPMNEdge>
      
      <bpmndi:BPMNEdge id="Flow_Bestellung_Abholung_di" bpmnElement="Flow_Bestellung_Abholung">
        <di:waypoint x="420" y="620" />
