<?xml version="1.0" encoding="UTF-8"?>
<bpmn:definitions
  xmlns:bpmn="http://www.omg.org/spec/BPMN/20100524/MODEL"
  xmlns:bpmndi="http://www.omg.org/spec/BPMN/20100524/DI"
  xmlns:dc="http://www.omg.org/spec/DD/20100524/DC"
  xmlns:di="http://www.omg.org/spec/DD/20100524/DI"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  targetNamespace="http://example.com/bpmn"
  exporter="ChatGPT-Template"
  exporterVersion="1.0" xmlns:camunda="http://camunda.org/schema/1.0/bpmn" modeler:executionPlatform="Camunda Platform" modeler:executionPlatformVersion="7.20.0">

  <bpmn:collaboration id="Collab_Personalausweis">
    <bpmn:participant id="Participant_Buerger"
                      name="Buerger"
                      processRef="Process_Buerger" />

    <bpmn:participant id="Participant_Buergeramt"
                      name="Buergeramt"
                      processRef="Process_Buergeramt" />

    <bpmn:messageFlow id="MsgFlow_Antrag"
                      name="Verlaengerungsantrag"
                      sourceRef="Task_AntragStellen"
                      targetRef="StartEvent_AntragEingegangen" />

    <bpmn:messageFlow id="MsgFlow_Abholung"
                      name="Abholinformation"
                      sourceRef="Task_AbholinfoSenden"
                      targetRef="Task_InfoEmpfangen" />
  </bpmn:collaboration>

  <bpmn:process id="Process_Buerger"
                name="Personalausweis_Verlaengerung_Buerger"
                isExecutable="true">

    <bpmn:startEvent id="StartEvent_Buerger" name="Verlaengerung notwendig">
      <bpmn:outgoing>Flow_Start_Antrag</bpmn:outgoing>
    </bpmn:startEvent>

    <bpmn:task id="Task_AntragStellen" name="Antrag stellen">
      <bpmn:incoming>Flow_Start_Antrag</bpmn:incoming>
      <bpmn:outgoing>Flow_Antrag_Info</bpmn:outgoing>
    </bpmn:task>

    <bpmn:task id="Task_InfoEmpfangen" name="Abholinformation empfangen">
      <bpmn:incoming>Flow_Antrag_Info</bpmn:incoming>
      <bpmn:outgoing>Flow_Info_Ende</bpmn:outgoing>
    </bpmn:task>

    <bpmn:endEvent id="EndEvent_Buerger" name="Prozess abgeschlossen">
      <bpmn:incoming>Flow_Info_Ende</bpmn:incoming>
    </bpmn:endEvent>

    <bpmn:sequenceFlow id="Flow_Start_Antrag"
                       sourceRef="StartEvent_Buerger"
                       targetRef="Task_AntragStellen" />

    <bpmn:sequenceFlow id="Flow_Antrag_Info"
                       sourceRef="Task_AntragStellen"
                       targetRef="Task_InfoEmpfangen" />

    <bpmn:sequenceFlow id="Flow_Info_Ende"
                       sourceRef="Task_InfoEmpfangen"
                       targetRef="EndEvent_Buerger" />
  </bpmn:process>

  <bpmn:process id="Process_Buergeramt"
                name="Personalausweis_Verlaengerung_Buergeramt"
                isExecutable="true">

    <bpmn:startEvent id="StartEvent_AntragEingegangen" name="Verlaengerungsantrag eingegangen">
      <bpmn:outgoing>Flow_Start_Zustaendigkeit</bpmn:outgoing>
    </bpmn:startEvent>

    <bpmn:task id="Task_ZustaendigkeitPruefen" name="Zustaendigkeit pruefen">
      <bpmn:incoming>Flow_Start_Zustaendigkeit</bpmn:incoming>
      <bpmn:outgoing>Flow_Zustaendigkeit_Vollstaendigkeit</bpmn:outgoing>
    </bpmn:task>

    <bpmn:task id="Task_VollstaendigkeitPruefen" name="Vollstaendigkeit der Unterlagen pruefen">
      <bpmn:incoming>Flow_Zustaendigkeit_Vollstaendigkeit</bpmn:incoming>
      <bpmn:outgoing>Flow_Vollstaendigkeit_Biometrie</bpmn:outgoing>
    </bpmn:task>

    <bpmn:task id="Task_BiometrieErfassen" name="Biometrische Daten erfassen">
      <bpmn:incoming>Flow_Vollstaendigkeit_Biometrie</bpmn:incoming>
      <bpmn:outgoing>Flow_Biometrie_Antrag</bpmn:outgoing>
    </bpmn:task>

    <bpmn:task id="Task_AntragErfassen" name="Antrag digital erfassen">
      <bpmn:incoming>Flow_Biometrie_Antrag</bpmn:incoming>
      <bpmn:outgoing>Flow_Antrag_Bestellen</bpmn:outgoing>
    </bpmn:task>

    <bpmn:task id="Task_AusweisBestellung" name="Personalausweis bestellen">
      <bpmn:incoming>Flow_Antrag_Bestellen</bpmn:incoming>
      <bpmn:outgoing>Flow_Bestellen_Abholen</bpmn:outgoing>
    </bpmn:task>

    <bpmn:task id="Task_AbholinfoSenden" name="Abholinformation versenden">
      <bpmn:incoming>Flow_Bestellen_Abholen</bpmn:incoming>
      <bpmn:outgoing>Flow_Abholen_Ende</bpmn:outgoing>
    </bpmn:task>

    <bpmn:endEvent id="EndEvent_Buergeramt" name="Prozess abgeschlossen">
      <bpmn:incoming>Flow_Abholen_Ende</bpmn:incoming>
    </bpmn:endEvent>

    <bpmn:sequenceFlow id="Flow_Start_Zustaendigkeit"
                       sourceRef="StartEvent_AntragEingegangen"
                       targetRef="Task_ZustaendigkeitPruefen" />

    <bpmn:sequenceFlow id="Flow_Zustaendigkeit_Vollstaendigkeit"
                       sourceRef="Task_ZustaendigkeitPruefen"
                       targetRef="Task_VollstaendigkeitPruefen" />

    <bpmn:sequenceFlow id="Flow_Vollstaendigkeit_Biometrie"
                       sourceRef="Task_VollstaendigkeitPruefen"
                       targetRef="Task_BiometrieErfassen" />

    <bpmn:sequenceFlow id="Flow_Biometrie_Antrag"
                       sourceRef="Task_BiometrieErfassen"
                       targetRef="Task_AntragErfassen" />

    <bpmn:sequenceFlow id="Flow_Antrag_Bestellen"
                       sourceRef="Task_AntragErfassen"
                       targetRef="Task_AusweisBestellung" />

    <bpmn:sequenceFlow id="Flow_Bestellen_Abholen"
                       sourceRef="Task_AusweisBestellung"
                       targetRef="Task_AbholinfoSenden" />

    <bpmn:sequenceFlow id="Flow_Abholen_Ende"
                       sourceRef="Task_AbholinfoSenden"
                       targetRef="EndEvent_Buergeramt" />
  </bpmn:process>

  <bpmndi:BPMNDiagram id="BPMNDiagram_1">
    <bpmndi:BPMNPlane id="BPMNPlane_1" bpmnElement="Collab_Personalausweis">
      <!-- Pool Buerger -->
      <bpmndi:BPMNShape id="Participant_Buerger_di" bpmnElement="Participant_Buerger" isHorizontal="true">
        <dc:Bounds x="160" y="80" width="900" height="250" />
      </bpmndi:BPMNShape>
      
      <!-- Pool Buergeramt -->
      <bpmndi:BPMNShape id="Participant_Buergeramt_di" bpmnElement="Participant_Buergeramt" isHorizontal="true">
        <dc:Bounds x="160" y="350" width="900" height="400" />
      </bpmndi:BPMNShape>

      <!-- Buerger Process Elements -->
      <bpmndi:BPMNShape id="StartEvent_Buerger_di" bpmnElement="StartEvent_Buerger">
        <dc:Bounds x="222" y="182" width="36" height="36" />
        <bpmndi:BPMNLabel>
          <dc:Bounds x="200" y="225" width="80" height="27" />
        </bpmndi:BPMNLabel>
      </bpmndi:BPMNShape>

      <bpmndi:BPMNShape id="Task_AntragStellen_di" bpmnElement="Task_AntragStellen">
        <dc:Bounds x="310" y="160" width="100" height="80" />
      </bpmndi:BPMNShape>

      <bpmndi:BPMNShape id="Task_InfoEmpfangen_di" bpmnElement="Task_InfoEmpfangen">
        <dc:Bounds x="760" y="160" width="100" height="80" />
      </bpmndi:BPMNShape>

      <bpmndi:BPMNShape id="EndEvent_Buerger_di" bpmnElement="EndEvent_Buerger">
        <dc:Bounds x="912" y="182" width="36" height="36" />
        <bpmndi:BPMNLabel>
          <dc:Bounds x="890" y="225" width="80" height="27" />
        </bpmndi:BPMNLabel>
      </bpmndi:BPMNShape>

      <!-- Buergeramt Process Elements -->
      <bpmndi:BPMNShape id="StartEvent_AntragEingegangen_di" bpmnElement="StartEvent_AntragEingegangen">
        <dc:Bounds x="222" y="452" width="36" height="36" />
        <bpmndi:BPMNLabel>
          <dc:Bounds x="200" y="495" width="80" height="27" />
        </bpmndi:BPMNLabel>
      </bpmndi:BPMNShape>

      <bpmndi:BPMNShape id="Task_ZustaendigkeitPruefen_di" bpmnElement="Task_ZustaendigkeitPruefen">
        <dc:Bounds x="310" y="430" width="100" height="80" />
      </bpmndi:BPMNShape>

      <bpmndi:BPMNShape id="Task_VollstaendigkeitPruefen_di" bpmnElement="Task_VollstaendigkeitPruefen">
        <dc:Bounds x="420" y="430" width="100" height="80" />
      </bpmndi:BPMNShape>

      <bpmndi:BPMNShape id="Task_BiometrieErfassen_di" bpmnElement="Task_BiometrieErfassen">
        <dc:Bounds x="530" y="430" width="100" height="80" />
      </bpmndi:BPMNShape>

      <bpmndi:BPMNShape id="Task_AntragErfassen_di" bpmnElement="Task_AntragErfassen">
        <dc:Bounds x="640" y="430" width="100" height="80" />
      </bpmndi:BPMNShape>

      <bpmndi:BPMNShape id="Task_AusweisBestellung_di" bpmnElement="Task_AusweisBestellung">
        <dc:Bounds x="750" y="430" width="100" height="80" />
      </bpmndi:BPMNShape>

      <bpmndi:BPMNShape id="Task_AbholinfoSenden_di" bpmnElement="Task_AbholinfoSenden">
        <dc:Bounds x="860" y="430" width="100" height="80" />
      </bpmndi:BPMNShape>

      <bpmndi:BPMNShape id="EndEvent_Buergeramt_di" bpmnElement="EndEvent_Buergeramt">
        <dc:Bounds x="972" y="452" width="36" height="36" />
        <bpmndi:BPMNLabel>
          <dc:Bounds x="950" y="495" width="80" height="27" />
        </bpmndi:BPMNLabel>
      </bpmndi:BPMNShape>

      <!-- Sequence Flows - Buerger -->
      <bpmndi:BPMNEdge id="Flow_Start_Antrag_di" bpmnElement="Flow_Start_Antrag">
        <di:waypoint x="258" y="200" />
        <di:waypoint x="310" y="200" />
      </bpmndi:BPMNEdge>

      <bpmndi:BPMNEdge id="Flow_Antrag_Info_di" bpmnElement="Flow_Antrag_Info">
        <di:waypoint x="410" y="200" />
        <di:waypoint x="760" y="200" />
      </bpmndi:BPMNEdge>

      <bpmndi:BPMNEdge id="Flow_Info_Ende_di" bpmnElement="Flow_Info_Ende">
        <di:waypoint x="860" y="200" />
        <di:waypoint x="912" y="200" />
      </bpmndi:BPMNEdge>

      <!-- Sequence Flows - Buergeramt -->
      <bpmndi:BPMNEdge id="Flow_Start_Zustaendigkeit_di" bpmnElement="Flow_Start_Zustaendigkeit">
        <di:waypoint x="258" y="470" />
        <di:waypoint x="310" y="470" />
      </bpmndi:BPMNEdge>

      <bpmndi:BPMNEdge id="Flow_Zustaendigkeit_Vollstaendigkeit_di" bpmnElement="Flow_Zustaendigkeit_Vollstaendigkeit">
        <di:waypoint x="410" y="470" />
        <di:waypoint x="420" y="470" />
      </bpmndi:BPMNEdge>

      <bpmndi:BPMNEdge id="Flow_Vollstaendigkeit_Biometrie_di" bpmnElement="Flow_Vollstaendigkeit_Biometrie">
        <di:waypoint x="520" y="470" />
        <di:waypoint x="530" y="470" />
      </bpmndi:BPMNEdge>

      <bpmndi:BPMNEdge id="Flow_Biometrie_Antrag_di" bpmnElement="Flow_Biometrie_Antrag">
        <di:waypoint x="630" y="470" />
        <di:waypoint x="640" y="470" />
      </bpmndi:BPMNEdge>

      <bpmndi:BPMNEdge id="Flow_Antrag_Bestellen_di" bpmnElement="Flow_Antrag_Bestellen">
        <di:waypoint x="740" y="470" />
        <di:waypoint x="750" y="470" />
      </bpmndi:BPMNEdge>

      <bpmndi:BPMNEdge id="Flow_Bestellen_Abholen_di" bpmnElement="Flow_Bestellen_Abholen">
        <di:waypoint x="850" y="470" />
        <di:waypoint x="860" y="470" />
      </bpmndi:BPMNEdge>

      <bpmndi:BPMNEdge id="Flow_Abholen_Ende_di" bpmnElement="Flow_Abholen_Ende">
        <di:waypoint x="960" y="470" />
        <di:waypoint x="972" y="470" />
      </bpmndi:BPMNEdge>

      <!-- Message Flows -->
      <bpmndi:BPMNEdge id="MsgFlow_Antrag_di" bpmnElement="MsgFlow_Antrag">
        <di:waypoint x="360" y="240" />
        <di:waypoint x="360" y="346" />
        <di:waypoint x="240" y="346" />
        <di:waypoint x="240" y="452" />
      </bpmndi:BPMNEdge>

      <bpmndi:BPMNEdge id="MsgFlow_Abholung_di" bpmnElement="MsgFlow_Abholung">
        <di:waypoint x="910" y="430" />
        <di:waypoint x="910" y="335" />
        <di:waypoint x="810" y="335" />
        <di:waypoint x="810" y="240" />
      </bpmndi:BPMNEdge>
    </bpmndi:BPMNPlane>
  </bpmndi:BPMNDiagram>

</bpmn:definitions>
