<?xml version="1.0" encoding="UTF-8"?>
<bpmn:definitions
  xmlns:bpmn="http://www.omg.org/spec/BPMN/20100524/MODEL"
  xmlns:bpmndi="http://www.omg.org/spec/BPMN/20100524/DI"
  xmlns:dc="http://www.omg.org/spec/DD/20100524/DC"
  xmlns:di="http://www.omg.org/spec/DD/20100524/DI"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  targetNamespace="http://example.com/bpmn"
  exporter="ChatGPT-Template"
  exporterVersion="1.0" xmlns:camunda="http://camunda.org/schema/1.0/bpmn" modeler:executionPlatform="Camunda Platform" modeler:executionPlatformVersion="7.20.0">

  <bpmn:collaboration id="Collab_Angelschein">
    <bpmn:participant id="Participant_Kunde"
                      name="Kunde"
                      processRef="Process_Kunde" />

    <bpmn:participant id="Participant_Organisation"
                      name="Organisation"
                      processRef="Process_Organisation" />

    <bpmn:messageFlow id="MsgFlow_Antrag"
                      name="Antrag"
                      sourceRef="Task_Antrag_stellen"
                      targetRef="StartEvent_Org" />

    <bpmn:messageFlow id="MsgFlow_Angelschein"
                      name="Angelschein"
                      sourceRef="Task_Angelschein_ausstellen"
                      targetRef="Task_Angelschein_empfangen" />
  </bpmn:collaboration>

  <bpmn:process id="Process_Kunde"
                name="Angelschein beantragen - Kunde"
                isExecutable="true">

    <bpmn:startEvent id="StartEvent_Kunde" name="Antragsbedarf festgestellt">
      <bpmn:outgoing>Flow_Start_Antrag</bpmn:outgoing>
    </bpmn:startEvent>

    <bpmn:task id="Task_Antrag_stellen" name="Antrag stellen">
      <bpmn:incoming>Flow_Start_Antrag</bpmn:incoming>
      <bpmn:outgoing>Flow_Antrag_Empfangen</bpmn:outgoing>
    </bpmn:task>

    <bpmn:task id="Task_Angelschein_empfangen" name="Angelschein empfangen">
      <bpmn:incoming>Flow_Antrag_Empfangen</bpmn:incoming>
      <bpmn:outgoing>Flow_Empfangen_Ende</bpmn:outgoing>
    </bpmn:task>

    <bpmn:endEvent id="EndEvent_Kunde" name="Angelschein erhalten">
      <bpmn:incoming>Flow_Empfangen_Ende</bpmn:incoming>
    </bpmn:endEvent>

    <bpmn:sequenceFlow id="Flow_Start_Antrag"
                       sourceRef="StartEvent_Kunde"
                       targetRef="Task_Antrag_stellen" />

    <bpmn:sequenceFlow id="Flow_Antrag_Empfangen"
                       sourceRef="Task_Antrag_stellen"
                       targetRef="Task_Angelschein_empfangen" />

    <bpmn:sequenceFlow id="Flow_Empfangen_Ende"
                       sourceRef="Task_Angelschein_empfangen"
                       targetRef="EndEvent_Kunde" />
  </bpmn:process>

  <bpmn:process id="Process_Organisation"
                name="Angelschein beantragen - Organisation"
                isExecutable="true">

    <bpmn:startEvent id="StartEvent_Org" name="Antrag eingegangen">
      <bpmn:outgoing>Flow_Start_Zustaendigkeit</bpmn:outgoing>
    </bpmn:startEvent>

    <bpmn:task id="Task_Zustaendigkeit" name="Zustaendigkeit pruefen">
      <bpmn:incoming>Flow_Start_Zustaendigkeit</bpmn:incoming>
      <bpmn:outgoing>Flow_Zustaendigkeit_Vollstaendigkeit</bpmn:outgoing>
    </bpmn:task>

    <bpmn:task id="Task_Vollstaendigkeit" name="Vollstaendigkeit pruefen">
      <bpmn:incoming>Flow_Zustaendigkeit_Vollstaendigkeit</bpmn:incoming>
      <bpmn:outgoing>Flow_Vollstaendigkeit_Bearbeiten</bpmn:outgoing>
    </bpmn:task>

    <bpmn:task id="Task_Antrag_bearbeiten" name="Antrag bearbeiten">
      <bpmn:incoming>Flow_Vollstaendigkeit_Bearbeiten</bpmn:incoming>
      <bpmn:outgoing>Flow_Bearbeiten_Ausstellen</bpmn:outgoing>
    </bpmn:task>

    <bpmn:task id="Task_Angelschein_ausstellen" name="Angelschein ausstellen">
      <bpmn:incoming>Flow_Bearbeiten_Ausstellen</bpmn:incoming>
      <bpmn:outgoing>Flow_Ausstellen_Ende</bpmn:outgoing>
    </bpmn:task>

    <bpmn:endEvent id="EndEvent_Org" name="Angelschein ausgestellt">
      <bpmn:incoming>Flow_Ausstellen_Ende</bpmn:incoming>
    </bpmn:endEvent>

    <bpmn:sequenceFlow id="Flow_Start_Zustaendigkeit"
                       sourceRef="StartEvent_Org"
                       targetRef="Task_Zustaendigkeit" />

    <bpmn:sequenceFlow id="Flow_Zustaendigkeit_Vollstaendigkeit"
                       sourceRef="Task_Zustaendigkeit"
                       targetRef="Task_Vollstaendigkeit" />

    <bpmn:sequenceFlow id="Flow_Vollstaendigkeit_Bearbeiten"
                       sourceRef="Task_Vollstaendigkeit"
                       targetRef="Task_Antrag_bearbeiten" />

    <bpmn:sequenceFlow id="Flow_Bearbeiten_Ausstellen"
                       sourceRef="Task_Antrag_bearbeiten"
                       targetRef="Task_Angelschein_ausstellen" />

    <bpmn:sequenceFlow id="Flow_Ausstellen_Ende"
                       sourceRef="Task_Angelschein_ausstellen"
                       targetRef="EndEvent_Org" />
  </bpmn:process>

  <bpmndi:BPMNDiagram id="BPMNDiagram_1">
    <bpmndi:BPMNPlane id="BPMNPlane_1" bpmnElement="Collab_Angelschein">

      <bpmndi:BPMNShape id="Participant_Kunde_di"
                        bpmnElement="Participant_Kunde"
                        isHorizontal="true">
        <dc:Bounds x="100" y="100" width="1200" height="250" />
      </bpmndi:BPMNShape>

      <bpmndi:BPMNShape id="Participant_Organisation_di"
                        bpmnElement="Participant_Organisation"
                        isHorizontal="true">
        <dc:Bounds x="100" y="380" width="1200" height="250" />
      </bpmndi:BPMNShape>

      <!-- Shapes Pool Kunde -->
      <bpmndi:BPMNShape id="StartEvent_Kunde_di" bpmnElement="StartEvent_Kunde">
        <dc:Bounds x="150" y="190" width="36" height="36" />
      </bpmndi:BPMNShape>

      <bpmndi:BPMNShape id="Task_Antrag_stellen_di" bpmnElement="Task_Antrag_stellen">
        <dc:Bounds x="230" y="170" width="120" height="80" />
      </bpmndi:BPMNShape>

      <bpmndi:BPMNShape id="Task_Angelschein_empfangen_di" bpmnElement="Task_Angelschein_empfangen">
        <dc:Bounds x="390" y="170" width="120" height="80" />
      </bpmndi:BPMNShape>

      <bpmndi:BPMNShape id="EndEvent_Kunde_di" bpmnElement="EndEvent_Kunde">
        <dc:Bounds x="550" y="190" width="36" height="36" />
      </bpmndi:BPMNShape>

      <!-- Shapes Pool Organisation -->
      <bpmndi:BPMNShape id="StartEvent_Org_di" bpmnElement="StartEvent_Org">
        <dc:Bounds x="150" y="470" width="36" height="36" />
      </bpmndi:BPMNShape>

      <bpmndi:BPMNShape id="Task_Zustaendigkeit_di" bpmnElement="Task_Zustaendigkeit">
        <dc:Bounds x="230" y="450" width="120" height="80" />
      </bpmndi:BPMNShape>

      <bpmndi:BPMNShape id="Task_Vollstaendigkeit_di" bpmnElement="Task_Vollstaendigkeit">
        <dc:Bounds x="390" y="450" width="120" height="80" />
      </bpmndi:BPMNShape>

      <bpmndi:BPMNShape id="Task_Antrag_bearbeiten_di" bpmnElement="Task_Antrag_bearbeiten">
        <dc:Bounds x="550" y="450" width="120" height="80" />
      </bpmndi:BPMNShape>

      <bpmndi:BPMNShape id="Task_Angelschein_ausstellen_di" bpmnElement="Task_Angelschein_ausstellen">
        <dc:Bounds x="710" y="450" width="120" height="80" />
      </bpmndi:BPMNShape>

      <bpmndi:BPMNShape id="EndEvent_Org_di" bpmnElement="EndEvent_Org">
        <dc:Bounds x="870" y="470" width="36" height="36" />
      </bpmndi:BPMNShape>

      <!-- Sequence Flows Kunde -->
      <bpmndi:BPMNEdge id="Flow_Start_Antrag_di" bpmnElement="Flow_Start_Antrag">
        <di:waypoint x="186" y="208" />
        <di:waypoint x="230" y="208" />
      </bpmndi:BPMNEdge>

      <bpmndi:BPMNEdge id="Flow_Antrag_Empfangen_di" bpmnElement="Flow_Antrag_Empfangen">
        <di:waypoint x="350" y="208" />
        <di:waypoint x="390" y="208" />
      </bpmndi:BPMNEdge>

      <bpmndi:BPMNEdge id="Flow_Empfangen_Ende_di" bpmnElement="Flow_Empfangen_Ende">
        <di:waypoint x="510" y="208" />
        <di:waypoint x="550" y="208" />
      </bpmndi:BPMNEdge>

      <!-- Sequence Flows Organisation -->
      <bpmndi:BPMNEdge id="Flow_Start_Zustaendigkeit_di" bpmnElement="Flow_Start_Zustaendigkeit">
        <di:waypoint x="186" y="488" />
        <di:waypoint x="230" y="488" />
      </bpmndi:BPMNEdge>

      <bpmndi:BPMNEdge id="Flow_Zustaendigkeit_Vollstaendigkeit_di" bpmnElement="Flow_Zustaendigkeit_Vollstaendigkeit">
        <di:waypoint x="350" y="488" />
        <di:waypoint x="390" y="488" />
      </bpmndi:BPMNEdge>

      <bpmndi:BPMNEdge id="Flow_Vollstaendigkeit_Bearbeiten_di" bpmnElement="Flow_Vollstaendigkeit_Bearbeiten">
        <di:waypoint x="510" y="488" />
        <di:waypoint x="550" y="488" />
      </bpmndi:BPMNEdge>

      <bpmndi:BPMNEdge id="Flow_Bearbeiten_Ausstellen_di" bpmnElement="Flow_Bearbeiten_Ausstellen">
        <di:waypoint x="670" y="488" />
        <di:waypoint x="710" y="488" />
      </bpmndi:BPMNEdge>

      <bpmndi:BPMNEdge id="Flow_Ausstellen_Ende_di" bpmnElement="Flow_Ausstellen_Ende">
        <di:waypoint x="830" y="488" />
        <di:waypoint x="870" y="488" />
      </bpmndi:BPMNEdge>

      <!-- Message Flows -->
      <bpmndi:BPMNEdge id="MsgFlow_Antrag_di" bpmnElement="MsgFlow_Antrag">
        <di:waypoint x="290" y="250" />
        <di:waypoint x="290" y="470" />
      </bpmndi:BPMNEdge>

      <bpmndi:BPMNEdge id="MsgFlow_Angelschein_di" bpmnElement="MsgFlow_Angelschein">
        <di:waypoint x="770" y="450" />
        <di:waypoint x="770" y="360" />
        <di:waypoint x="450" y="360" />
        <di:waypoint x="450" y="250" />
      </bpmndi:BPMNEdge>

    </bpmndi:BPMNPlane>
  </bpmndi:BPMNDiagram>
</bpmn:definitions>
