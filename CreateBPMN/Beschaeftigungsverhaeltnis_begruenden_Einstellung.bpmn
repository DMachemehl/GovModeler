<?xml version="1.0" encoding="UTF-8"?>
<bpmn:definitions
  xmlns:bpmn="http://www.omg.org/spec/BPMN/20100524/MODEL"
  xmlns:bpmndi="http://www.omg.org/spec/BPMN/20100524/DI"
  xmlns:dc="http://www.omg.org/spec/DD/20100524/DC"
  xmlns:di="http://www.omg.org/spec/DD/20100524/DI"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xmlns:camunda="http://camunda.org/schema/1.0/bpmn"
  targetNamespace="http://example.com/bpmn"
  exporter="GitHub-Copilot-GovModeler"
  exporterVersion="2.0"
  modeler:executionPlatform="Camunda Platform"
  modeler:executionPlatformVersion="7.20.0">

  <!-- =====================================================
       COLLABORATION - Prozess-Interaktion
       ===================================================== -->
  <bpmn:collaboration id="Collab_Beschaeftigungsverhaeltnis_begruenden">
    
    <!-- Participant: Kunde -->
    <bpmn:participant id="Participant_Kunde"
                      name="Kunde"
                      processRef="Process_Kunde" />
    
    <!-- Participant: Personalstelle dezentral -->
    <bpmn:participant id="Participant_Personalstelle_dezentral"
                      name="Personalstelle dezentral"
                      processRef="Process_Personalstelle_dezentral" />
    
    <!-- Participant: Personalstelle zentral -->
    <bpmn:participant id="Participant_Personalstelle_zentral"
                      name="Personalstelle zentral"
                      processRef="Process_Personalstelle_zentral" />
    
    <!-- Message Flows zwischen Participanten -->
    <bpmn:messageFlow id="MsgFlow_Einstellungsauftrag"
                      name="Einstellungsauftrag"
                      sourceRef="Task_Einstellungsauftrag_uebermitteln"
                      targetRef="Event_Einstellungsauftrag_eingegangen" />
    
    <bpmn:messageFlow id="MsgFlow_Beratung"
                      name="Beratung"
                      sourceRef="Task_Kundenberatung"
                      targetRef="Event_Beratung_erhalten" />
    
    <bpmn:messageFlow id="MsgFlow_Einstellungsinformationen"
                      name="Einstellungsinformationen"
                      sourceRef="Task_Einstellungsinfo_konsolidieren"
                      targetRef="Event_Einstellungsinfo_erhalten" />
    
    <bpmn:messageFlow id="MsgFlow_Personalratsbeteiligung"
                      name="Personalratsbeteiligung"
                      sourceRef="Task_Personalratsbeteiligung"
                      targetRef="Event_Personalratsbeteiligung_erhalten" />
    
    <bpmn:messageFlow id="MsgFlow_Vertragskonditionen"
                      name="Vertragskonditionen"
                      sourceRef="Task_Vertragskonditionen"
                      targetRef="Event_Vertragskonditionen_erhalten" />
    
    <bpmn:messageFlow id="MsgFlow_Einstellungsunterlagen"
                      name="Einstellungsunterlagen"
                      sourceRef="Task_Einstellungsunterlagen_versenden"
                      targetRef="Event_Einstellungsunterlagen_erhalten" />
    
    <bpmn:messageFlow id="MsgFlow_Ausgefuellte_Unterlagen"
                      name="Ausgefüllte Unterlagen"
                      sourceRef="Task_Unterlagen_zuruecksenden"
                      targetRef="Event_Ausgefuellte_Unterlagen_erhalten" />
    
    <bpmn:messageFlow id="MsgFlow_Eintrittsgespräch"
                      name="Eintrittsgespräch"
                      sourceRef="Task_Eintrittsgespräch"
                      targetRef="Event_Eintrittsgespräch_erhalten" />
    
    <bpmn:messageFlow id="MsgFlow_Eintritt_abgeschlossen"
                      name="Eintritt abgeschlossen"
                      sourceRef="Task_Eintritt_bearbeiten"
                      targetRef="Event_Eintritt_abgeschlossen" />
  </bpmn:collaboration>

  <!-- =====================================================
       KUNDE PROZESS
       ===================================================== -->
  <bpmn:process id="Process_Kunde"
                name="Beschaeftigungsverhaeltnis_begruenden_Kunde"
                isExecutable="true">
    
    <!-- Start Event -->
    <bpmn:startEvent id="StartEvent_Kunde" name="Einstellungsbedarf bestätigt">
      <bpmn:outgoing>Flow_Start_Auftrag</bpmn:outgoing>
    </bpmn:startEvent>
    
    <!-- User Task: Einstellungsauftrag übermitteln -->
    <bpmn:userTask id="Task_Einstellungsauftrag_uebermitteln" name="Einstellungsauftrag übermitteln">
      <bpmn:incoming>Flow_Start_Auftrag</bpmn:incoming>
      <bpmn:outgoing>Flow_Auftrag_Warten</bpmn:outgoing>
    </bpmn:userTask>
    
    <!-- Intermediate Catch Event: Beratung erhalten -->
    <bpmn:intermediateCatchEvent id="Event_Beratung_erhalten" name="Beratung erhalten">
      <bpmn:incoming>Flow_Auftrag_Warten</bpmn:incoming>
      <bpmn:outgoing>Flow_Beratung_Personalrat</bpmn:outgoing>
      <bpmn:messageEventDefinition id="MessageEventDef_Beratung" messageRef="Message_Beratung" />
    </bpmn:intermediateCatchEvent>
    
    <!-- Intermediate Catch Event: Personalratsbeteiligung -->
    <bpmn:intermediateCatchEvent id="Event_Personalratsbeteiligung_erhalten" name="Personalratsbeteiligung erhalten">
      <bpmn:incoming>Flow_Beratung_Personalrat</bpmn:incoming>
      <bpmn:outgoing>Flow_Personalrat_Vertragskonditionen</bpmn:outgoing>
      <bpmn:messageEventDefinition id="MessageEventDef_Personalratsbeteiligung" messageRef="Message_Personalratsbeteiligung" />
    </bpmn:intermediateCatchEvent>
    
    <!-- Intermediate Catch Event: Vertragskonditionen -->
    <bpmn:intermediateCatchEvent id="Event_Vertragskonditionen_erhalten" name="Vertragskonditionen erhalten">
      <bpmn:incoming>Flow_Personalrat_Vertragskonditionen</bpmn:incoming>
      <bpmn:outgoing>Flow_Vertragskonditionen_Unterlagen</bpmn:outgoing>
      <bpmn:messageEventDefinition id="MessageEventDef_Vertragskonditionen" messageRef="Message_Vertragskonditionen" />
    </bpmn:intermediateCatchEvent>
    
    <!-- Intermediate Catch Event: Einstellungsunterlagen erhalten -->
    <bpmn:intermediateCatchEvent id="Event_Einstellungsunterlagen_erhalten" name="Einstellungsunterlagen erhalten">
      <bpmn:incoming>Flow_Vertragskonditionen_Unterlagen</bpmn:incoming>
      <bpmn:outgoing>Flow_Unterlagen_Zuruecksenden</bpmn:outgoing>
      <bpmn:messageEventDefinition id="MessageEventDef_Einstellungsunterlagen" messageRef="Message_Einstellungsunterlagen" />
    </bpmn:intermediateCatchEvent>
    
    <!-- User Task: Unterlagen zurücksenden -->
    <bpmn:userTask id="Task_Unterlagen_zuruecksenden" name="Unterlagen zurücksenden">
      <bpmn:incoming>Flow_Unterlagen_Zuruecksenden</bpmn:incoming>
      <bpmn:outgoing>Flow_Zuruecksenden_Eintrittsgespräch</bpmn:outgoing>
    </bpmn:userTask>
    
    <!-- Intermediate Catch Event: Eintrittsgespräch erhalten -->
    <bpmn:intermediateCatchEvent id="Event_Eintrittsgespräch_erhalten" name="Eintrittsgespräch erhalten">
      <bpmn:incoming>Flow_Zuruecksenden_Eintrittsgespräch</bpmn:incoming>
      <bpmn:outgoing>Flow_Eintrittsgespräch_Eintritt</bpmn:outgoing>
      <bpmn:messageEventDefinition id="MessageEventDef_Eintrittsgespräch" messageRef="Message_Eintrittsgespräch" />
    </bpmn:intermediateCatchEvent>
    
    <!-- Intermediate Catch Event: Eintritt abgeschlossen -->
    <bpmn:intermediateCatchEvent id="Event_Eintritt_abgeschlossen" name="Eintritt abgeschlossen">
      <bpmn:incoming>Flow_Eintrittsgespräch_Eintritt</bpmn:incoming>
      <bpmn:outgoing>Flow_Eintritt_Ende</bpmn:outgoing>
      <bpmn:messageEventDefinition id="MessageEventDef_Eintritt_abgeschlossen" messageRef="Message_Eintritt_abgeschlossen" />
    </bpmn:intermediateCatchEvent>
    
    <!-- End Event -->
    <bpmn:endEvent id="EndEvent_Kunde" name="Einstellung vorbereitet">
      <bpmn:incoming>Flow_Eintritt_Ende</bpmn:incoming>
    </bpmn:endEvent>
    
    <!-- Sequence Flows - Kunde -->
    <bpmn:sequenceFlow id="Flow_Start_Auftrag"
                       sourceRef="StartEvent_Kunde"
                       targetRef="Task_Einstellungsauftrag_uebermitteln" />
    
    <bpmn:sequenceFlow id="Flow_Auftrag_Warten"
                       sourceRef="Task_Einstellungsauftrag_uebermitteln"
                       targetRef="Event_Beratung_erhalten" />
    
    <bpmn:sequenceFlow id="Flow_Beratung_Personalrat"
                       sourceRef="Event_Beratung_erhalten"
                       targetRef="Event_Personalratsbeteiligung_erhalten" />
    
    <bpmn:sequenceFlow id="Flow_Personalrat_Vertragskonditionen"
                       sourceRef="Event_Personalratsbeteiligung_erhalten"
                       targetRef="Event_Vertragskonditionen_erhalten" />
    
    <bpmn:sequenceFlow id="Flow_Vertragskonditionen_Unterlagen"
                       sourceRef="Event_Vertragskonditionen_erhalten"
                       targetRef="Event_Einstellungsunterlagen_erhalten" />
    
    <bpmn:sequenceFlow id="Flow_Unterlagen_Zuruecksenden"
                       sourceRef="Event_Einstellungsunterlagen_erhalten"
                       targetRef="Task_Unterlagen_zuruecksenden" />
    
    <bpmn:sequenceFlow id="Flow_Zuruecksenden_Eintrittsgespräch"
                       sourceRef="Task_Unterlagen_zuruecksenden"
                       targetRef="Event_Eintrittsgespräch_erhalten" />
    
    <bpmn:sequenceFlow id="Flow_Eintrittsgespräch_Eintritt"
                       sourceRef="Event_Eintrittsgespräch_erhalten"
                       targetRef="Event_Eintritt_abgeschlossen" />
    
    <bpmn:sequenceFlow id="Flow_Eintritt_Ende"
                       sourceRef="Event_Eintritt_abgeschlossen"
                       targetRef="EndEvent_Kunde" />
  </bpmn:process>

  <!-- =====================================================
       PERSONALSTELLE DEZENTRAL PROZESS
       ===================================================== -->
  <bpmn:process id="Process_Personalstelle_dezentral"
                name="Personalstelle_dezentral_Prozess"
                isExecutable="true">
    
    <!-- Start Event -->
    <bpmn:startEvent id="StartEvent_Personalstelle_dezentral" name="Beratungsanfrage eingegangen">
      <bpmn:outgoing>Flow_Dez_Start_Beratung</bpmn:outgoing>
    </bpmn:startEvent>
    
    <!-- User Task: Kundenberatung durchführen -->
    <bpmn:userTask id="Task_Kundenberatung" name="Kundenberatung durchführen">
      <bpmn:incoming>Flow_Dez_Start_Beratung</bpmn:incoming>
      <bpmn:outgoing>Flow_Dez_Beratung_Konsolidieren</bpmn:outgoing>
    </bpmn:userTask>
    
    <!-- User Task: Einstellungsinformationen konsolidieren -->
    <bpmn:userTask id="Task_Einstellungsinfo_konsolidieren" name="Einstellungsinformationen konsolidieren">
      <bpmn:incoming>Flow_Dez_Beratung_Konsolidieren</bpmn:incoming>
      <bpmn:outgoing>Flow_Dez_Konsolidieren_Ende</bpmn:outgoing>
    </bpmn:userTask>
    
    <!-- End Event -->
    <bpmn:endEvent id="EndEvent_Personalstelle_dezentral" name="Beratung abgeschlossen">
      <bpmn:incoming>Flow_Dez_Konsolidieren_Ende</bpmn:incoming>
    </bpmn:endEvent>
    
    <!-- Sequence Flows - Personalstelle dezentral -->
    <bpmn:sequenceFlow id="Flow_Dez_Start_Beratung"
                       sourceRef="StartEvent_Personalstelle_dezentral"
                       targetRef="Task_Kundenberatung" />
    
    <bpmn:sequenceFlow id="Flow_Dez_Beratung_Konsolidieren"
                       sourceRef="Task_Kundenberatung"
                       targetRef="Task_Einstellungsinfo_konsolidieren" />
    
    <bpmn:sequenceFlow id="Flow_Dez_Konsolidieren_Ende"
                       sourceRef="Task_Einstellungsinfo_konsolidieren"
                       targetRef="EndEvent_Personalstelle_dezentral" />
  </bpmn:process>

  <!-- =====================================================
       PERSONALSTELLE ZENTRAL PROZESS
       ===================================================== -->
  <bpmn:process id="Process_Personalstelle_zentral"
                name="Personalstelle_zentral_Prozess"
                isExecutable="true">
    
    <!-- Start Event -->
    <bpmn:startEvent id="Event_Einstellungsauftrag_eingegangen" name="Einstellungsauftrag eingegangen">
      <bpmn:outgoing>Flow_Zen_Start_Einstellungsinfo</bpmn:outgoing>
    </bpmn:startEvent>
    
    <!-- Intermediate Catch Event: Einstellungsinformationen -->
    <bpmn:intermediateCatchEvent id="Event_Einstellungsinfo_erhalten" name="Einstellungsinformationen erhalten">
      <bpmn:incoming>Flow_Zen_Start_Einstellungsinfo</bpmn:incoming>
      <bpmn:outgoing>Flow_Zen_Einstellungsinfo_Zustaendigkeit</bpmn:outgoing>
      <bpmn:messageEventDefinition id="MessageEventDef_Einstellungsinfo" messageRef="Message_Einstellungsinfo" />
    </bpmn:intermediateCatchEvent>
    
    <!-- Service Task: Zuständigkeit prüfen -->
    <bpmn:serviceTask id="Task_Zustaendigkeit_pruefen" name="Zuständigkeit prüfen">
      <bpmn:incoming>Flow_Zen_Einstellungsinfo_Zustaendigkeit</bpmn:incoming>
      <bpmn:outgoing>Flow_Zen_Zustaendigkeit_Vollstaendigkeit</bpmn:outgoing>
    </bpmn:serviceTask>
    
    <!-- Service Task: Vollständigkeit prüfen -->
    <bpmn:serviceTask id="Task_Vollstaendigkeit_pruefen" name="Vollständigkeit prüfen">
      <bpmn:incoming>Flow_Zen_Zustaendigkeit_Vollstaendigkeit</bpmn:incoming>
      <bpmn:outgoing>Flow_Zen_Vollstaendigkeit_Personalrat</bpmn:outgoing>
    </bpmn:serviceTask>
    
    <!-- User Task: Personalratsbeteiligung einleiten -->
    <bpmn:userTask id="Task_Personalratsbeteiligung" name="Personalratsbeteiligung einleiten">
      <bpmn:incoming>Flow_Zen_Vollstaendigkeit_Personalrat</bpmn:incoming>
      <bpmn:outgoing>Flow_Zen_Personalrat_Vertragskonditionen</bpmn:outgoing>
    </bpmn:userTask>
    
    <!-- User Task: Vertragskonditionen finalisieren -->
    <bpmn:userTask id="Task_Vertragskonditionen" name="Vertragskonditionen finalisieren">
      <bpmn:incoming>Flow_Zen_Personalrat_Vertragskonditionen</bpmn:incoming>
      <bpmn:outgoing>Flow_Zen_Vertragskonditionen_Vertrag</bpmn:outgoing>
    </bpmn:userTask>
    
    <!-- User Task: Arbeitsvertrag fertigen -->
    <bpmn:userTask id="Task_Arbeitsvertrag_fertigen" name="Arbeitsvertrag fertigen">
      <bpmn:incoming>Flow_Zen_Vertragskonditionen_Vertrag</bpmn:incoming>
      <bpmn:outgoing>Flow_Zen_Vertrag_Versenden</bpmn:outgoing>
    </bpmn:userTask>
    
    <!-- User Task: Einstellungsunterlagen versenden -->
    <bpmn:userTask id="Task_Einstellungsunterlagen_versenden" name="Einstellungsunterlagen versenden">
      <bpmn:incoming>Flow_Zen_Vertrag_Versenden</bpmn:incoming>
      <bpmn:outgoing>Flow_Zen_Versenden_Warten</bpmn:outgoing>
    </bpmn:userTask>
    
    <!-- Intermediate Catch Event: Ausgefüllte Unterlagen erhalten -->
    <bpmn:intermediateCatchEvent id="Event_Ausgefuellte_Unterlagen_erhalten" name="Ausgefüllte Unterlagen erhalten">
      <bpmn:incoming>Flow_Zen_Versenden_Warten</bpmn:incoming>
      <bpmn:outgoing>Flow_Zen_Unterlagen_Gehaltsrelevant</bpmn:outgoing>
      <bpmn:messageEventDefinition id="MessageEventDef_Ausgefuellte_Unterlagen" messageRef="Message_Ausgefuellte_Unterlagen" />
    </bpmn:intermediateCatchEvent>
    
    <!-- User Task: Gehaltsrelevante Unterlagen weiterverarbeiten -->
    <bpmn:userTask id="Task_Gehaltsrelevante_Unterlagen" name="Gehaltsrelevante Unterlagen weiterverarbeiten">
      <bpmn:incoming>Flow_Zen_Unterlagen_Gehaltsrelevant</bpmn:incoming>
      <bpmn:outgoing>Flow_Zen_Gehaltsrelevant_Voraberfassung</bpmn:outgoing>
    </bpmn:userTask>
    
    <!-- User Task: Voraberfassung durchführen -->
    <bpmn:userTask id="Task_Voraberfassung" name="Voraberfassung durchführen">
      <bpmn:incoming>Flow_Zen_Gehaltsrelevant_Voraberfassung</bpmn:incoming>
      <bpmn:outgoing>Flow_Zen_Voraberfassung_Eintrittsgespräch</bpmn:outgoing>
    </bpmn:userTask>
    
    <!-- User Task: Eintrittsgespräch terminieren und durchführen -->
    <bpmn:userTask id="Task_Eintrittsgespräch" name="Eintrittsgespräch terminieren und durchführen">
      <bpmn:incoming>Flow_Zen_Voraberfassung_Eintrittsgespräch</bpmn:incoming>
      <bpmn:outgoing>Flow_Zen_Eintrittsgespräch_Eintritt</bpmn:outgoing>
    </bpmn:userTask>
    
    <!-- User Task: Eintritt bearbeiten -->
    <bpmn:userTask id="Task_Eintritt_bearbeiten" name="Eintritt bearbeiten (Onboarding-Administration abschließen)">
      <bpmn:incoming>Flow_Zen_Eintrittsgespräch_Eintritt</bpmn:incoming>
      <bpmn:outgoing>Flow_Zen_Eintritt_Ende</bpmn:outgoing>
    </bpmn:userTask>
    
    <!-- End Event -->
    <bpmn:endEvent id="EndEvent_Personalstelle_zentral" name="Beschäftigungsverhältnis administrativ begründet">
      <bpmn:incoming>Flow_Zen_Eintritt_Ende</bpmn:incoming>
    </bpmn:endEvent>
    
    <!-- Sequence Flows - Personalstelle zentral -->
    <bpmn:sequenceFlow id="Flow_Zen_Start_Einstellungsinfo"
                       sourceRef="Event_Einstellungsauftrag_eingegangen"
                       targetRef="Event_Einstellungsinfo_erhalten" />
    
    <bpmn:sequenceFlow id="Flow_Zen_Einstellungsinfo_Zustaendigkeit"
                       sourceRef="Event_Einstellungsinfo_erhalten"
                       targetRef="Task_Zustaendigkeit_pruefen" />
    
    <bpmn:sequenceFlow id="Flow_Zen_Zustaendigkeit_Vollstaendigkeit"
                       sourceRef="Task_Zustaendigkeit_pruefen"
                       targetRef="Task_Vollstaendigkeit_pruefen" />
    
    <bpmn:sequenceFlow id="Flow_Zen_Vollstaendigkeit_Personalrat"
                       sourceRef="Task_Vollstaendigkeit_pruefen"
                       targetRef="Task_Personalratsbeteiligung" />
    
    <bpmn:sequenceFlow id="Flow_Zen_Personalrat_Vertragskonditionen"
                       sourceRef="Task_Personalratsbeteiligung"
                       targetRef="Task_Vertragskonditionen" />
    
    <bpmn:sequenceFlow id="Flow_Zen_Vertragskonditionen_Vertrag"
                       sourceRef="Task_Vertragskonditionen"
                       targetRef="Task_Arbeitsvertrag_fertigen" />
    
    <bpmn:sequenceFlow id="Flow_Zen_Vertrag_Versenden"
                       sourceRef="Task_Arbeitsvertrag_fertigen"
                       targetRef="Task_Einstellungsunterlagen_versenden" />
    
    <bpmn:sequenceFlow id="Flow_Zen_Versenden_Warten"
                       sourceRef="Task_Einstellungsunterlagen_versenden"
                       targetRef="Event_Ausgefuellte_Unterlagen_erhalten" />
    
    <bpmn:sequenceFlow id="Flow_Zen_Unterlagen_Gehaltsrelevant"
                       sourceRef="Event_Ausgefuellte_Unterlagen_erhalten"
                       targetRef="Task_Gehaltsrelevante_Unterlagen" />
    
    <bpmn:sequenceFlow id="Flow_Zen_Gehaltsrelevant_Voraberfassung"
                       sourceRef="Task_Gehaltsrelevante_Unterlagen"
                       targetRef="Task_Voraberfassung" />
    
    <bpmn:sequenceFlow id="Flow_Zen_Voraberfassung_Eintrittsgespräch"
                       sourceRef="Task_Voraberfassung"
                       targetRef="Task_Eintrittsgespräch" />
    
    <bpmn:sequenceFlow id="Flow_Zen_Eintrittsgespräch_Eintritt"
                       sourceRef="Task_Eintrittsgespräch"
                       targetRef="Task_Eintritt_bearbeiten" />
    
    <bpmn:sequenceFlow id="Flow_Zen_Eintritt_Ende"
                       sourceRef="Task_Eintritt_bearbeiten"
                       targetRef="EndEvent_Personalstelle_zentral" />
  </bpmn:process>

  <!-- =====================================================
       MESSAGE DEFINITIONEN
       ===================================================== -->
  <bpmn:message id="Message_Einstellungsauftrag" name="Einstellungsauftrag" />
  <bpmn:message id="Message_Beratung" name="Beratung" />
  <bpmn:message id="Message_Einstellungsinfo" name="Einstellungsinformationen" />
  <bpmn:message id="Message_Personalratsbeteiligung" name="Personalratsbeteiligung" />
  <bpmn:message id="Message_Vertragskonditionen" name="Vertragskonditionen" />
  <bpmn:message id="Message_Einstellungsunterlagen" name="Einstellungsunterlagen" />
  <bpmn:message id="Message_Ausgefuellte_Unterlagen" name="Ausgefüllte Unterlagen" />
  <bpmn:message id="Message_Eintrittsgespräch" name="Eintrittsgespräch" />
  <bpmn:message id="Message_Eintritt_abgeschlossen" name="Eintritt abgeschlossen" />

  <!-- =====================================================
       BPMN-DI VISUALISIERUNG - Horizontale Task-Anordnung
       ===================================================== -->
  <bpmndi:BPMNDiagram id="BPMNDiagram_Beschaeftigungsverhaeltnis_begruenden">
    <bpmndi:BPMNPlane id="BPMNPlane_Beschaeftigungsverhaeltnis_begruenden" bpmnElement="Collab_Beschaeftigungsverhaeltnis_begruenden">
      
      <!-- ===============================
           POOL: KUNDE (oben)
           =============================== -->
      <bpmndi:BPMNShape id="Participant_Kunde_di" bpmnElement="Participant_Kunde" isHorizontal="true">
        <dc:Bounds x="160" y="80" width="2200" height="250" />
        <bpmndi:BPMNLabel>
          <dc:Bounds x="170" y="190" width="20" height="50" />
        </bpmndi:BPMNLabel>
      </bpmndi:BPMNShape>
      
      <!-- Kunde Process Elements - ALLE auf Y=160 -->
      <bpmndi:BPMNShape id="StartEvent_Kunde_di" bpmnElement="StartEvent_Kunde">
        <dc:Bounds x="222" y="182" width="36" height="36" />
        <bpmndi:BPMNLabel>
          <dc:Bounds x="200" y="225" width="80" height="27" />
        </bpmndi:BPMNLabel>
      </bpmndi:BPMNShape>
      
      <bpmndi:BPMNShape id="Task_Einstellungsauftrag_uebermitteln_di" bpmnElement="Task_Einstellungsauftrag_uebermitteln">
        <dc:Bounds x="310" y="160" width="100" height="80" />
      </bpmndi:BPMNShape>
      
      <bpmndi:BPMNShape id="Event_Beratung_erhalten_di" bpmnElement="Event_Beratung_erhalten">
        <dc:Bounds x="492" y="182" width="36" height="36" />
        <bpmndi:BPMNLabel>
          <dc:Bounds x="470" y="225" width="80" height="14" />
        </bpmndi:BPMNLabel>
      </bpmndi:BPMNShape>
      
      <bpmndi:BPMNShape id="Event_Personalratsbeteiligung_erhalten_di" bpmnElement="Event_Personalratsbeteiligung_erhalten">
        <dc:Bounds x="612" y="182" width="36" height="36" />
        <bpmndi:BPMNLabel>
          <dc:Bounds x="590" y="145" width="80" height="27" />
        </bpmndi:BPMNLabel>
      </bpmndi:BPMNShape>
      
      <bpmndi:BPMNShape id="Event_Vertragskonditionen_erhalten_di" bpmnElement="Event_Vertragskonditionen_erhalten">
        <dc:Bounds x="732" y="182" width="36" height="36" />
        <bpmndi:BPMNLabel>
          <dc:Bounds x="710" y="225" width="80" height="27" />
