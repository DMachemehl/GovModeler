
<?xml version="1.0" encoding="UTF-8"?>
<bpmn:definitions
  xmlns:bpmn="http://www.omg.org/spec/BPMN/20100524/MODEL"
  xmlns:bpmndi="http://www.omg.org/spec/BPMN/20100524/DI"
  xmlns:dc="http://www.omg.org/spec/DD/20100524/DC"
  xmlns:di="http://www.omg.org/spec/DD/20100524/DI"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xmlns:camunda="http://camunda.org/schema/1.0/bpmn"
  targetNamespace="http://example.com/bpmn"
  exporter="GitHub-Copilot-GovModeler"
  exporterVersion="2.0"
  modeler:executionPlatform="Camunda Platform"
  modeler:executionPlatformVersion="7.20.0">

  <!-- COLLABORATION -->
  <bpmn:collaboration id="Collab_Wohngeld_verlaengern">
    
    <bpmn:participant id="Participant_Buerger"
                      name="Bürger"
                      processRef="Process_Buerger" />
    
    <bpmn:participant id="Participant_IT"
                      name="IT"
                      processRef="Process_IT" />
    
    <bpmn:participant id="Participant_Wohngeldstelle"
                      name="Wohngeldstelle"
                      processRef="Process_Wohngeldstelle" />
    
    <!-- Message Flows -->
    <bpmn:messageFlow id="MsgFlow_Verlaengerungsantrag"
                      name="Verlängerungsantrag"
                      sourceRef="Task_Verlaengerungsantrag_stellen"
                      targetRef="StartEvent_Verlaengerungsantrag_eingegangen" />
    
    <bpmn:messageFlow id="MsgFlow_Wohngeldbescheid"
                      name="Verlängerter Wohngeldbescheid"
                      sourceRef="Task_Wohngeldbescheid_erstellen"
                      targetRef="Event_Wohngeldbescheid_erhalten" />
                      
    <bpmn:messageFlow id="MsgFlow_IT_Anfrage"
                      name="Datenabfrage"
                      sourceRef="Task_IT_System_abfragen"
                      targetRef="StartEvent_IT_Anfrage_erhalten" />
                      
    <bpmn:messageFlow id="MsgFlow_IT_Antwort"
                      name="Aktuelle Daten"
                      sourceRef="Task_IT_Daten_bereitstellen"
                      targetRef="Event_IT_Antwort_erhalten" />
  </bpmn:collaboration>

  <!-- BÜRGER PROZESS -->
  <bpmn:process id="Process_Buerger" name="Wohngeld_verlaengern_Buerger" isExecutable="true">
    
    <bpmn:startEvent id="StartEvent_Buerger" name="Wohngeld läuft ab">
      <bpmn:outgoing>Flow_Start_Verlaengerungsantrag</bpmn:outgoing>
    </bpmn:startEvent>
    
    <bpmn:userTask id="Task_Verlaengerungsantrag_stellen" name="Verlängerungsantrag vollständig stellen">
      <bpmn:incoming>Flow_Start_Verlaengerungsantrag</bpmn:incoming>
      <bpmn:outgoing>Flow_Verlaengerungsantrag_Warten</bpmn:outgoing>
    </bpmn:userTask>
    
    <bpmn:intermediateCatchEvent id="Event_Wohngeldbescheid_erhalten" name="Verlängerten Wohngeldbescheid erhalten">
      <bpmn:incoming>Flow_Verlaengerungsantrag_Warten</bpmn:incoming>
      <bpmn:outgoing>Flow_Wohngeldbescheid_Ende</bpmn:outgoing>
      <bpmn:messageEventDefinition id="MessageEventDef_Wohngeldbescheid" messageRef="Message_Wohngeldbescheid" />
    </bpmn:intermediateCatchEvent>
    
    <bpmn:endEvent id="EndEvent_Buerger" name="Wohngeld erfolgreich verlängert">
      <bpmn:incoming>Flow_Wohngeldbescheid_Ende</bpmn:incoming>
    </bpmn:endEvent>
    
    <!-- Sequence Flows - Bürger -->
    <bpmn:sequenceFlow id="Flow_Start_Verlaengerungsantrag" sourceRef="StartEvent_Buerger" targetRef="Task_Verlaengerungsantrag_stellen" />
    <bpmn:sequenceFlow id="Flow_Verlaengerungsantrag_Warten" sourceRef="Task_Verlaengerungsantrag_stellen" targetRef="Event_Wohngeldbescheid_erhalten" />
    <bpmn:sequenceFlow id="Flow_Wohngeldbescheid_Ende" sourceRef="Event_Wohngeldbescheid_erhalten" targetRef="EndEvent_Buerger" />
  </bpmn:process>

  <!-- WOHNGELDSTELLE PROZESS -->
  <bpmn:process id="Process_Wohngeldstelle" name="Wohngeld_verlaengern_Wohngeldstelle" isExecutable="true">
    
    <bpmn:startEvent id="StartEvent_Verlaengerungsantrag_eingegangen" name="Verlängerungsantrag eingegangen">
      <bpmn:outgoing>Flow_Start_Pruefung</bpmn:outgoing>
    </bpmn:startEvent>
    
    <bpmn:serviceTask id="Task_Antrag_pruefen" name="Antrag auf Vollständigkeit prüfen">
      <bpmn:incoming>Flow_Start_Pruefung</bpmn:incoming>
      <bpmn:outgoing>Flow_Pruefung_IT</bpmn:outgoing>
    </bpmn:serviceTask>
    
    <bpmn:userTask id="Task_IT_System_abfragen" name="IT-System für aktuelle Daten abfragen">
      <bpmn:incoming>Flow_Pruefung_IT</bpmn:incoming>
      <bpmn:outgoing>Flow_IT_Warten</bpmn:outgoing>
    </bpmn:userTask>
    
    <bpmn:intermediateCatchEvent id="Event_IT_Antwort_erhalten" name="Aktuelle Daten erhalten">
      <bpmn:incoming>Flow_IT_Warten</bpmn:incoming>
      <bpmn:outgoing>Flow_IT_Berechnung</bpmn:outgoing>
      <bpmn:messageEventDefinition id="MessageEventDef_IT_Antwort" messageRef="Message_IT_Antwort" />
    </bpmn:intermediateCatchEvent>
    
    <bpmn:serviceTask id="Task_Wohngeld_berechnen" name="Wohngeld neu berechnen">
      <bpmn:incoming>Flow_IT_Berechnung</bpmn:incoming>
      <bpmn:outgoing>Flow_Berechnung_Bescheid</bpmn:outgoing>
    </bpmn:serviceTask>
    
    <bpmn:userTask id="Task_Wohngeldbescheid_erstellen" name="Verlängerten Wohngeldbescheid erstellen">
      <bpmn:incoming>Flow_Berechnung_Bescheid</bpmn:incoming>
      <bpmn:outgoing>Flow_Bescheid_Ende</bpmn:outgoing>
    </bpmn:userTask>
    
    <bpmn:endEvent id="EndEvent_Wohngeldstelle" name="Verlängerung erfolgreich bearbeitet">
      <bpmn:incoming>Flow_Bescheid_Ende</bpmn:incoming>
    </bpmn:endEvent>
    
    <!-- Sequence Flows - Wohngeldstelle -->
    <bpmn:sequenceFlow id="Flow_Start_Pruefung" sourceRef="StartEvent_Verlaengerungsantrag_eingegangen" targetRef="Task_Antrag_pruefen" />
    <bpmn:sequenceFlow id="Flow_Pruefung_IT" sourceRef="Task_Antrag_pruefen" targetRef="Task_IT_System_abfragen" />
    <bpmn:sequenceFlow id="Flow_IT_Warten" sourceRef="Task_IT_System_abfragen" targetRef="Event_IT_Antwort_erhalten" />
    <bpmn:sequenceFlow id="Flow_IT_Berechnung" sourceRef="Event_IT_Antwort_erhalten" targetRef="Task_Wohngeld_berechnen" />
    <bpmn:sequenceFlow id="Flow_Berechnung_Bescheid" sourceRef="Task_Wohngeld_berechnen" targetRef="Task_Wohngeldbescheid_erstellen" />
    <bpmn:sequenceFlow id="Flow_Bescheid_Ende" sourceRef="Task_Wohngeldbescheid_erstellen" targetRef="EndEvent_Wohngeldstelle" />
  </bpmn:process>

  <!-- IT PROZESS -->
  <bpmn:process id="Process_IT" name="IT_Prozess" isExecutable="true">
    
    <bpmn:startEvent id="StartEvent_IT_Anfrage_erhalten" name="Datenabfrage erhalten">
      <bpmn:outgoing>Flow_IT_Start_Abfrage</bpmn:outgoing>
    </bpmn:startEvent>
    
    <bpmn:serviceTask id="Task_IT_Daten_abrufen" name="Aktuelle Daten abrufen">
      <bpmn:incoming>Flow_IT_Start_Abfrage</bpmn:incoming>
      <bpmn:outgoing>Flow_IT_Abrufen_Validieren</bpmn:outgoing>
    </bpmn:serviceTask>
    
    <bpmn:serviceTask id="Task_IT_Daten_validieren" name="Daten validieren und aufbereiten">
      <bpmn:incoming>Flow_IT_Abrufen_Validieren</bpmn:incoming>
      <bpmn:outgoing>Flow_IT_Validieren_Bereitstellen</bpmn:outgoing>
    </bpmn:serviceTask>
    
    <bpmn:serviceTask id="Task_IT_Daten_bereitstellen" name="Validierte Daten bereitstellen">
      <bpmn:incoming>Flow_IT_Validieren_Bereitstellen</bpmn:incoming>
      <bpmn:outgoing>Flow_IT_Bereitstellen_Ende</bpmn:outgoing>
    </bpmn:serviceTask>
    
    <bpmn:endEvent id="EndEvent_IT" name="Datenabfrage erfolgreich abgeschlossen">
      <bpmn:incoming>Flow_IT_Bereitstellen_Ende</bpmn:incoming>
    </bpmn:endEvent>
    
    <!-- Sequence Flows - IT -->
    <bpmn:sequenceFlow id="Flow_IT_Start_Abfrage" sourceRef="StartEvent_IT_Anfrage_erhalten" targetRef="Task_IT_Daten_abrufen" />
    <bpmn:sequenceFlow id="Flow_IT_Abrufen_Validieren" sourceRef="Task_IT_Daten_abrufen" targetRef="Task_IT_Daten_validieren" />
    <bpmn:sequenceFlow id="Flow_IT_Validieren_Bereitstellen" sourceRef="Task_IT_Daten_validieren" targetRef="Task_IT_Daten_bereitstellen" />
    <bpmn:sequenceFlow id="Flow_IT_Bereitstellen_Ende" sourceRef="Task_IT_Daten_bereitstellen" targetRef="EndEvent_IT" />
  </bpmn:process>

  <!-- MESSAGE DEFINITIONEN -->
