<?xml version="1.0" encoding="UTF-8"?>
<bpmn:definitions
  xmlns:bpmn="http://www.omg.org/spec/BPMN/20100524/MODEL"
  xmlns:bpmndi="http://www.omg.org/spec/BPMN/20100524/DI"
  xmlns:dc="http://www.omg.org/spec/DD/20100524/DC"
  xmlns:di="http://www.omg.org/spec/DD/20100524/DI"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xmlns:camunda="http://camunda.org/schema/1.0/bpmn"
  targetNamespace="http://example.com/bpmn"
  exporter="GitHub-Copilot-GovModeler"
  exporterVersion="2.0"
  modeler:executionPlatform="Camunda Platform"
  modeler:executionPlatformVersion="7.20.0">

  <!-- =====================================================
       COLLABORATION - Prozess-Interaktion
       ===================================================== -->
  <bpmn:collaboration id="Collab_BuT_Leistungen">
    
    <!-- Participant: Bürger/Antragsteller -->
    <bpmn:participant id="Participant_Buerger"
                      name="Buerger"
                      processRef="Process_Buerger" />
    
    <!-- Participant: IT -->
    <bpmn:participant id="Participant_IT"
                      name="IT"
                      processRef="Process_IT" />
    
    <!-- Participant: Behörde -->
    <bpmn:participant id="Participant_Behoerde"
                      name="Behoerde"
                      processRef="Process_Behoerde" />
    
    <!-- Message Flows zwischen Participanten -->
    <bpmn:messageFlow id="MsgFlow_Antrag"
                      name="Antrag"
                      sourceRef="Task_Antrag_absenden"
                      targetRef="Task_Antrag_entgegennehmen" />
    
    <bpmn:messageFlow id="MsgFlow_Daten_abrufen"
                      name="Vorgangsdaten"
                      sourceRef="Task_Daten_abrufen"
                      targetRef="Event_Daten_erhalten" />
    
    <bpmn:messageFlow id="MsgFlow_Bescheid_IT"
                      name="Bescheid"
                      sourceRef="Task_Bescheid_erstellen"
                      targetRef="Task_Bescheid_bereitstellen" />
    
    <bpmn:messageFlow id="MsgFlow_Bescheid_Buerger"
                      name="Bescheid"
                      sourceRef="Task_Bescheid_bereitstellen"
                      targetRef="Event_Bescheid_empfangen" />
    
    <bpmn:messageFlow id="MsgFlow_Rueckmeldung"
                      name="Statusmeldung"
                      sourceRef="Task_Vollstaendigkeit_pruefen"
                      targetRef="Event_Rueckmeldung_empfangen" />
  </bpmn:collaboration>

  <!-- =====================================================
       BÜRGER PROZESS
       ===================================================== -->
  <bpmn:process id="Process_Buerger"
                name="BuT_Leistungen_Buerger"
                isExecutable="true">
    
    <!-- Start Event -->
    <bpmn:startEvent id="StartEvent_Buerger" name="Antrag eingegangen">
      <bpmn:outgoing>Flow_Start_Ausfuellen</bpmn:outgoing>
    </bpmn:startEvent>
    
    <!-- User Task: Antrag ausfüllen -->
    <bpmn:userTask id="Task_Antrag_ausfuellen" name="Antrag ausfuellen" camunda:formKey="embedded:app:forms/BuT_Leistungen_Antrag.form">
      <bpmn:incoming>Flow_Start_Ausfuellen</bpmn:incoming>
      <bpmn:outgoing>Flow_Ausfuellen_Absenden</bpmn:outgoing>
    </bpmn:userTask>
    
    <!-- User Task: Antrag absenden -->
    <bpmn:userTask id="Task_Antrag_absenden" name="Antrag absenden">
      <bpmn:incoming>Flow_Ausfuellen_Absenden</bpmn:incoming>
      <bpmn:outgoing>Flow_Absenden_Rueckmeldung</bpmn:outgoing>
    </bpmn:userTask>
    
    <!-- Intermediate Catch Event: Rückmeldung empfangen -->
    <bpmn:intermediateCatchEvent id="Event_Rueckmeldung_empfangen" name="Rueckmeldung empfangen">
      <bpmn:incoming>Flow_Absenden_Rueckmeldung</bpmn:incoming>
      <bpmn:outgoing>Flow_Rueckmeldung_Bescheid</bpmn:outgoing>
      <bpmn:messageEventDefinition id="MessageEventDef_Rueckmeldung" messageRef="Message_Rueckmeldung" />
    </bpmn:intermediateCatchEvent>
    
    <!-- Intermediate Catch Event: Bescheid empfangen -->
    <bpmn:intermediateCatchEvent id="Event_Bescheid_empfangen" name="Bescheid empfangen">
      <bpmn:incoming>Flow_Rueckmeldung_Bescheid</bpmn:incoming>
      <bpmn:outgoing>Flow_Bescheid_Ende</bpmn:outgoing>
      <bpmn:messageEventDefinition id="MessageEventDef_Bescheid" messageRef="Message_Bescheid" />
    </bpmn:intermediateCatchEvent>
    
    <!-- End Event -->
    <bpmn:endEvent id="EndEvent_Buerger" name="Prozess abgeschlossen">
      <bpmn:incoming>Flow_Bescheid_Ende</bpmn:incoming>
    </bpmn:endEvent>
    
    <!-- Sequence Flows - Bürger -->
    <bpmn:sequenceFlow id="Flow_Start_Ausfuellen"
                       sourceRef="StartEvent_Buerger"
                       targetRef="Task_Antrag_ausfuellen" />
    
    <bpmn:sequenceFlow id="Flow_Ausfuellen_Absenden"
                       sourceRef="Task_Antrag_ausfuellen"
                       targetRef="Task_Antrag_absenden" />
    
    <bpmn:sequenceFlow id="Flow_Absenden_Rueckmeldung"
                       sourceRef="Task_Antrag_absenden"
                       targetRef="Event_Rueckmeldung_empfangen" />
    
    <bpmn:sequenceFlow id="Flow_Rueckmeldung_Bescheid"
                       sourceRef="Event_Rueckmeldung_empfangen"
                       targetRef="Event_Bescheid_empfangen" />
    
    <bpmn:sequenceFlow id="Flow_Bescheid_Ende"
                       sourceRef="Event_Bescheid_empfangen"
                       targetRef="EndEvent_Buerger" />
  </bpmn:process>

  <!-- =====================================================
       IT-PROZESS
       ===================================================== -->
  <bpmn:process id="Process_IT"
                name="IT_Prozess"
                isExecutable="true">
    
    <!-- Start Event -->
    <bpmn:startEvent id="StartEvent_IT" name="System gestartet">
      <bpmn:outgoing>Flow_IT_Start_Entgegennehmen</bpmn:outgoing>
    </bpmn:startEvent>
    
    <!-- Service Task: Antrag entgegennehmen -->
    <bpmn:serviceTask id="Task_Antrag_entgegennehmen" name="Antrag entgegennehmen">
      <bpmn:incoming>Flow_IT_Start_Entgegennehmen</bpmn:incoming>
      <bpmn:outgoing>Flow_Entgegennehmen_Vollstaendigkeit</bpmn:outgoing>
    </bpmn:serviceTask>
    
    <!-- Service Task: Vollständigkeit prüfen -->
    <bpmn:serviceTask id="Task_Vollstaendigkeit_pruefen" name="Vollstaendigkeit pruefen">
      <bpmn:incoming>Flow_Entgegennehmen_Vollstaendigkeit</bpmn:incoming>
      <bpmn:outgoing>Flow_Vollstaendigkeit_Abrufen</bpmn:outgoing>
    </bpmn:serviceTask>
    
    <!-- Service Task: Daten abrufen -->
    <bpmn:serviceTask id="Task_Daten_abrufen" name="Daten abrufen">
      <bpmn:incoming>Flow_Vollstaendigkeit_Abrufen</bpmn:incoming>
      <bpmn:outgoing>Flow_Abrufen_Bereitstellen</bpmn:outgoing>
    </bpmn:serviceTask>
    
    <!-- Service Task: Bescheid bereitstellen -->
    <bpmn:serviceTask id="Task_Bescheid_bereitstellen" name="Bescheid bereitstellen">
      <bpmn:incoming>Flow_Abrufen_Bereitstellen</bpmn:incoming>
      <bpmn:outgoing>Flow_Bereitstellen_Ende</bpmn:outgoing>
    </bpmn:serviceTask>
    
    <!-- End Event -->
    <bpmn:endEvent id="EndEvent_IT" name="System abgeschlossen">
      <bpmn:incoming>Flow_Bereitstellen_Ende</bpmn:incoming>
    </bpmn:endEvent>
    
    <!-- Sequence Flows - IT-Prozess -->
    <bpmn:sequenceFlow id="Flow_IT_Start_Entgegennehmen"
                       sourceRef="StartEvent_IT"
                       targetRef="Task_Antrag_entgegennehmen" />
    
    <bpmn:sequenceFlow id="Flow_Entgegennehmen_Vollstaendigkeit"
                       sourceRef="Task_Antrag_entgegennehmen"
                       targetRef="Task_Vollstaendigkeit_pruefen" />
    
    <bpmn:sequenceFlow id="Flow_Vollstaendigkeit_Abrufen"
                       sourceRef="Task_Vollstaendigkeit_pruefen"
                       targetRef="Task_Daten_abrufen" />
    
    <bpmn:sequenceFlow id="Flow_Abrufen_Bereitstellen"
                       sourceRef="Task_Daten_abrufen"
                       targetRef="Task_Bescheid_bereitstellen" />
    
    <bpmn:sequenceFlow id="Flow_Bereitstellen_Ende"
                       sourceRef="Task_Bescheid_bereitstellen"
                       targetRef="EndEvent_IT" />
  </bpmn:process>

  <!-- =====================================================
       BEHÖRDEN PROZESS
       ===================================================== -->
  <bpmn:process id="Process_Behoerde"
                name="BuT_Leistungen_Behoerde"
                isExecutable="true">
    
    <!-- Start Event -->
    <bpmn:startEvent id="StartEvent_Behoerde" name="Vorgang eingegangen">
      <bpmn:outgoing>Flow_Start_Daten</bpmn:outgoing>
    </bpmn:startEvent>
    
    <!-- Intermediate Catch Event: Daten erhalten -->
    <bpmn:intermediateCatchEvent id="Event_Daten_erhalten" name="Daten erhalten">
      <bpmn:incoming>Flow_Start_Daten</bpmn:incoming>
      <bpmn:outgoing>Flow_Daten_Anspruch</bpmn:outgoing>
      <bpmn:messageEventDefinition id="MessageEventDef_Daten" messageRef="Message_Daten" />
    </bpmn:intermediateCatchEvent>
    
    <!-- User Task: Anspruch prüfen -->
    <bpmn:userTask id="Task_Anspruch_pruefen" name="Anspruch pruefen">
      <bpmn:incoming>Flow_Daten_Anspruch</bpmn:incoming>
      <bpmn:outgoing>Flow_Anspruch_Bewilligung</bpmn:outgoing>
    </bpmn:userTask>
    
    <!-- User Task: Bewilligung festlegen -->
    <bpmn:userTask id="Task_Bewilligung_festlegen" name="Bewilligung festlegen">
      <bpmn:incoming>Flow_Anspruch_Bewilligung</bpmn:incoming>
      <bpmn:outgoing>Flow_Bewilligung_Bescheid</bpmn:outgoing>
    </bpmn:userTask>
    
    <!-- User Task: Bescheid erstellen -->
    <bpmn:userTask id="Task_Bescheid_erstellen" name="Bescheid erstellen">
      <bpmn:incoming>Flow_Bewilligung_Bescheid</bpmn:incoming>
      <bpmn:outgoing>Flow_Bescheid_Ende</bpmn:outgoing>
    </bpmn:userTask>
    
    <!-- End Event -->
    <bpmn:endEvent id="EndEvent_Behoerde" name="Leistung bewilligt">
      <bpmn:incoming>Flow_Bescheid_Ende</bpmn:incoming>
    </bpmn:endEvent>
    
    <!-- Sequence Flows - Behörde -->
    <bpmn:sequenceFlow id="Flow_Start_Daten"
                       sourceRef="StartEvent_Behoerde"
                       targetRef="Event_Daten_erhalten" />
    
    <bpmn:sequenceFlow id="Flow_Daten_Anspruch"
                       sourceRef="Event_Daten_erhalten"
                       targetRef="Task_Anspruch_pruefen" />
    
    <bpmn:sequenceFlow id="Flow_Anspruch_Bewilligung"
                       sourceRef="Task_Anspruch_pruefen"
                       targetRef="Task_Bewilligung_festlegen" />
    
    <bpmn:sequenceFlow id="Flow_Bewilligung_Bescheid"
                       sourceRef="Task_Bewilligung_festlegen"
                       targetRef="Task_Bescheid_erstellen" />
    
    <bpmn:sequenceFlow id="Flow_Bescheid_Ende"
                       sourceRef="Task_Bescheid_erstellen"
                       targetRef="EndEvent_Behoerde" />
  </bpmn:process>

  <!-- =====================================================
       MESSAGE DEFINITIONEN
       ===================================================== -->
  <bpmn:message id="Message_Rueckmeldung" name="Rueckmeldung" />
  <bpmn:message id="Message_Bescheid" name="Bescheid" />
  <bpmn:message id="Message_Daten" name="Vorgangsdaten" />

