<?xml version="1.0" encoding="UTF-8"?>
<bpmn:definitions
  xmlns:bpmn="http://www.omg.org/spec/BPMN/20100524/MODEL"
  xmlns:bpmndi="http://www.omg.org/spec/BPMN/20100524/DI"
  xmlns:dc="http://www.omg.org/spec/DD/20100524/DC"
  xmlns:di="http://www.omg.org/spec/DD/20100524/DI"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xmlns:camunda="http://camunda.org/schema/1.0/bpmn"
  targetNamespace="http://example.com/bpmn"
  exporter="GitHub-Copilot-GovModeler"
  exporterVersion="2.0"
  id="Definitions_Stammdatenaenderungen_KMPersonal">

  <!-- =====================================================
       COLLABORATION - Stammdatenaenderungen ausfuehren (KM.Personal)
       Pflege von Persoenlichen Daten, Betrieblichen Funktionen,
       Kostenstellen, Adressen, Bankverbindung etc.
       ===================================================== -->
  <bpmn:collaboration id="Collab_Stammdatenaenderungen_KMPersonal">
    
    <!-- Participant: Buerger (Mitarbeitende/Antragstellende) -->
    <bpmn:participant id="Participant_Buerger"
                      name="Buerger"
                      processRef="Process_Buerger" />
    
    <!-- Participant: IT (KM.Personal) -->
    <bpmn:participant id="Participant_IT"
                      name="IT (KM.Personal)"
                      processRef="Process_IT" />
    
    <!-- Participant: Behoerde/Organisation (Personalstelle) -->
    <bpmn:participant id="Participant_Behoerde"
                      name="Personalstelle"
                      processRef="Process_Behoerde" />
    
    <!-- Message Flows zwischen Participanten -->
    <bpmn:messageFlow id="MsgFlow_Antrag_an_IT"
                      name="Aenderungsantrag"
                      sourceRef="Task_Antrag_absenden"
                      targetRef="StartEvent_IT" />
    
    <bpmn:messageFlow id="MsgFlow_Zuweisung_an_Behoerde"
                      name="Vorgang zuweisen"
                      sourceRef="Task_Antrag_zuweisen"
                      targetRef="StartEvent_Behoerde" />
    
    <bpmn:messageFlow id="MsgFlow_Freigabe_an_IT"
                      name="Freigabe"
                      sourceRef="Task_Stammdaten_aktualisieren"
                      targetRef="Event_Freigabe_erhalten" />
    
    <bpmn:messageFlow id="MsgFlow_Bestaetigung_an_Buerger"
                      name="Bestaetigung"
                      sourceRef="Task_Stammdaten_speichern"
                      targetRef="Event_Bestaetigung_erhalten" />
  </bpmn:collaboration>

  <!-- =====================================================
       BUERGER PROZESS - Antragstellung Stammdatenaenderung
       ===================================================== -->
  <bpmn:process id="Process_Buerger"
                name="Stammdatenaenderungen_Buerger"
                isExecutable="true">
    
    <!-- Start Event -->
    <bpmn:startEvent id="StartEvent_Buerger" name="Aenderungsbedarf gemeldet">
      <bpmn:outgoing>Flow_B_Start_Erfassen</bpmn:outgoing>
    </bpmn:startEvent>
    
    <!-- User Task: Aenderungsantrag erfassen -->
    <bpmn:userTask id="Task_Antrag_erfassen" name="Aenderungsantrag erfassen">
      <bpmn:incoming>Flow_B_Start_Erfassen</bpmn:incoming>
      <bpmn:outgoing>Flow_B_Erfassen_Absenden</bpmn:outgoing>
    </bpmn:userTask>
    
    <!-- User Task: Aenderungsantrag absenden -->
    <bpmn:userTask id="Task_Antrag_absenden" name="Aenderungsantrag absenden">
      <bpmn:incoming>Flow_B_Erfassen_Absenden</bpmn:incoming>
      <bpmn:outgoing>Flow_B_Absenden_Warten</bpmn:outgoing>
    </bpmn:userTask>
    
    <!-- Intermediate Catch Event: Bestaetigung erhalten -->
    <bpmn:intermediateCatchEvent id="Event_Bestaetigung_erhalten" name="Bestaetigung erhalten">
      <bpmn:incoming>Flow_B_Absenden_Warten</bpmn:incoming>
      <bpmn:outgoing>Flow_B_Bestaetigung_Ende</bpmn:outgoing>
      <bpmn:messageEventDefinition id="MsgEventDef_Bestaetigung" />
    </bpmn:intermediateCatchEvent>
    
    <!-- End Event -->
    <bpmn:endEvent id="EndEvent_Buerger" name="Stammdaten aktualisiert">
      <bpmn:incoming>Flow_B_Bestaetigung_Ende</bpmn:incoming>
    </bpmn:endEvent>
    
    <!-- Sequence Flows - Buerger -->
    <bpmn:sequenceFlow id="Flow_B_Start_Erfassen"
                       sourceRef="StartEvent_Buerger"
                       targetRef="Task_Antrag_erfassen" />
    
    <bpmn:sequenceFlow id="Flow_B_Erfassen_Absenden"
                       sourceRef="Task_Antrag_erfassen"
                       targetRef="Task_Antrag_absenden" />
    
    <bpmn:sequenceFlow id="Flow_B_Absenden_Warten"
                       sourceRef="Task_Antrag_absenden"
                       targetRef="Event_Bestaetigung_erhalten" />
    
    <bpmn:sequenceFlow id="Flow_B_Bestaetigung_Ende"
                       sourceRef="Event_Bestaetigung_erhalten"
                       targetRef="EndEvent_Buerger" />
  </bpmn:process>

  <!-- =====================================================
       IT PROZESS - KM.Personal Datenverarbeitung
       ===================================================== -->
  <bpmn:process id="Process_IT"
                name="Stammdatenaenderungen_IT"
                isExecutable="true">
    
    <!-- Start Event -->
    <bpmn:startEvent id="StartEvent_IT" name="Aenderungsantrag eingegangen">
      <bpmn:outgoing>Flow_IT_Start_Validieren</bpmn:outgoing>
      <bpmn:messageEventDefinition id="MsgEventDef_IT_Start" />
    </bpmn:startEvent>
    
    <!-- Service Task: Aenderungsdaten validieren -->
    <bpmn:serviceTask id="Task_Daten_validieren" name="Aenderungsdaten validieren">
      <bpmn:incoming>Flow_IT_Start_Validieren</bpmn:incoming>
      <bpmn:outgoing>Flow_IT_Validieren_Zuweisen</bpmn:outgoing>
    </bpmn:serviceTask>
    
    <!-- Service Task: Antrag zur Bearbeitung zuweisen -->
    <bpmn:serviceTask id="Task_Antrag_zuweisen" name="Antrag zur Bearbeitung zuweisen">
      <bpmn:incoming>Flow_IT_Validieren_Zuweisen</bpmn:incoming>
      <bpmn:outgoing>Flow_IT_Zuweisen_Warten</bpmn:outgoing>
    </bpmn:serviceTask>
    
    <!-- Intermediate Catch Event: Freigabe erhalten -->
    <bpmn:intermediateCatchEvent id="Event_Freigabe_erhalten" name="Freigabe erhalten">
      <bpmn:incoming>Flow_IT_Zuweisen_Warten</bpmn:incoming>
      <bpmn:outgoing>Flow_IT_Freigabe_Speichern</bpmn:outgoing>
      <bpmn:messageEventDefinition id="MsgEventDef_Freigabe" />
    </bpmn:intermediateCatchEvent>
    
    <!-- Service Task: Stammdaten speichern und bestaetigen -->
    <bpmn:serviceTask id="Task_Stammdaten_speichern" name="Stammdaten speichern und bestaetigen">
      <bpmn:incoming>Flow_IT_Freigabe_Speichern</bpmn:incoming>
      <bpmn:outgoing>Flow_IT_Speichern_Ende</bpmn:outgoing>
    </bpmn:serviceTask>
    
    <!-- End Event -->
    <bpmn:endEvent id="EndEvent_IT" name="Daten persistiert">
      <bpmn:incoming>Flow_IT_Speichern_Ende</bpmn:incoming>
    </bpmn:endEvent>
    
    <!-- Sequence Flows - IT -->
    <bpmn:sequenceFlow id="Flow_IT_Start_Validieren"
                       sourceRef="StartEvent_IT"
                       targetRef="Task_Daten_validieren" />
    
    <bpmn:sequenceFlow id="Flow_IT_Validieren_Zuweisen"
                       sourceRef="Task_Daten_validieren"
                       targetRef="Task_Antrag_zuweisen" />
    
    <bpmn:sequenceFlow id="Flow_IT_Zuweisen_Warten"
                       sourceRef="Task_Antrag_zuweisen"
                       targetRef="Event_Freigabe_erhalten" />
    
    <bpmn:sequenceFlow id="Flow_IT_Freigabe_Speichern"
                       sourceRef="Event_Freigabe_erhalten"
                       targetRef="Task_Stammdaten_speichern" />
    
    <bpmn:sequenceFlow id="Flow_IT_Speichern_Ende"
                       sourceRef="Task_Stammdaten_speichern"
                       targetRef="EndEvent_IT" />
  </bpmn:process>

  <!-- =====================================================
       BEHOERDE PROZESS - Personalstelle Bearbeitung
       ===================================================== -->
  <bpmn:process id="Process_Behoerde"
                name="Stammdatenaenderungen_Behoerde"
                isExecutable="true">
    
    <!-- Start Event -->
    <bpmn:startEvent id="StartEvent_Behoerde" name="Vorgang eingegangen">
      <bpmn:outgoing>Flow_Beh_Start_Pruefen</bpmn:outgoing>
      <bpmn:messageEventDefinition id="MsgEventDef_Beh_Start" />
    </bpmn:startEvent>
    
    <!-- User Task: Aenderungsantrag pruefen -->
    <bpmn:userTask id="Task_Antrag_pruefen" name="Aenderungsantrag pruefen">
      <bpmn:incoming>Flow_Beh_Start_Pruefen</bpmn:incoming>
      <bpmn:outgoing>Flow_Beh_Pruefen_Aktualisieren</bpmn:outgoing>
    </bpmn:userTask>
    
    <!-- User Task: Stammdaten aktualisieren (Freigabe) -->
    <bpmn:userTask id="Task_Stammdaten_aktualisieren" name="Stammdaten aktualisieren">
      <bpmn:incoming>Flow_Beh_Pruefen_Aktualisieren</bpmn:incoming>
      <bpmn:outgoing>Flow_Beh_Aktualisieren_Abschluss</bpmn:outgoing>
    </bpmn:userTask>
    
    <!-- User Task: Abschluss bestaetigen -->
    <bpmn:userTask id="Task_Abschluss_bestaetigen" name="Abschluss bestaetigen">
      <bpmn:incoming>Flow_Beh_Aktualisieren_Abschluss</bpmn:incoming>
      <bpmn:outgoing>Flow_Beh_Abschluss_Ende</bpmn:outgoing>
    </bpmn:userTask>
    
    <!-- End Event -->
    <bpmn:endEvent id="EndEvent_Behoerde" name="Vorgang abgeschlossen">
      <bpmn:incoming>Flow_Beh_Abschluss_Ende</bpmn:incoming>
    </bpmn:endEvent>
    
    <!-- Sequence Flows - Behoerde -->
    <bpmn:sequenceFlow id="Flow_Beh_Start_Pruefen"
                       sourceRef="StartEvent_Behoerde"
                       targetRef="Task_Antrag_pruefen" />
    
    <bpmn:sequenceFlow id="Flow_Beh_Pruefen_Aktualisieren"
                       sourceRef="Task_Antrag_pruefen"
                       targetRef="Task_Stammdaten_aktualisieren" />
    
    <bpmn:sequenceFlow id="Flow_Beh_Aktualisieren_Abschluss"
                       sourceRef="Task_Stammdaten_aktualisieren"
                       targetRef="Task_Abschluss_bestaetigen" />
    
    <bpmn:sequenceFlow id="Flow_Beh_Abschluss_Ende"
                       sourceRef="Task_Abschluss_bestaetigen"
                       targetRef="EndEvent_Behoerde" />
  </bpmn:process>

  <!-- =====================================================
       BPMN DIAGRAM INTERCHANGE (BPMN-DI)
       ===================================================== -->
  <bpmndi:BPMNDiagram id="BPMNDiagram_Stammdatenaenderungen_KMPersonal">
    <bpmndi:BPMNPlane id="BPMNPlane_Stammdatenaenderungen_KMPersonal" bpmnElement="Collab_Stammdatenaenderungen_KMPersonal">
      
