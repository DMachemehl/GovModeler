        <bpmndi:BPMNLabel>
          <dc:Bounds x="343" y="615" width="83" height="14" />
        </bpmndi:BPMNLabel>
      </bpmndi:BPMNEdge>
      
      <!-- Behoerde -> IT: Freigabe -->
      <bpmndi:BPMNEdge id="MsgFlow_Freigabe_an_IT_di" bpmnElement="MsgFlow_Freigabe_an_IT">
        <di:waypoint x="520" y="745" />
        <di:waypoint x="520" y="635" />
        <di:waypoint x="650" y="635" />
        <di:waypoint x="650" y="513" />
        <bpmndi:BPMNLabel>
          <dc:Bounds x="565" y="615" width="42" height="14" />
        </bpmndi:BPMNLabel>
      </bpmndi:BPMNEdge>
      
      <!-- IT -> Buerger: Bestaetigung -->
      <bpmndi:BPMNEdge id="MsgFlow_Bestaetigung_an_Buerger_di" bpmnElement="MsgFlow_Bestaetigung_an_Buerger">
        <di:waypoint x="780" y="455" />
        <di:waypoint x="780" y="345" />
        <di:waypoint x="650" y="345" />
