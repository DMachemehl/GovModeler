      <bpmndi:BPMNShape id="StartEvent_Buerger_di" bpmnElement="StartEvent_Buerger">
        <dc:Bounds x="222" y="182" width="36" height="36" />
        <bpmndi:BPMNLabel>
          <dc:Bounds x="196" y="225" width="88" height="27" />
        </bpmndi:BPMNLabel>
      </bpmndi:BPMNShape>
      
      <!-- Buerger: Task 1 - Aenderungsantrag erfassen -->
      <bpmndi:BPMNShape id="Task_Antrag_erfassen_di" bpmnElement="Task_Antrag_erfassen">
        <dc:Bounds x="310" y="160" width="100" height="80" />
        <bpmndi:BPMNLabel />
      </bpmndi:BPMNShape>
      
      <!-- Buerger: Task 2 - Aenderungsantrag absenden -->
      <bpmndi:BPMNShape id="Task_Antrag_absenden_di" bpmnElement="Task_Antrag_absenden">
        <dc:Bounds x="470" y="160" width="100" height="80" />
        <bpmndi:BPMNLabel />
      </bpmndi:BPMNShape>
      
      <!-- Buerger: Intermediate Catch Event - Bestaetigung erhalten -->
      <bpmndi:BPMNShape id="Event_Bestaetigung_erhalten_di" bpmnElement="Event_Bestaetigung_erhalten">
        <dc:Bounds x="632" y="182" width="36" height="36" />
        <bpmndi:BPMNLabel>
          <dc:Bounds x="614" y="225" width="73" height="27" />
        </bpmndi:BPMNLabel>
      </bpmndi:BPMNShape>
      
      <!-- Buerger: End Event -->
      <bpmndi:BPMNShape id="EndEvent_Buerger_di" bpmnElement="EndEvent_Buerger">
        <dc:Bounds x="732" y="182" width="36" height="36" />
        <bpmndi:BPMNLabel>
          <dc:Bounds x="709" y="225" width="82" height="27" />
        </bpmndi:BPMNLabel>
      </bpmndi:BPMNShape>
      
      <!-- ===== POOL: IT (KM.Personal) ===== -->
      <bpmndi:BPMNShape id="Participant_IT_di" bpmnElement="Participant_IT" isHorizontal="true">
        <dc:Bounds x="160" y="370" width="1200" height="250" />
        <bpmndi:BPMNLabel />
      </bpmndi:BPMNShape>
      
      <!-- IT: Start Event -->
      <bpmndi:BPMNShape id="StartEvent_IT_di" bpmnElement="StartEvent_IT">
        <dc:Bounds x="222" y="477" width="36" height="36" />
        <bpmndi:BPMNLabel>
          <dc:Bounds x="196" y="520" width="88" height="27" />
        </bpmndi:BPMNLabel>
      </bpmndi:BPMNShape>
      
      <!-- IT: Task 1 - Aenderungsdaten validieren -->
      <bpmndi:BPMNShape id="Task_Daten_validieren_di" bpmnElement="Task_Daten_validieren">
        <dc:Bounds x="310" y="455" width="100" height="80" />
        <bpmndi:BPMNLabel />
      </bpmndi:BPMNShape>
      
      <!-- IT: Task 2 - Antrag zuweisen -->
      <bpmndi:BPMNShape id="Task_Antrag_zuweisen_di" bpmnElement="Task_Antrag_zuweisen">
        <dc:Bounds x="470" y="455" width="100" height="80" />
        <bpmndi:BPMNLabel />
      </bpmndi:BPMNShape>
      
      <!-- IT: Intermediate Catch Event - Freigabe erhalten -->
      <bpmndi:BPMNShape id="Event_Freigabe_erhalten_di" bpmnElement="Event_Freigabe_erhalten">
        <dc:Bounds x="632" y="477" width="36" height="36" />
        <bpmndi:BPMNLabel>
          <dc:Bounds x="614" y="520" width="73" height="27" />
        </bpmndi:BPMNLabel>
      </bpmndi:BPMNShape>
      
      <!-- IT: Task 3 - Stammdaten speichern -->
      <bpmndi:BPMNShape id="Task_Stammdaten_speichern_di" bpmnElement="Task_Stammdaten_speichern">
        <dc:Bounds x="730" y="455" width="100" height="80" />
        <bpmndi:BPMNLabel />
      </bpmndi:BPMNShape>
      
      <!-- IT: End Event -->
      <bpmndi:BPMNShape id="EndEvent_IT_di" bpmnElement="EndEvent_IT">
        <dc:Bounds x="892" y="477" width="36" height="36" />
        <bpmndi:BPMNLabel>
          <dc:Bounds x="872" y="520" width="77" height="14" />
        </bpmndi:BPMNLabel>
      </bpmndi:BPMNShape>
      
      <!-- ===== POOL: BEHOERDE (Personalstelle) ===== -->
      <bpmndi:BPMNShape id="Participant_Behoerde_di" bpmnElement="Participant_Behoerde" isHorizontal="true">
        <dc:Bounds x="160" y="660" width="1200" height="250" />
        <bpmndi:BPMNLabel />
      </bpmndi:BPMNShape>
      
      <!-- Behoerde: Start Event -->
      <bpmndi:BPMNShape id="StartEvent_Behoerde_di" bpmnElement="StartEvent_Behoerde">
        <dc:Bounds x="222" y="767" width="36" height="36" />
        <bpmndi:BPMNLabel>
          <dc:Bounds x="199" y="810" width="82" height="27" />
        </bpmndi:BPMNLabel>
      </bpmndi:BPMNShape>
      
      <!-- Behoerde: Task 1 - Aenderungsantrag pruefen -->
      <bpmndi:BPMNShape id="Task_Antrag_pruefen_di" bpmnElement="Task_Antrag_pruefen">
        <dc:Bounds x="310" y="745" width="100" height="80" />
        <bpmndi:BPMNLabel />
      </bpmndi:BPMNShape>
      
      <!-- Behoerde: Task 2 - Stammdaten aktualisieren -->
      <bpmndi:BPMNShape id="Task_Stammdaten_aktualisieren_di" bpmnElement="Task_Stammdaten_aktualisieren">
        <dc:Bounds x="470" y="745" width="100" height="80" />
        <bpmndi:BPMNLabel />
      </bpmndi:BPMNShape>
      
      <!-- Behoerde: Task 3 - Abschluss bestaetigen -->
      <bpmndi:BPMNShape id="Task_Abschluss_bestaetigen_di" bpmnElement="Task_Abschluss_bestaetigen">
        <dc:Bounds x="630" y="745" width="100" height="80" />
        <bpmndi:BPMNLabel />
      </bpmndi:BPMNShape>
      
      <!-- Behoerde: End Event -->
      <bpmndi:BPMNShape id="EndEvent_Behoerde_di" bpmnElement="EndEvent_Behoerde">
        <dc:Bounds x="792" y="767" width="36" height="36" />
        <bpmndi:BPMNLabel>
          <dc:Bounds x="768" y="810" width="85" height="27" />
        </bpmndi:BPMNLabel>
      </bpmndi:BPMNShape>
      
      <!-- ===== SEQUENCE FLOWS: BUERGER ===== -->
      <bpmndi:BPMNEdge id="Flow_B_Start_Erfassen_di" bpmnElement="Flow_B_Start_Erfassen">
        <di:waypoint x="258" y="200" />
        <di:waypoint x="310" y="200" />
      </bpmndi:BPMNEdge>
      
      <bpmndi:BPMNEdge id="Flow_B_Erfassen_Absenden_di" bpmnElement="Flow_B_Erfassen_Absenden">
        <di:waypoint x="410" y="200" />
        <di:waypoint x="470" y="200" />
      </bpmndi:BPMNEdge>
      
      <bpmndi:BPMNEdge id="Flow_B_Absenden_Warten_di" bpmnElement="Flow_B_Absenden_Warten">
        <di:waypoint x="570" y="200" />
        <di:waypoint x="632" y="200" />
      </bpmndi:BPMNEdge>
      
      <bpmndi:BPMNEdge id="Flow_B_Bestaetigung_Ende_di" bpmnElement="Flow_B_Bestaetigung_Ende">
        <di:waypoint x="668" y="200" />
        <di:waypoint x="732" y="200" />
      </bpmndi:BPMNEdge>
      
      <!-- ===== SEQUENCE FLOWS: IT ===== -->
      <bpmndi:BPMNEdge id="Flow_IT_Start_Validieren_di" bpmnElement="Flow_IT_Start_Validieren">
        <di:waypoint x="258" y="495" />
        <di:waypoint x="310" y="495" />
      </bpmndi:BPMNEdge>
      
      <bpmndi:BPMNEdge id="Flow_IT_Validieren_Zuweisen_di" bpmnElement="Flow_IT_Validieren_Zuweisen">
        <di:waypoint x="410" y="495" />
        <di:waypoint x="470" y="495" />
      </bpmndi:BPMNEdge>
      
      <bpmndi:BPMNEdge id="Flow_IT_Zuweisen_Warten_di" bpmnElement="Flow_IT_Zuweisen_Warten">
        <di:waypoint x="570" y="495" />
        <di:waypoint x="632" y="495" />
      </bpmndi:BPMNEdge>
      
      <bpmndi:BPMNEdge id="Flow_IT_Freigabe_Speichern_di" bpmnElement="Flow_IT_Freigabe_Speichern">
        <di:waypoint x="668" y="495" />
        <di:waypoint x="730" y="495" />
      </bpmndi:BPMNEdge>
      
      <bpmndi:BPMNEdge id="Flow_IT_Speichern_Ende_di" bpmnElement="Flow_IT_Speichern_Ende">
        <di:waypoint x="830" y="495" />
        <di:waypoint x="892" y="495" />
      </bpmndi:BPMNEdge>
      
      <!-- ===== SEQUENCE FLOWS: BEHOERDE ===== -->
      <bpmndi:BPMNEdge id="Flow_Beh_Start_Pruefen_di" bpmnElement="Flow_Beh_Start_Pruefen">
        <di:waypoint x="258" y="785" />
        <di:waypoint x="310" y="785" />
      </bpmndi:BPMNEdge>
      
      <bpmndi:BPMNEdge id="Flow_Beh_Pruefen_Aktualisieren_di" bpmnElement="Flow_Beh_Pruefen_Aktualisieren">
        <di:waypoint x="410" y="785" />
        <di:waypoint x="470" y="785" />
      </bpmndi:BPMNEdge>
      
      <bpmndi:BPMNEdge id="Flow_Beh_Aktualisieren_Abschluss_di" bpmnElement="Flow_Beh_Aktualisieren_Abschluss">
        <di:waypoint x="570" y="785" />
        <di:waypoint x="630" y="785" />
      </bpmndi:BPMNEdge>
      
      <bpmndi:BPMNEdge id="Flow_Beh_Abschluss_Ende_di" bpmnElement="Flow_Beh_Abschluss_Ende">
        <di:waypoint x="730" y="785" />
        <di:waypoint x="792" y="785" />
      </bpmndi:BPMNEdge>
      
      <!-- ===== MESSAGE FLOWS ===== -->
      <!-- Buerger -> IT: Aenderungsantrag -->
      <bpmndi:BPMNEdge id="MsgFlow_Antrag_an_IT_di" bpmnElement="MsgFlow_Antrag_an_IT">
        <di:waypoint x="520" y="240" />
        <di:waypoint x="520" y="345" />
        <di:waypoint x="240" y="345" />
        <di:waypoint x="240" y="477" />
        <bpmndi:BPMNLabel>
          <dc:Bounds x="342" y="325" width="85" height="14" />
        </bpmndi:BPMNLabel>
      </bpmndi:BPMNEdge>
      
      <!-- IT -> Behoerde: Vorgang zuweisen -->
      <bpmndi:BPMNEdge id="MsgFlow_Zuweisung_an_Behoerde_di" bpmnElement="MsgFlow_Zuweisung_an_Behoerde">
        <di:waypoint x="520" y="535" />
        <di:waypoint x="520" y="635" />
