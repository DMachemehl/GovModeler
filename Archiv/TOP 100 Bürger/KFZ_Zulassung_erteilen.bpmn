<?xml version="1.0" encoding="UTF-8"?>
<bpmn:definitions
  xmlns:bpmn="http://www.omg.org/spec/BPMN/20100524/MODEL"
  xmlns:bpmndi="http://www.omg.org/spec/BPMN/20100524/DI"
  xmlns:dc="http://www.omg.org/spec/DD/20100524/DC"
  xmlns:di="http://www.omg.org/spec/DD/20100524/DI"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xmlns:camunda="http://camunda.org/schema/1.0/bpmn"
  targetNamespace="http://example.com/bpmn"
  exporter="GitHub-Copilot-GovModeler"
  exporterVersion="2.0">

  <!-- =====================================================
       COLLABORATION - KFZ-Zulassung
       ===================================================== -->
  <bpmn:collaboration id="Collab_KFZ_Zulassung">
    
    <!-- Participant: Bürger -->
    <bpmn:participant id="Participant_Buerger"
                      name="Buerger"
                      processRef="Process_Buerger" />
    
    <!-- Participant: IT -->
    <bpmn:participant id="Participant_IT"
                      name="IT"
                      processRef="Process_IT" />
    
    <!-- Participant: Zulassungsbehörde -->
    <bpmn:participant id="Participant_Behoerde"
                      name="Zulassungsbehoerde"
                      processRef="Process_Behoerde" />
    
    <!-- Message Flows -->
    <bpmn:messageFlow id="MsgFlow_Antrag"
                      name="Zulassungsantrag"
                      sourceRef="Task_Antrag_einreichen"
                      targetRef="StartEvent_Antrag_eingegangen" />
    
    <bpmn:messageFlow id="MsgFlow_Pruefergebnis"
                      name="Pruefergebnis"
                      sourceRef="Task_Fahrzeugdaten_erfassen"
                      targetRef="Task_Unterlagen_pruefen" />
    
    <bpmn:messageFlow id="MsgFlow_Zulassung"
                      name="Zulassungsbescheinigung"
                      sourceRef="Task_Zulassungsbescheinigung_ausstellen"
                      targetRef="Event_Zulassung_erhalten" />
  </bpmn:collaboration>

  <!-- =====================================================
       BÜRGER PROZESS
       ===================================================== -->
  <bpmn:process id="Process_Buerger"
                name="KFZ_Zulassung_Buerger"
                isExecutable="true">
    
    <!-- Start Event -->
    <bpmn:startEvent id="StartEvent_Buerger" name="Zulassungsantrag eingegangen">
      <bpmn:outgoing>Flow_Start_Zusammenstellen</bpmn:outgoing>
    </bpmn:startEvent>
    
    <!-- User Task: Antrag zusammenstellen -->
    <bpmn:userTask id="Task_Antrag_zusammenstellen" name="Antrag zusammenstellen">
      <bpmn:incoming>Flow_Start_Zusammenstellen</bpmn:incoming>
      <bpmn:outgoing>Flow_Zusammenstellen_Einreichen</bpmn:outgoing>
    </bpmn:userTask>
    
    <!-- User Task: Antrag einreichen -->
    <bpmn:userTask id="Task_Antrag_einreichen" name="Antrag einreichen">
      <bpmn:incoming>Flow_Zusammenstellen_Einreichen</bpmn:incoming>
      <bpmn:outgoing>Flow_Einreichen_Warten</bpmn:outgoing>
    </bpmn:userTask>
    
    <!-- Intermediate Catch Event: Zulassung erhalten -->
    <bpmn:intermediateCatchEvent id="Event_Zulassung_erhalten" name="Zulassung erhalten">
      <bpmn:incoming>Flow_Einreichen_Warten</bpmn:incoming>
      <bpmn:outgoing>Flow_Zulassung_Ende</bpmn:outgoing>
      <bpmn:messageEventDefinition id="MessageEventDef_Zulassung" messageRef="Message_Zulassung" />
    </bpmn:intermediateCatchEvent>
    
    <!-- End Event -->
    <bpmn:endEvent id="EndEvent_Buerger" name="KFZ zugelassen">
      <bpmn:incoming>Flow_Zulassung_Ende</bpmn:incoming>
    </bpmn:endEvent>
    
    <!-- Sequence Flows - Bürger -->
    <bpmn:sequenceFlow id="Flow_Start_Zusammenstellen"
                       sourceRef="StartEvent_Buerger"
                       targetRef="Task_Antrag_zusammenstellen" />
    
    <bpmn:sequenceFlow id="Flow_Zusammenstellen_Einreichen"
                       sourceRef="Task_Antrag_zusammenstellen"
                       targetRef="Task_Antrag_einreichen" />
    
    <bpmn:sequenceFlow id="Flow_Einreichen_Warten"
                       sourceRef="Task_Antrag_einreichen"
                       targetRef="Event_Zulassung_erhalten" />
    
    <bpmn:sequenceFlow id="Flow_Zulassung_Ende"
                       sourceRef="Event_Zulassung_erhalten"
                       targetRef="EndEvent_Buerger" />
  </bpmn:process>

  <!-- =====================================================
       IT PROZESS
       ===================================================== -->
  <bpmn:process id="Process_IT"
                name="KFZ_Zulassung_IT"
                isExecutable="true">
    
    <!-- Start Event -->
    <bpmn:startEvent id="StartEvent_IT" name="Pruefauftrag eingegangen">
      <bpmn:outgoing>Flow_IT_Start_Vollstaendigkeit</bpmn:outgoing>
    </bpmn:startEvent>
    
    <!-- Service Task: Vollständigkeit prüfen -->
    <bpmn:serviceTask id="Task_Vollstaendigkeit_pruefen" name="Vollstaendigkeit pruefen">
      <bpmn:incoming>Flow_IT_Start_Vollstaendigkeit</bpmn:incoming>
      <bpmn:outgoing>Flow_Vollstaendigkeit_Versicherung</bpmn:outgoing>
    </bpmn:serviceTask>
    
    <!-- Service Task: Versicherungsschutz prüfen -->
    <bpmn:serviceTask id="Task_Versicherungsschutz_pruefen" name="Versicherungsschutz pruefen">
      <bpmn:incoming>Flow_Vollstaendigkeit_Versicherung</bpmn:incoming>
      <bpmn:outgoing>Flow_Versicherung_Erfassen</bpmn:outgoing>
    </bpmn:serviceTask>
    
    <!-- Service Task: Fahrzeugdaten erfassen -->
    <bpmn:serviceTask id="Task_Fahrzeugdaten_erfassen" name="Fahrzeugdaten erfassen">
      <bpmn:incoming>Flow_Versicherung_Erfassen</bpmn:incoming>
      <bpmn:outgoing>Flow_Erfassen_Ende</bpmn:outgoing>
    </bpmn:serviceTask>
    
    <!-- End Event -->
    <bpmn:endEvent id="EndEvent_IT" name="Pruefung abgeschlossen">
      <bpmn:incoming>Flow_Erfassen_Ende</bpmn:incoming>
    </bpmn:endEvent>
    
    <!-- Sequence Flows - IT -->
    <bpmn:sequenceFlow id="Flow_IT_Start_Vollstaendigkeit"
                       sourceRef="StartEvent_IT"
                       targetRef="Task_Vollstaendigkeit_pruefen" />
    
    <bpmn:sequenceFlow id="Flow_Vollstaendigkeit_Versicherung"
                       sourceRef="Task_Vollstaendigkeit_pruefen"
                       targetRef="Task_Versicherungsschutz_pruefen" />
    
    <bpmn:sequenceFlow id="Flow_Versicherung_Erfassen"
                       sourceRef="Task_Versicherungsschutz_pruefen"
                       targetRef="Task_Fahrzeugdaten_erfassen" />
    
    <bpmn:sequenceFlow id="Flow_Erfassen_Ende"
                       sourceRef="Task_Fahrzeugdaten_erfassen"
                       targetRef="EndEvent_IT" />
  </bpmn:process>

  <!-- =====================================================
       BEHÖRDEN PROZESS
       ===================================================== -->
  <bpmn:process id="Process_Behoerde"
                name="KFZ_Zulassung_Behoerde"
                isExecutable="true">
    
    <!-- Start Event -->
    <bpmn:startEvent id="StartEvent_Antrag_eingegangen" name="Zulassungsantrag eingegangen">
      <bpmn:outgoing>Flow_Start_Unterlagen</bpmn:outgoing>
    </bpmn:startEvent>
    
    <!-- User Task: Unterlagen prüfen -->
    <bpmn:userTask id="Task_Unterlagen_pruefen" name="Unterlagen pruefen">
      <bpmn:incoming>Flow_Start_Unterlagen</bpmn:incoming>
      <bpmn:outgoing>Flow_Unterlagen_Kennzeichen</bpmn:outgoing>
    </bpmn:userTask>
    
    <!-- User Task: Kennzeichen zuteilen -->
    <bpmn:userTask id="Task_Kennzeichen_zuteilen" name="Kennzeichen zuteilen">
      <bpmn:incoming>Flow_Unterlagen_Kennzeichen</bpmn:incoming>
      <bpmn:outgoing>Flow_Kennzeichen_Zulassung</bpmn:outgoing>
    </bpmn:userTask>
    
    <!-- User Task: Zulassungsbescheinigung ausstellen -->
    <bpmn:userTask id="Task_Zulassungsbescheinigung_ausstellen" name="Zulassungsbescheinigung ausstellen">
      <bpmn:incoming>Flow_Kennzeichen_Zulassung</bpmn:incoming>
      <bpmn:outgoing>Flow_Zulassung_Ende</bpmn:outgoing>
    </bpmn:userTask>
    
    <!-- End Event -->
    <bpmn:endEvent id="EndEvent_Behoerde" name="KFZ-Zulassung erteilt">
      <bpmn:incoming>Flow_Zulassung_Ende</bpmn:incoming>
    </bpmn:endEvent>
    
    <!-- Sequence Flows - Behörde -->
    <bpmn:sequenceFlow id="Flow_Start_Unterlagen"
                       sourceRef="StartEvent_Antrag_eingegangen"
                       targetRef="Task_Unterlagen_pruefen" />
    
    <bpmn:sequenceFlow id="Flow_Unterlagen_Kennzeichen"
                       sourceRef="Task_Unterlagen_pruefen"
                       targetRef="Task_Kennzeichen_zuteilen" />
    
    <bpmn:sequenceFlow id="Flow_Kennzeichen_Zulassung"
                       sourceRef="Task_Kennzeichen_zuteilen"
                       targetRef="Task_Zulassungsbescheinigung_ausstellen" />
    
    <bpmn:sequenceFlow id="Flow_Zulassung_Ende"
                       sourceRef="Task_Zulassungsbescheinigung_ausstellen"
                       targetRef="EndEvent_Behoerde" />
  </bpmn:process>

  <!-- =====================================================
       MESSAGE DEFINITIONEN
       ===================================================== -->
  <bpmn:message id="Message_Zulassung" name="Zulassungsbescheinigung" />

  <!-- =====================================================
       BPMN-DI VISUALISIERUNG
       ===================================================== -->
  <bpmndi:BPMNDiagram id="BPMNDiagram_KFZ_Zulassung">
    <bpmndi:BPMNPlane id="BPMNPlane_KFZ_Zulassung" bpmnElement="Collab_KFZ_Zulassung">
      
      <!-- ===============================
           POOL: BÜRGER
           =============================== -->
      <bpmndi:BPMNShape id="Participant_Buerger_di" bpmnElement="Participant_Buerger" isHorizontal="true">
        <dc:Bounds x="160" y="80" width="1200" height="250" />
        <bpmndi:BPMNLabel>
          <dc:Bounds x="170" y="185" width="20" height="50" />
        </bpmndi:BPMNLabel>
      </bpmndi:BPMNShape>
      
      <!-- Bürger Process Elements -->
      <bpmndi:BPMNShape id="StartEvent_Buerger_di" bpmnElement="StartEvent_Buerger">
        <dc:Bounds x="222" y="182" width="36" height="36" />
        <bpmndi:BPMNLabel>
          <dc:Bounds x="200" y="225" width="80" height="27" />
        </bpmndi:BPMNLabel>
      </bpmndi:BPMNShape>
      
      <bpmndi:BPMNShape id="Task_Antrag_zusammenstellen_di" bpmnElement="Task_Antrag_zusammenstellen">
        <dc:Bounds x="310" y="160" width="100" height="80" />
      </bpmndi:BPMNShape>
      
      <bpmndi:BPMNShape id="Task_Antrag_einreichen_di" bpmnElement="Task_Antrag_einreichen">
        <dc:Bounds x="470" y="160" width="100" height="80" />
      </bpmndi:BPMNShape>
      
      <bpmndi:BPMNShape id="Event_Zulassung_erhalten_di" bpmnElement="Event_Zulassung_erhalten">
        <dc:Bounds x="632" y="182" width="36" height="36" />
        <bpmndi:BPMNLabel>
          <dc:Bounds x="610" y="225" width="80" height="27" />
        </bpmndi:BPMNLabel>
      </bpmndi:BPMNShape>
      
      <bpmndi:BPMNShape id="EndEvent_Buerger_di" bpmnElement="EndEvent_Buerger">
        <dc:Bounds x="732" y="182" width="36" height="36" />
        <bpmndi:BPMNLabel>
          <dc:Bounds x="710" y="225" width="80" height="27" />
        </bpmndi:BPMNLabel>
      </bpmndi:BPMNShape>
      
      <!-- Bürger Sequence Flows -->
      <bpmndi:BPMNEdge id="Flow_Start_Zusammenstellen_di" bpmnElement="Flow_Start_Zusammenstellen">
        <di:waypoint x="258" y="200" />
        <di:waypoint x="310" y="200" />
      </bpmndi:BPMNEdge>
      
      <bpmndi:BPMNEdge id="Flow_Zusammenstellen_Einreichen_di" bpmnElement="Flow_Zusammenstellen_Einreichen">
        <di:waypoint x="410" y="200" />
        <di:waypoint x="470" y="200" />
      </bpmndi:BPMNEdge>
      
      <bpmndi:BPMNEdge id="Flow_Einreichen_Warten_di" bpmnElement="Flow_Einreichen_Warten">
        <di:waypoint x="570" y="200" />
        <di:waypoint x="632" y="200" />
      </bpmndi:BPMNEdge>
      
      <bpmndi:BPMNEdge id="Flow_Zulassung_Ende_di" bpmnElement="Flow_Zulassung_Ende">
        <di:waypoint x="668" y="200" />
        <di:waypoint x="732" y="200" />
      </bpmndi:BPMNEdge>
      
      <!-- ===============================
           POOL: IT
           =============================== -->
      <bpmndi:BPMNShape id="Participant_IT_di" bpmnElement="Participant_IT" isHorizontal="true">
        <dc:Bounds x="160" y="370" width="1200" height="250" />
        <bpmndi:BPMNLabel>
          <dc:Bounds x="170" y="475" width="20" height="30" />
        </bpmndi:BPMNLabel>
      </bpmndi:BPMNShape>
      
      <!-- IT Process Elements -->
      <bpmndi:BPMNShape id="StartEvent_IT_di" bpmnElement="StartEvent_IT">
        <dc:Bounds x="222" y="477" width="36" height="36" />
        <bpmndi:BPMNLabel>
          <dc:Bounds x="200" y="520" width="80" height="27" />
        </bpmndi:BPMNLabel>
      </bpmndi:BPMNShape>
      
      <bpmndi:BPMNShape id="Task_Vollstaendigkeit_pruefen_di" bpmnElement="Task_Vollstaendigkeit_pruefen">
        <dc:Bounds x="310" y="455" width="100" height="80" />
      </bpmndi:BPMNShape>
      
      <bpmndi:BPMNShape id="Task_Versicherungsschutz_pruefen_di" bpmnElement="Task_Versicherungsschutz_pruefen">
        <dc:Bounds x="470" y="455" width="100" height="80" />
      </bpmndi:BPMNShape>
      
      <bpmndi:BPMNShape id="Task_Fahrzeugdaten_erfassen_di" bpmnElement="Task_Fahrzeugdaten_erfassen">
        <dc:Bounds x="630" y="455" width="100" height="80" />
      </bpmndi:BPMNShape>
      
      <bpmndi:BPMNShape id="EndEvent_IT_di" bpmnElement="EndEvent_IT">
        <dc:Bounds x="792" y="477" width="36" height="36" />
        <bpmndi:BPMNLabel>
          <dc:Bounds x="770" y="520" width="80" height="27" />
        </bpmndi:BPMNLabel>
      </bpmndi:BPMNShape>
      
      <!-- IT Sequence Flows -->
      <bpmndi:BPMNEdge id="Flow_IT_Start_Vollstaendigkeit_di" bpmnElement="Flow_IT_Start_Vollstaendigkeit">
        <di:waypoint x="258" y="495" />
        <di:waypoint x="310" y="495" />
      </bpmndi:BPMNEdge>
      
      <bpmndi:BPMNEdge id="Flow_Vollstaendigkeit_Versicherung_di" bpmnElement="Flow_Vollstaendigkeit_Versicherung">
        <di:waypoint x="410" y="495" />
        <di:waypoint x="470" y="495" />
      </bpmndi:BPMNEdge>
      
      <bpmndi:BPMNEdge id="Flow_Versicherung_Erfassen_di" bpmnElement="Flow_Versicherung_Erfassen">
        <di:waypoint x="570" y="495" />
        <di:waypoint x="630" y="495" />
      </bpmndi:BPMNEdge>
      
      <bpmndi:BPMNEdge id="Flow_Erfassen_Ende_di" bpmnElement="Flow_Erfassen_Ende">
        <di:waypoint x="730" y="495" />
        <di:waypoint x="792" y="495" />
      </bpmndi:BPMNEdge>
      
      <!-- ===============================
           POOL: BEHÖRDE
           =============================== -->
      <bpmndi:BPMNShape id="Participant_Behoerde_di" bpmnElement="Participant_Behoerde" isHorizontal="true">
        <dc:Bounds x="160" y="660" width="1200" height="250" />
        <bpmndi:BPMNLabel>
          <dc:Bounds x="170" y="735" width="20" height="120" />
        </bpmndi:BPMNLabel>
      </bpmndi:BPMNShape>
      
      <!-- Behörde Process Elements -->
      <bpmndi:BPMNShape id="StartEvent_Antrag_eingegangen_di" bpmnElement="StartEvent_Antrag_eingegangen">
        <dc:Bounds x="222" y="782" width="36" height="36" />
        <bpmndi:BPMNLabel>
          <dc:Bounds x="200" y="825" width="80" height="27" />
        </bpmndi:BPMNLabel>
      </bpmndi:BPMNShape>
      
      <bpmndi:BPMNShape id="Task_Unterlagen_pruefen_di" bpmnElement="Task_Unterlagen_pruefen">
        <dc:Bounds x="310" y="760" width="100" height="80" />
      </bpmndi:BPMNShape>
      
      <bpmndi:BPMNShape id="Task_Kennzeichen_zuteilen_di" bpmnElement="Task_Kennzeichen_zuteilen">
        <dc:Bounds x="470" y="760" width="100" height="80" />
      </bpmndi:BPMNShape>
      
      <bpmndi:BPMNShape id="Task_Zulassungsbescheinigung_ausstellen_di" bpmnElement="Task_Zulassungsbescheinigung_ausstellen">
        <dc:Bounds x="630" y="760" width="100" height="80" />
      </bpmndi:BPMNShape>
      
      <bpmndi:BPMNShape id="EndEvent_Behoerde_di" bpmnElement="EndEvent_Behoerde">
        <dc:Bounds x="792" y="782" width="36" height="36" />
        <bpmndi:BPMNLabel>
          <dc:Bounds x="770" y="825" width="80" height="27" />
        </bpmndi:BPMNLabel>
      </bpmndi:BPMNShape>
      
      <!-- Behörde Sequence Flows -->
      <bpmndi:BPMNEdge id="Flow_Start_Unterlagen_di" bpmnElement="Flow_Start_Unterlagen">
        <di:waypoint x="258" y="800" />
        <di:waypoint x="310" y="800" />
      </bpmndi:BPMNEdge>
      
      <bpmndi:BPMNEdge id="Flow_Unterlagen_Kennzeichen_di" bpmnElement="Flow_Unterlagen_Kennzeichen">
        <di:waypoint x="410" y="800" />
        <di:waypoint x="470" y="800" />
      </bpmndi:BPMNEdge>
      
      <bpmndi:BPMNEdge id="Flow_Kennzeichen_Zulassung_di" bpmnElement="Flow_Kennzeichen_Zulassung">
        <di:waypoint x="570" y="800" />
        <di:waypoint x="630" y="800" />
      </bpmndi:BPMNEdge>
      
      <bpmndi:BPMNEdge id="Flow_Zulassung_Ende_di" bpmnElement="Flow_Zulassung_Ende">
        <di:waypoint x="730" y="800" />
        <di:waypoint x="792" y="800" />
      </bpmndi:BPMNEdge>
      
      <!-- ===============================
           MESSAGE FLOWS
           =============================== -->
      <bpmndi:BPMNEdge id="MsgFlow_Antrag_di" bpmnElement="MsgFlow_Antrag">
        <di:waypoint x="520" y="240" />
        <di:waypoint x="520" y="640" />
        <di:waypoint x="240" y="640" />
        <di:waypoint x="240" y="782" />
        <bpmndi:BPMNLabel>
          <dc:Bounds x="350" y="620" width="100" height="14" />
        </bpmndi:BPMNLabel>
      </bpmndi:BPMNEdge>
      
      <bpmndi:BPMNEdge id="MsgFlow_Pruefergebnis_di" bpmnElement="MsgFlow_Pruefergebnis">
        <di:waypoint x="680" y="535" />
        <di:waypoint x="680" y="640" />
        <di:waypoint x="360" y="640" />
        <di:waypoint x="360" y="760" />
        <bpmndi:BPMNLabel>
          <dc:Bounds x="490" y="620" width="70" height="14" />
        </bpmndi:BPMNLabel>
      </bpmndi:BPMNEdge>
      
      <bpmndi:BPMNEdge id="MsgFlow_Zulassung_di" bpmnElement="MsgFlow_Zulassung">
        <di:waypoint x="680" y="760" />
        <di:waypoint x="680" y="640" />
        <di:waypoint x="650" y="640" />
        <di:waypoint x="650" y="218" />
        <bpmndi:BPMNLabel>
          <dc:Bounds x="600" y="620" width="120" height="14" />
        </bpmndi:BPMNLabel>
      </bpmndi:BPMNEdge>
      
    </bpmndi:BPMNPlane>
  </bpmndi:BPMNDiagram>

</bpmn:definitions>
