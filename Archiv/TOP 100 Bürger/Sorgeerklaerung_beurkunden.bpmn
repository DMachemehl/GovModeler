<?xml version="1.0" encoding="UTF-8"?>
<bpmn:definitions
  xmlns:bpmn="http://www.omg.org/spec/BPMN/20100524/MODEL"
  xmlns:bpmndi="http://www.omg.org/spec/BPMN/20100524/DI"
  xmlns:dc="http://www.omg.org/spec/DD/20100524/DC"
  xmlns:di="http://www.omg.org/spec/DD/20100524/DI"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xmlns:camunda="http://camunda.org/schema/1.0/bpmn"
  targetNamespace="http://example.com/bpmn"
  exporter="GitHub-Copilot-GovModeler"
  exporterVersion="2.0"
  modeler:executionPlatform="Camunda Platform"
  modeler:executionPlatformVersion="7.20.0">

  <!-- =====================================================
       COLLABORATION - Sorgeerklärung beurkunden
       ===================================================== -->
  <bpmn:collaboration id="Collab_Sorgeerklaerung_beurkunden">
    
    <!-- Participant: Bürger -->
    <bpmn:participant id="Participant_Buerger"
                      name="Buerger"
                      processRef="Process_Buerger" />
    
    <!-- Participant: IT -->
    <bpmn:participant id="Participant_IT"
                      name="IT"
                      processRef="Process_IT" />
    
    <!-- Participant: Behörde -->
    <bpmn:participant id="Participant_Behoerde"
                      name="Jugendamt"
                      processRef="Process_Behoerde" />
    
    <!-- Message Flows -->
    <bpmn:messageFlow id="MsgFlow_Terminanfrage"
                      name="Terminanfrage"
                      sourceRef="Task_Termin_vereinbaren"
                      targetRef="StartEvent_Antrag_eingegangen" />
    
    <bpmn:messageFlow id="MsgFlow_Urkunde"
                      name="Urkunde"
                      sourceRef="Task_Urkunde_uebergeben"
                      targetRef="Event_Urkunde_erhalten" />
    
    <bpmn:messageFlow id="MsgFlow_Personendaten_Anfrage"
                      name="Personendaten-Anfrage"
                      sourceRef="Task_Personendaten_abfragen"
                      targetRef="StartEvent_Anfrage_erhalten" />
    
    <bpmn:messageFlow id="MsgFlow_Personendaten_Antwort"
                      name="Personendaten"
                      sourceRef="Task_Personendaten_bereitstellen"
                      targetRef="Event_Personendaten_erhalten" />
    
    <bpmn:messageFlow id="MsgFlow_Sorgerecht_Daten"
                      name="Sorgerechts-Daten"
                      sourceRef="Task_Sorgerechtsdaten_uebermitteln"
                      targetRef="Event_Daten_empfangen" />
  </bpmn:collaboration>

  <!-- =====================================================
       BÜRGER PROZESS
       ===================================================== -->
  <bpmn:process id="Process_Buerger"
                name="Sorgeerklaerung_beurkunden_Buerger"
                isExecutable="true">
    
    <!-- Start Event -->
    <bpmn:startEvent id="StartEvent_Buerger" name="Sorgeerklaerung benoetigt">
      <bpmn:outgoing>Flow_Start_Termin</bpmn:outgoing>
    </bpmn:startEvent>
    
    <!-- User Task: Termin vereinbaren -->
    <bpmn:userTask id="Task_Termin_vereinbaren" name="Termin vereinbaren">
      <bpmn:incoming>Flow_Start_Termin</bpmn:incoming>
      <bpmn:outgoing>Flow_Termin_Warten</bpmn:outgoing>
    </bpmn:userTask>
    
    <!-- Intermediate Catch Event: Urkunde erhalten -->
    <bpmn:intermediateCatchEvent id="Event_Urkunde_erhalten" name="Urkunde erhalten">
      <bpmn:incoming>Flow_Termin_Warten</bpmn:incoming>
      <bpmn:outgoing>Flow_Urkunde_Ende</bpmn:outgoing>
      <bpmn:messageEventDefinition id="MessageEventDef_Urkunde" messageRef="Message_Urkunde" />
    </bpmn:intermediateCatchEvent>
    
    <!-- End Event -->
    <bpmn:endEvent id="EndEvent_Buerger" name="Sorgeerklaerung beurkundet">
      <bpmn:incoming>Flow_Urkunde_Ende</bpmn:incoming>
    </bpmn:endEvent>
    
    <!-- Sequence Flows - Bürger -->
    <bpmn:sequenceFlow id="Flow_Start_Termin"
                       sourceRef="StartEvent_Buerger"
                       targetRef="Task_Termin_vereinbaren" />
    
    <bpmn:sequenceFlow id="Flow_Termin_Warten"
                       sourceRef="Task_Termin_vereinbaren"
                       targetRef="Event_Urkunde_erhalten" />
    
    <bpmn:sequenceFlow id="Flow_Urkunde_Ende"
                       sourceRef="Event_Urkunde_erhalten"
                       targetRef="EndEvent_Buerger" />
  </bpmn:process>

  <!-- =====================================================
       IT PROZESS
       ===================================================== -->
  <bpmn:process id="Process_IT"
                name="IT_Prozess"
                isExecutable="true">
    
    <!-- Start Event -->
    <bpmn:startEvent id="StartEvent_Anfrage_erhalten" name="Anfrage erhalten">
      <bpmn:outgoing>Flow_IT_Start_Bereitstellen</bpmn:outgoing>
    </bpmn:startEvent>
    
    <!-- Service Task: Personendaten bereitstellen -->
    <bpmn:serviceTask id="Task_Personendaten_bereitstellen" name="Personendaten bereitstellen">
      <bpmn:incoming>Flow_IT_Start_Bereitstellen</bpmn:incoming>
      <bpmn:outgoing>Flow_IT_Bereitstellen_Ende</bpmn:outgoing>
    </bpmn:serviceTask>
    
    <!-- Intermediate Catch Event: Sorgerechts-Daten empfangen -->
    <bpmn:intermediateCatchEvent id="Event_Daten_empfangen" name="Daten empfangen">
      <bpmn:incoming>Flow_IT_Bereitstellen_Ende</bpmn:incoming>
      <bpmn:outgoing>Flow_IT_Daten_Speichern</bpmn:outgoing>
      <bpmn:messageEventDefinition id="MessageEventDef_SorgerechtsDaten" messageRef="Message_SorgerechtsDaten" />
    </bpmn:intermediateCatchEvent>
    
    <!-- Service Task: Daten speichern -->
    <bpmn:serviceTask id="Task_Daten_speichern" name="Daten im Register speichern">
      <bpmn:incoming>Flow_IT_Daten_Speichern</bpmn:incoming>
      <bpmn:outgoing>Flow_IT_Speichern_Ende</bpmn:outgoing>
    </bpmn:serviceTask>
    
    <!-- End Event -->
    <bpmn:endEvent id="EndEvent_IT" name="Anfrage bearbeitet">
      <bpmn:incoming>Flow_IT_Speichern_Ende</bpmn:incoming>
    </bpmn:endEvent>
    
    <!-- Sequence Flows - IT -->
    <bpmn:sequenceFlow id="Flow_IT_Start_Bereitstellen"
                       sourceRef="StartEvent_Anfrage_erhalten"
                       targetRef="Task_Personendaten_bereitstellen" />
    
    <bpmn:sequenceFlow id="Flow_IT_Bereitstellen_Ende"
                       sourceRef="Task_Personendaten_bereitstellen"
                       targetRef="Event_Daten_empfangen" />
    
    <bpmn:sequenceFlow id="Flow_IT_Daten_Speichern"
                       sourceRef="Event_Daten_empfangen"
                       targetRef="Task_Daten_speichern" />
    
    <bpmn:sequenceFlow id="Flow_IT_Speichern_Ende"
                       sourceRef="Task_Daten_speichern"
                       targetRef="EndEvent_IT" />
  </bpmn:process>

  <!-- =====================================================
       BEHÖRDEN PROZESS
       ===================================================== -->
  <bpmn:process id="Process_Behoerde"
                name="Sorgeerklaerung_beurkunden_Behoerde"
                isExecutable="true">
    
    <!-- Start Event -->
    <bpmn:startEvent id="StartEvent_Antrag_eingegangen" name="Antrag eingegangen">
      <bpmn:outgoing>Flow_Start_Zustaendigkeit</bpmn:outgoing>
    </bpmn:startEvent>
    
    <!-- Service Task: Zuständigkeit prüfen -->
    <bpmn:serviceTask id="Task_Zustaendigkeit_pruefen" name="Zustaendigkeit pruefen">
      <bpmn:incoming>Flow_Start_Zustaendigkeit</bpmn:incoming>
      <bpmn:outgoing>Flow_Zustaendigkeit_Identitaet</bpmn:outgoing>
    </bpmn:serviceTask>
    
    <!-- User Task: Identität prüfen -->
    <bpmn:userTask id="Task_Identitaet_pruefen" name="Identitaet pruefen">
      <bpmn:incoming>Flow_Zustaendigkeit_Identitaet</bpmn:incoming>
      <bpmn:outgoing>Flow_Identitaet_Daten</bpmn:outgoing>
    </bpmn:userTask>
    
    <!-- User Task: Personendaten abfragen -->
    <bpmn:userTask id="Task_Personendaten_abfragen" name="Personendaten abfragen">
      <bpmn:incoming>Flow_Identitaet_Daten</bpmn:incoming>
      <bpmn:outgoing>Flow_Daten_Warten</bpmn:outgoing>
    </bpmn:userTask>
    
    <!-- Intermediate Catch Event: Personendaten erhalten -->
    <bpmn:intermediateCatchEvent id="Event_Personendaten_erhalten" name="Personendaten erhalten">
      <bpmn:incoming>Flow_Daten_Warten</bpmn:incoming>
      <bpmn:outgoing>Flow_Daten_Beurkunden</bpmn:outgoing>
      <bpmn:messageEventDefinition id="MessageEventDef_Personendaten" messageRef="Message_Personendaten" />
    </bpmn:intermediateCatchEvent>
    
    <!-- User Task: Sorgeerklärung beurkunden -->
    <bpmn:userTask id="Task_Sorgeerklaerung_beurkunden" name="Sorgeerklaerung beurkunden">
      <bpmn:incoming>Flow_Daten_Beurkunden</bpmn:incoming>
      <bpmn:outgoing>Flow_Beurkunden_Ausstellen</bpmn:outgoing>
    </bpmn:userTask>
    
    <!-- User Task: Urkunde ausstellen -->
    <bpmn:userTask id="Task_Urkunde_ausstellen" name="Urkunde ausstellen">
      <bpmn:incoming>Flow_Beurkunden_Ausstellen</bpmn:incoming>
      <bpmn:outgoing>Flow_Ausstellen_Uebergeben</bpmn:outgoing>
    </bpmn:userTask>
    
    <!-- User Task: Urkunde übergeben -->
    <bpmn:userTask id="Task_Urkunde_uebergeben" name="Urkunde uebergeben">
      <bpmn:incoming>Flow_Ausstellen_Uebergeben</bpmn:incoming>
      <bpmn:outgoing>Flow_Uebergeben_Uebermitteln</bpmn:outgoing>
    </bpmn:userTask>
    
    <!-- User Task: Sorgerechtsdaten übermitteln -->
    <bpmn:userTask id="Task_Sorgerechtsdaten_uebermitteln" name="Sorgerechtsdaten uebermitteln">
      <bpmn:incoming>Flow_Uebergeben_Uebermitteln</bpmn:incoming>
      <bpmn:outgoing>Flow_Uebermitteln_Ende</bpmn:outgoing>
    </bpmn:userTask>
    
    <!-- End Event -->
    <bpmn:endEvent id="EndEvent_Behoerde" name="Sorgeerklaerung beurkundet">
      <bpmn:incoming>Flow_Uebermitteln_Ende</bpmn:incoming>
    </bpmn:endEvent>
    
    <!-- Sequence Flows - Behörde -->
    <bpmn:sequenceFlow id="Flow_Start_Zustaendigkeit"
                       sourceRef="StartEvent_Antrag_eingegangen"
                       targetRef="Task_Zustaendigkeit_pruefen" />
    
    <bpmn:sequenceFlow id="Flow_Zustaendigkeit_Identitaet"
                       sourceRef="Task_Zustaendigkeit_pruefen"
                       targetRef="Task_Identitaet_pruefen" />
    
    <bpmn:sequenceFlow id="Flow_Identitaet_Daten"
                       sourceRef="Task_Identitaet_pruefen"
                       targetRef="Task_Personendaten_abfragen" />
    
    <bpmn:sequenceFlow id="Flow_Daten_Warten"
                       sourceRef="Task_Personendaten_abfragen"
                       targetRef="Event_Personendaten_erhalten" />
    
    <bpmn:sequenceFlow id="Flow_Daten_Beurkunden"
                       sourceRef="Event_Personendaten_erhalten"
                       targetRef="Task_Sorgeerklaerung_beurkunden" />
    
    <bpmn:sequenceFlow id="Flow_Beurkunden_Ausstellen"
                       sourceRef="Task_Sorgeerklaerung_beurkunden"
                       targetRef="Task_Urkunde_ausstellen" />
    
    <bpmn:sequenceFlow id="Flow_Ausstellen_Uebergeben"
                       sourceRef="Task_Urkunde_ausstellen"
                       targetRef="Task_Urkunde_uebergeben" />
    
    <bpmn:sequenceFlow id="Flow_Uebergeben_Uebermitteln"
                       sourceRef="Task_Urkunde_uebergeben"
                       targetRef="Task_Sorgerechtsdaten_uebermitteln" />
    
    <bpmn:sequenceFlow id="Flow_Uebermitteln_Ende"
                       sourceRef="Task_Sorgerechtsdaten_uebermitteln"
                       targetRef="EndEvent_Behoerde" />
  </bpmn:process>

  <!-- =====================================================
       MESSAGE DEFINITIONEN
       ===================================================== -->
